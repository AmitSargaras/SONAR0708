<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.pincodemapping.bus">
	<class entity-name="actualOBPincodeMapping" name="OBPincodeMapping"
		table="CMS_STATE_PINCODE_MAP" lazy="false">
		<id name="id" column="ID" type="long">
			 <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STATE_PINCODE_MAP_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		
		
		<many-to-one name="stateId" column="STATE_ID" entity-name="actualState" lazy="false" />
		
		<!-- <property name="stateCode" column="STATE_CODE" />
		<property name="stateId" column="STATE_ID" /> -->
		
		<property name="pincode" column="PINCODE" />
	
        
	    
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
	</class>

	<class entity-name="stageOBPincodeMapping" name="OBPincodeMapping"
		table="CMS_STAGE_STATE_PINCODE_MAP" lazy="false">
		<id name="id" column="ID" type="long">
			 <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_STATE_PINCODE_MAP_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		
		<many-to-one name="stateId" column="STATE_ID" entity-name="actualState" lazy="false" />
		
		<!-- <property name="stateId" column="STATE_ID" />
		<property name="stateCode" column="STATE_CODE" /> -->
		<property name="pincode" column="PINCODE" />
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
	</class>
</hibernate-mapping>
