<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.cersaiMapping.bus">
	<class  entity-name="actualCersaiMapping" name="OBCersaiMapping" table="CMS_CERSAI_MAPPING" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_CERSAI_MAPPING_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

		<property name="climsValue" column="CLIMS_VALUE" />
		<property name="cersaiValue" column="CERSAI_VALUE" />
		<property name="masterName" column="MASTER_NAME" />
		
		<property name="status" column="STATUS" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
	</class>
	<class  entity-name="stageCersaiMapping" name="OBCersaiMapping" table="CMS_STAGE_CERSAI_MAPPING" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_CERSAI_MAPPING_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
	<property name="climsValue" column="CLIMS_VALUE" />
		<property name="cersaiValue" column="CERSAI_VALUE" />
		<property name="masterName" column="MASTER_NAME" />
		
		<property name="status" column="STATUS" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
	</class>
</hibernate-mapping>
