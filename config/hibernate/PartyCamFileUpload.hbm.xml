<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.fileUpload.bus">
	<class  entity-name="actualPartyCamFileUpload" name="OBPartyCamFile" table="CMS_PARTYCAM_FILE_UPLOAD" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_FILEUPLOAD_SEQ</param>
			</generator>
		</id>		
			<property name="fileId" column="FILE_ID" />
			<property name="partyId" column="PARTY_ID" />
			<property name="camDate" column="CAM_DATE" />
			<property name="camLoginDate" column="CAM_LOGIN_DATE" />
			<property name="ramRating" column="RAM_RATING" />
			<property name="ramRatingYear" column="RAM_RATING_YEAR" />
			<property name="customerRamId" column="CUSTOMER_RAM_ID" />
			<property name="camExpiryDate" column="CAM_EXPIRY_DATE" />
			<property name="camExtensionDate" column="CAM_EXTENSION_DATE" />
			<property name="line" column="LINE_NO" />
			<property name="serialNo" column="SERIAL_NO" />
			<property name="status" column="STATUS" />
			<property name="reason" column="REASON" />
			<property name="uploadStatus" column="UPLOAD_STATUS" />
	</class>
	
	
	<class  entity-name="stagePartyCamFileUpload" name="OBPartyCamFile" table="STAGE_PARTYCAM_FILE_UPLOAD" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_FILEUPLOAD_SEQ</param>
			</generator>
		</id>
			<property name="fileId" column="FILE_ID" />
			<property name="partyId" column="PARTY_ID" />
			<property name="camDate" column="CAM_DATE" />
			<property name="camLoginDate" column="CAM_LOGIN_DATE" />
			<property name="ramRating" column="RAM_RATING" />
			<property name="ramRatingYear" column="RAM_RATING_YEAR" />
			<property name="customerRamId" column="CUSTOMER_RAM_ID" />
			<property name="camExpiryDate" column="CAM_EXPIRY_DATE" />
			<property name="camExtensionDate" column="CAM_EXTENSION_DATE" />
			<property name="line" column="LINE_NO" />
			<property name="serialNo" column="SERIAL_NO" />
			<property name="status" column="STATUS" />
			<property name="reason" column="REASON" />
			<property name="uploadStatus" column="UPLOAD_STATUS" />
			
	</class>
	

</hibernate-mapping>
