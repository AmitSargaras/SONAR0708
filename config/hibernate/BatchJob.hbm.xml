<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.integrosys.cms.batch.factory.OBBatchJobStatus" table="CMS_BATCH_JOB" lazy="false">
  	<id name="batchId" column="BATCH_ID" type="long">
  		<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
  			<param name="sequence">BATCH_JOB_SEQ</param>
  		</generator>
  	</id>
  	
  	<property name="jobName" column="JOB_NAME" />
  	<property name="parameters" column="JOB_PARAMETERS" />
  	<property name="className" column="CLASS_NAME" />
  	<property name="startDate" column="START_DATE" type="timestamp" />
  	<property name="endDate" column="END_DATE" type="timestamp" />
  	<property name="status" column="JOB_STATUS" />
  	<property name="message" column="MESSAGE" />
  </class>
</hibernate-mapping>