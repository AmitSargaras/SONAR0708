<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="actualIfsccode" 
		name="com.integrosys.cms.app.customer.bus.OBIfscMethod" table="CMS_IFSC_CODE" lazy="false">
            <id name="id">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_IFSC_CODE_SEQ</param>
			</generator>
            </id>
			<property name="customerId" column="CMS_LE_MAIN_PROFILE_ID"/>
			<property name="nodal" column="NODAL"/>
			<property name="lead" column="LEAD"/>
			<property name="ifscCode" column="IFSC_CODE"/>
			<property name="bankName" column="BANK_NAME"/>
			<property name="branchName" column="BRANCH_NAME"/>
			<property name="branchNameAddress" column="BRANCH_NAME_ADD"/>
			<property name="bankType" column="BANK_TYPE"/>
			<property name="emailID" column="EMAIL_ID"/>
			<property name="status" column="STATUS"/>
			<property name="revisedEmailID" column="REVISED_EMAIL_ID"/>
    </class> 
    
    <class entity-name="stageIfsccode" 
		name="com.integrosys.cms.app.customer.bus.OBIfscMethod" table="CMS_STAGE_IFSC_CODE" lazy="false">
            <id name="id">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_IFSC_CODE_SEQ</param>
			</generator>
            </id>
			<property name="customerId" column="CMS_LE_MAIN_PROFILE_ID"/>
			<property name="nodal" column="NODAL"/>
			<property name="lead" column="LEAD"/>
			<property name="ifscCode" column="IFSC_CODE"/>
			<property name="bankName" column="BANK_NAME"/>
			<property name="branchName" column="BRANCH_NAME"/>
			<property name="branchNameAddress" column="BRANCH_NAME_ADD"/>
			<property name="bankType" column="BANK_TYPE"/>
			<property name="emailID" column="EMAIL_ID"/>
			<property name="status" column="STATUS"/>
			<property name="revisedEmailID" column="REVISED_EMAIL_ID"/>
    </class> 
    
</hibernate-mapping>