<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="actualBhavCopy" 
		name="com.integrosys.cms.app.collateral.bus.OBBhavCopy" table="CMS_BHAVCOPY" lazy="false">
            <id name="scCode" column="SC_CODE">
                <generator class="increment">
                </generator>
            </id>

			<property name="scName" column="SC_NAME"/>
			<property name="scGroup" column="SC_GROUP"/>
			<property name="openValue" column="OPEN_VALUE"/>
			<property name="closeValue" column="CLOSE_VALUE"/>
			<property name="lastValue" column="LAST_VALUE"/>
			
    </class>    
    
</hibernate-mapping>