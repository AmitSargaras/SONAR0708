<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.erosion.bus">
	<class name="OBErosionStatus"  table="CMS_EROSION_STATUS" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_EROSION_STATUS_SEQ</param>
			 </generator>
		</id>
			<property name="reportingDate" column="REPORTING_DATE" type="date" />
			<property name="activity" column="ACTIVITY"/>
			<property name="status" column="STATUS"  />
	</class>
</hibernate-mapping>