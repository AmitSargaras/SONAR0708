<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="actualRecurrent" name="com.integrosys.cms.host.eai.covenant.bus.RecurrentDoc"
		table="CMS_RECURRENT_DOC" polymorphism="explicit">
		<id name="RecurrentDocID" column="RECURRENT_DOC_ID" type="long">
			<generator
				class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">RECURRENT_DOC_SEQ</param>
			</generator>
		</id>
		<property name="CmsSubProfileID" column="CMS_LMP_SUB_PROFILE_ID" type="long" />
		<property name="CmsLimitProfileID" column="CMS_LSP_LMT_PROFILE_ID" type="long" />
		<property name="Status" column="STATUS" />
	</class>
	
	<class entity-name="stageRecurrent" name="com.integrosys.cms.host.eai.covenant.bus.StageRecurrentDoc"
		table="STAGE_RECURRENT_DOC" polymorphism="explicit">
		<id name="RecurrentDocID" column="RECURRENT_DOC_ID" type="long">
			<generator
				class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">RECURRENT_DOC_SEQ</param>
			</generator>
		</id>
		<property name="CmsSubProfileID" column="CMS_LMP_SUB_PROFILE_ID" type="long" />
		<property name="CmsLimitProfileID" column="CMS_LSP_LMT_PROFILE_ID" type="long" />
		<property name="Status" column="STATUS" />
	</class>
</hibernate-mapping>