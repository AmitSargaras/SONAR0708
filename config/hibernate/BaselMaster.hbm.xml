<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.baselmaster.bus">
	<class  entity-name="actualBasel" name="OBBaselMaster" table="CMS_BASEL_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_BASEL_MASTER_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
			<property name="status" column="STATUS" />
			<property name="deprecated" column="DEPRECATED" />
			<property name="system" column="SYSTEM" />
			<property name="createdBy" column="CREATE_BY" />
			<property name="createdOn" column="CREATION_DATE" />
			<property name="lastUpdatedBy" column="LAST_UPDATE_BY" />
			<property name="lastUpdatedOn" column="LAST_UPDATE_DATE" />
			<property name="systemValue" column="SYSTEM_VALUE" />
			<property name="exposureSource" column="EXPOSURE_SOURCE" />
			<property name="baselValidation" column="BASEL_VALIDATION" />
			<property name="reportHandOff" column="REPORTED_HANDOFF" />
			
	</class>
	
	
	<class  entity-name="stageBasel" name="OBBaselMaster" table="CMS_STAGE_BASEL_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_BASEL_MASTER_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
			<property name="status" column="STATUS" />
			<property name="deprecated" column="DEPRECATED" />
			<property name="system" column="SYSTEM" />
			<property name="createdBy" column="CREATE_BY" />
			<property name="createdOn" column="CREATION_DATE" />
			<property name="lastUpdatedBy" column="LAST_UPDATE_BY" />
			<property name="lastUpdatedOn" column="LAST_UPDATE_DATE" />
			<property name="systemValue" column="SYSTEM_VALUE" />
			<property name="exposureSource" column="EXPOSURE_SOURCE" />
			<property name="baselValidation" column="BASEL_VALIDATION" />
			<property name="reportHandOff" column="REPORTED_HANDOFF" />
			
	</class>
	

</hibernate-mapping>
