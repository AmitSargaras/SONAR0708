<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.npaTraqCodeMaster.bus">

	<class  entity-name="actualNpaTraqCodeMaster" name="OBNpaTraqCodeMaster" table="CMS_NPA_TRAQ_CODE_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_NPA_TRAQ_CODE_MASTER_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="npaTraqCode" column="NPA_TRAQ_CODE" />
		<property name="securityType" column="SECURITY_TYPE" />
		<property name="securitySubType" column="SECURITY_SUB_TYPE" />
		<property name="propertyTypeCodeDesc" column="PROPERTY_TYPE_CODE_DESC" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
		<property name="deprecated" column="DEPRECATED" />
	</class>

	<class  entity-name="stageNpaTraqCodeMaster" name="OBNpaTraqCodeMaster" table="CMS_STAGE_NPA_TRAQ_CODE_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">STG_NPA_TRAQ_CODE_MASTER_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="npaTraqCode" column="NPA_TRAQ_CODE" />
		<property name="securityType" column="SECURITY_TYPE" />
		<property name="securitySubType" column="SECURITY_SUB_TYPE" />
		<property name="propertyTypeCodeDesc" column="PROPERTY_TYPE_CODE_DESC" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
		<property name="deprecated" column="DEPRECATED" />
		
	</class>
</hibernate-mapping>
