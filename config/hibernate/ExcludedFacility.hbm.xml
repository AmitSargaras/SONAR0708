<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.excludedfacility.bus">
	<class  entity-name="actualExcludedFacility" name="OBExcludedFacility" table="CMS_EXCLUDED_FACILITY" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_EXCLUDED_FACILITY_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

		<!-- <property name="excludedFacilityCategory" column="EXCLUDED_FACILITY_CATEGORY" /> -->
		<property name="excludedFacilityDescription" column="EXCLUDED_FACILITY_CATEGORY" />
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
	</class>
	<class  entity-name="stageExcludedFacility" name="OBExcludedFacility" table="CMS_STAGE_EXCLUDED_FACILITY" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_EXCLUDED_FACILITY_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
		<!-- <property name="excludedFacilityCategory" column="EXCLUDED_FACILITY_CATEGORY" /> -->
		<property name="excludedFacilityDescription" column="EXCLUDED_FACILITY_CATEGORY" />
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
	</class>
</hibernate-mapping>
