<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="actualInsuranceCoverageDtls" 
		name="com.integrosys.cms.app.insurancecoveragedtls.bus.OBInsuranceCoverageDtls" table="CMS_INSURANCE_COVERAGE_DTLS" lazy="false">
            <id name="id">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_INSR_COVRG_DTLS_SEQ</param>
			</generator>
            </id>

			<property name="insuranceType" column="INSURANCE_TYPE"/>
			<property name="insuranceCategoryName" column="CATEGORY_NAME"/>

			<property name="versionTime" column="VERSION_TIME" type="long"/>
            <property name="createdBy" column="CREATE_BY"/>
            <property name="creationDate" column="CREATION_DATE" />
            <property name="lastUpdateBy" column="LAST_UPDATE_BY"/>
            <property name="lastUpdateDate" column="LAST_UPDATE_DATE"/>            
			<property name="status" column="STATUS"/>
			<property name="deprecated"	column="DEPRECATED"/>
			
			<many-to-one name="insuranceCoverageCode" column="INSURANCE_COVERAGE_CODE" entity-name="actualInsuranceCoverage" lazy="false" />
			
    </class>    
    
    <class entity-name="stagingInsuranceCoverageDtls" 
		name="com.integrosys.cms.app.insurancecoveragedtls.bus.OBInsuranceCoverageDtls" table="CMS_STAGE_INSURANCE_COVERAGE_DTLS" lazy="false">
            <id name="id">
               <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_INSR_COVRG_DTLS_SEQ</param>
			</generator>
            </id>
            <property name="insuranceType" column="INSURANCE_TYPE"/>
			<property name="insuranceCategoryName" column="CATEGORY_NAME"/>

			<property name="versionTime" column="VERSION_TIME" type="long"/>
            <property name="createdBy" column="CREATE_BY"/>
            <property name="creationDate" column="CREATION_DATE" />
            <property name="lastUpdateBy" column="LAST_UPDATE_BY"/>
            <property name="lastUpdateDate" column="LAST_UPDATE_DATE"/>            
			<property name="status" column="STATUS"/>
			<property name="deprecated"	column="DEPRECATED"/>
			
			<many-to-one name="insuranceCoverageCode" column="INSURANCE_COVERAGE_CODE" entity-name="actualInsuranceCoverage" lazy="false" />
    </class>
</hibernate-mapping>