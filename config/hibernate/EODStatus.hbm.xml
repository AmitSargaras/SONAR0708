<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.eod.bus">
	<class name="OBEODStatus"  table="CMS_EOD_STATUS" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_EOD_STATUS_SEQ</param>
			 </generator>
		</id>
			<property name="eodDate" column="EODDATE" type="date" />
			<property name="activity" column="ACTIVITY"/>
			<property name="status" column="STATUS"  />
	</class>
	<class name="OBAdfRbiStatus"  table="CMS_ADF_RBI_STATUS" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_ADF_RBI_STATUS_SEQ</param>
			 </generator>
		</id>
			<property name="eodDate" column="EODDATE" type="date" />
			<property name="activity" column="ACTIVITY"/>
			<property name="status" column="STATUS"  />
	</class>
</hibernate-mapping>

	
