<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.fileUpload.bus">
	<class  entity-name="actualLeiDetailsUpload" name="OBLeiDetailsFile" table="ACTUAL_LEI_DETAILS_UPLOAD" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">ACTUAL_LEI_DETAILS_UPLOAD_SEQ</param>
			</generator>
		</id>		
			<property name="fileId" column="FILE_ID" />
			<property name="partyId" column="PARTY_ID" />
			<property name="leiCode" column="LEI_Code" />
			<property name="leiExpDate" column="LEI_Exp_Date" />
			<property name="status" column="STATUS" />
			<property name="reason" column="REASON" />
			<property name="uploadStatus" column="UPLOAD_STATUS" />
			<property name="checkerDate" column="CHECKER_DATE" />
			<property name="leiValidationFlag" column="LEI_VALIDATION_FLAG" />
	</class>
	
	
	<class  entity-name="stageLeiDetailsUpload" name="OBLeiDetailsFile" table="STAGE_LEI_DETAILS_UPLOAD" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">STAGE_LEI_DETAILS_UPLOAD_SEQ</param>
			</generator>
		</id>
			<property name="fileId" column="FILE_ID" />
			<property name="partyId" column="PARTY_ID" />
			<property name="leiCode" column="LEI_Code" />
			<property name="leiExpDate" column="LEI_Exp_Date" />
			<property name="status" column="STATUS" />
			<property name="reason" column="REASON" />
			<property name="uploadStatus" column="UPLOAD_STATUS" />
			<property name="checkerDate" column="CHECKER_DATE" />			
			<property name="leiValidationFlag" column="LEI_VALIDATION_FLAG" />
	</class>
</hibernate-mapping>
