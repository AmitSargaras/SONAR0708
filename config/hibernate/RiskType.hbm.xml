<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.riskType.bus">
	<class  entity-name="actualRiskType" name="OBRiskType" table="CMS_RISK_TYPE" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_RISK_TYPE_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

		<property name="riskTypeName" column="RISK_TYPE_NAME" />
		<property name="riskTypeCode" column="RISK_TYPE_CODE" />
		
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="deprecated" column="DEPRECATED" />
	</class>
	<class  entity-name="stageRiskType" name="OBRiskType" table="CMS_STAGE_RISK_TYPE" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STAGE_RISK_TYPE_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="riskTypeName" column="RISK_TYPE_NAME" />
		<property name="riskTypeCode" column="RISK_TYPE_CODE" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="deprecated" column="DEPRECATED" />
	</class>
</hibernate-mapping>
