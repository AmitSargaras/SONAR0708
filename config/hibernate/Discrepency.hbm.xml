 <class name="com.integrosys.cms.app.discrepency.bus.OBDiscrepency" entity-name="actualDiscrepency" table="CMS_DISCREPENCY" lazy="false" >  
    <id name="id" column="ID" type="long">
    	<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_DISCREPENCY_SEQ</param>
			</generator>
    </id>
    
    <property name="customerId"				column="CUSTOMER_ID" />
    <property name="discrepencyType"		column="DISCREPENCY_TYPE" />
