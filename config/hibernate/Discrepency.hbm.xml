    <property name="acceptedDate"     		column="ACCEPTED_DATE" />
    <property name="originalTargetDate"     column="ORIGINAL_TARGET_DATE" />
    <property name="nextDueDate"     		column="NEXT_DUE_DATE" />
    <property name="critical"     			column="CRITICAL" />
    <property name="approvedBy"     		column="WAIVED_BY" />
    <property name="discrepency"     		column="DISCREPENCY" />
    <property name="counter"    			column="DISCREPENCY_COUNTER" />
    <property name="discrepencyRemark"     	column="REMARKS" />
    <property name="totalDeferedDays"     	column="TOTAL_DEFERED_DAYS" />
    <property name="originalDeferedDays"     	column="ORG_DATE_DEFERED_DAYS" />
    <property name="transactionStatus"     	column="TRANSACTION_STATUS" />
    <property name="deferedCounter"     	column="DEFERED_COUNTER" />
