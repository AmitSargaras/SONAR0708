<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.collateralrocandinsurance.bus">
	<class  entity-name="actualCollateralRoc" name="OBCollateralRoc" table="CMS_COLLATERAL_ROC_MAP" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_COLLATERAL_ROC_MAP_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

		<property name="collateralCategory" column="COLLATERAL_CATEGORY" />
		<property name="collateralRocCode" column="COLLATERAL_ROC_CODE" />
		<property name="collateralRocDescription" column="COLLATERAL_ROC_DESCRIPTION" />
		<property name="irbCategory" column="IRB_CATEGORY" />
		<property name="insuranceApplicable" column="INSURANCE_APPLICABLE" />
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
	</class>
	<class  entity-name="stageCollateralRoc" name="OBCollateralRoc" table="CMS_STAGE_COLLATERAL_ROC_MAP" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_COLLATERAL_ROC_MAP_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
		<property name="collateralCategory" column="COLLATERAL_CATEGORY" />
		<property name="collateralRocCode" column="COLLATERAL_ROC_CODE" />
		<property name="collateralRocDescription" column="COLLATERAL_ROC_DESCRIPTION" />
		<property name="irbCategory" column="IRB_CATEGORY" />
		<property name="insuranceApplicable" column="INSURANCE_APPLICABLE" />
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
	</class>
</hibernate-mapping>
