<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.eod.sync.bus">
	<class name="OBEODSyncStatus"  table="CMS_EODSYNC_STATUS" lazy="false" entity-name="eodSyncStatus">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_EODSYNC_STATUS_SEQ</param>
			 </generator>
		</id>
		<property name="versionTime"		column="VERSION_TIME"		type="long"/>
		<property name="applicationDate"	column="APPLICATIONDATE"	type="date"/>
		<property name="currentDate"		column="CURRENTDATE"		type="date"/>
		<property name="syncDirection"		column="SYNCDIRECTION"		/>
		<property name="processKey"         column="PROCESSKEY"			/>
		<property name="processDesc"        column="PROCESSDESC"		/>
		<property name="processStartTime"   column="PROCESSSTARTTIME"	type="timestamp"/>
		<property name="processEndTime"		column="PROCESSENDTIME"		type="timestamp"/>
		<property name="processStatus"      column="PROCESSSTATUS"        	/>
		<property name="processException"   column="PROCESSEXCEPTION"     	/>
		<property name="totalCount"         column="TOTALCOUNT"				/> 
		<property name="successCount"		column="SUCCESSCOUNT"           /> 
		<property name="failedCount"        column="FAILEDCOUNT"            />
		<property name="fileName"        	column="file_name"            />
		<property name="acknowledgementFileName"        column="ack_file_name"            />
	</class>
	<class name="OBEODSyncStatusLog"  table="CMS_EODSYNC_STATUS_LOG" lazy="false" entity-name="eodSyncStatusLog">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_EODSYNC_STATUS_LOG_SEQ</param>
			 </generator>
		</id>
		<property name="versionTime"		column="VERSION_TIME"		type="long"/>
		<property name="applicationDate"	column="APPLICATIONDATE"	type="date"/>
		<property name="currentDate"		column="CURRENTDATE"		type="date"/>
		<property name="syncDirection"		column="SYNCDIRECTION"		/>
		<property name="processKey"         column="PROCESSKEY"			/>
		<property name="processDesc"        column="PROCESSDESC"		/>
		<property name="processStartTime"   column="PROCESSSTARTTIME"	type="timestamp"/>
		<property name="processEndTime"		column="PROCESSENDTIME"		type="timestamp"/>
		<property name="processStatus"      column="PROCESSSTATUS"        	/>
		<property name="processException"   column="PROCESSEXCEPTION"     	/>
		<property name="totalCount"         column="TOTALCOUNT"				/> 
		<property name="successCount"		column="SUCCESSCOUNT"           /> 
		<property name="failedCount"        column="FAILEDCOUNT"            />
	</class>
</hibernate-mapping>

	
