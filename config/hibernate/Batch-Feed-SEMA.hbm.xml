<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="com.integrosys.cms.batch.sema">
	<class name="OBFacilityFile" table="SI_TEMP_SEMA_CA002">
		<id name="tempID" column="TEMP_ID" type="long">
			<generator
				class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">SI_TEMP_SEMA_CA002_SEQ</param>
			</generator>
		</id>
		<property name="recordType" column="RECORD_TYPE" />
		<property name="newIC" column="NEW_IC"  />
		<property name="oldIC" column="OLD_IC"  />
		<property name="others" column="OTHER"  />
		<property name="statusDateNPL" column="NPL_STATUS_DATE" type="timestamp" />
		<property name="outstandingSign" column="OUTSTD_SIGN" />
		<property name="outstandingBalance" column="OUTSTD_BALANCE" type="big_decimal" />
        <property name="customerStatus" column="CUSTOMER_STATUS" />
        <property name="userCode3" column="USER_CODE_3" />
		<property name="statusNPL" column="NPL_STATUS" />
	</class>
</hibernate-mapping>
