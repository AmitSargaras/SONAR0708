<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.valuationAmountAndRating.bus">
	<class  entity-name="actualValuationAmountAndRating" name="OBValuationAmountAndRating" table="CMS_VALUATION_AMT_RATING" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_VAL_AMOUNT_AND_RATING_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

		<property name="criteria" column="CRITERIA" />
		<property name="valuationAmount" column="VALUATION_AMOUNT" />
		<property name="ramRating" column="RAM_RATING" />
		<property name="excludePartyId" column="EXCLUDE_PARTY_ID" />
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="deprecated" column="DEPRECATED" />
	</class>
	<class  entity-name="stageValuationAmountAndRating" name="OBValuationAmountAndRating" table="CMS_STAGE_VALUATION_AMT_RATING" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STAGE_VAL_AMT_AND_RAT_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
		<property name="criteria" column="CRITERIA" />
		<property name="valuationAmount" column="VALUATION_AMOUNT" />
		<property name="ramRating" column="RAM_RATING" />
		<property name="excludePartyId" column="EXCLUDE_PARTY_ID"/>
		<property name="status" column="STATUS" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="deprecated" column="DEPRECATED" />
	</class>
</hibernate-mapping>
