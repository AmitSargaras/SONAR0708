      <data-source-name>CMSPool</data-source-name>
      <table-map>
         <table-name>COMMON_CODE_CATEGORY</table-name>
         <field-map>
            <cmp-field>cMPCommonCodeTypeId</cmp-field>
            <dbms-column>CATEGORY_ID</dbms-column>
