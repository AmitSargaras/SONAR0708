         </field-map>
         <field-map>
            <cmp-field>cMPStatus</cmp-field>
            <dbms-column>STATUS</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>refCategoryCode</cmp-field>
            <dbms-column>REF_CATEGORY_CODE</dbms-column>
         </field-map>
      </table-map>
   </weblogic-rdbms-bean>
   <weblogic-rdbms-bean>
