            <persistence-type>Container</persistence-type>
            <prim-key-class>java.lang.Long</prim-key-class>
            <reentrant>False</reentrant>
            <cmp-version>2.x</cmp-version>
            <abstract-schema-name>EBCCDocumentLocation</abstract-schema-name>
            <cmp-field id="cmp_EBCCDocumentLocation_cMPDocLocationID">
                <field-name>cMPDocLocationID</field-name>
            </cmp-field>
            <cmp-field id="cmp_EBCCDocumentLocation_docLocationCategory">
                <field-name>docLocationCategory</field-name>
            </cmp-field>  
            <cmp-field id="cmp_EBCCDocumentLocation_cMPLimitProfileID">
