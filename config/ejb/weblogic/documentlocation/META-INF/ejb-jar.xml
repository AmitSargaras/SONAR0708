                <query-method>
                    <method-name>findByLimitProfileAndSubProfile</method-name>
                    <method-params>
                        <method-param>java.lang.Long</method-param>
                        <method-param>java.lang.Long</method-param>                        
                    </method-params>
                </query-method>
                <ejb-ql><![CDATA[SELECT  OBJECT(o) FROM EBCCDocumentLocation o WHERE o.cMPLimitProfileID = ?1 AND o.cMPSubProfileID = ?2 AND o.isDeletedIndStr = 'N']]></ejb-ql>
            </query>
                       
            <query>
                <query-method>
