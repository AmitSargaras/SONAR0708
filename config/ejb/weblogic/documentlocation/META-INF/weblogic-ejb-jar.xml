<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBDocumentLocationProxyManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBDocumentLocationProxyManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>SBDocumentLocationBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBDocumentLocationBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBStagingDocumentLocationBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBStagingDocumentLocationBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBCCDocumentLocation</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
                <res-ref-name>jdbc/CMSPool</res-ref-name>              
                <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBCCDocumentLocationHome</jndi-name>
        <local-jndi-name>EBCCDocumentLocationLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  
    
    <weblogic-enterprise-bean>
        <ejb-name>EBStagingCCDocumentLocation</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
                <res-ref-name>jdbc/CMSPool</res-ref-name>              
                <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBStagingCCDocumentLocationHome</jndi-name>
        <local-jndi-name>EBStagingCCDocumentLocationLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  
</weblogic-ejb-jar>