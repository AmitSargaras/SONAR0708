<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN' 'http://java.sun.com/dtd/ejb-jar_2_0.dtd'>

<ejb-jar>
 <display-name>Workspace</display-name>
 <enterprise-beans>
  <session>
   <ejb-name>SBWorkspaceManager</ejb-name>
   <home>com.integrosys.cms.app.workspace.SBWorkspaceManagerHome</home>
   <remote>com.integrosys.cms.app.workspace.SBWorkspaceManager</remote>
   <ejb-class>com.integrosys.cms.app.workspace.SBWorkspaceManagerBean</ejb-class>
   <session-type>Stateless</session-type>
   <transaction-type>Container</transaction-type>
      <resource-ref>
         <res-ref-name>jdbc/CMSPool</res-ref-name>
         <res-type>javax.sql.DataSource</res-type>
         <res-auth>Container</res-auth>
         <res-sharing-scope>Shareable</res-sharing-scope>
      </resource-ref>
  </session>
 </enterprise-beans>
</ejb-jar>
