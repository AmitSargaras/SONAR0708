<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBEventMonitorController</ejb-name>
        <reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>
        <jndi-name>SBEventMonitorControllerHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>SBEventMonitorFirstPass</ejb-name>
        <reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>
        <jndi-name>SBEventMonitorFirstPassHome</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>SBEventManager</ejb-name>
       <reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>
        <jndi-name>SBEventManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBDocExpiry</ejb-name>
        <reference-descriptor>          
          <resource-description>              
            <res-ref-name>jdbc/CMSPool</res-ref-name>              
              <jndi-name>CMSPool</jndi-name>          
          </resource-description>      
        </reference-descriptor>
        <jndi-name>SBDocExpiryHome</jndi-name>
    </weblogic-enterprise-bean>
 </weblogic-ejb-jar>

