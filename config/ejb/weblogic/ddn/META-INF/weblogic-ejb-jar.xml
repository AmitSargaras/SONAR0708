<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBDDNProxyManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBDDNProxyManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>SBDDNBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBDDNBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBStagingDDNBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBStagingDDNBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBDDN</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor><reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>
        <jndi-name>EBDDNHome</jndi-name>
        <local-jndi-name>EBDDNLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBDDNItem</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor><reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>        
        <jndi-name>EBDDNItemHome</jndi-name>
        <local-jndi-name>EBDDNItemLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  
  

    <weblogic-enterprise-bean>
        <ejb-name>EBStagingDDN</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
            <res-ref-name>jdbc/CMSPool</res-ref-name>              
            <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBStagingDDNHome</jndi-name>
        <local-jndi-name>EBStagingDDNLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBStagingDDNItem</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>        
        <reference-descriptor>          
            <resource-description>              
            <res-ref-name>jdbc/CMSPool</res-ref-name>              
            <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBStagingDDNItemHome</jndi-name>
        <local-jndi-name>EBStagingDDNItemLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  

</weblogic-ejb-jar>