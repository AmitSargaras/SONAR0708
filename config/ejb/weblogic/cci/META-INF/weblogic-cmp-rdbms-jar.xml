<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar
  PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd">
<weblogic-rdbms-jar>
    <weblogic-rdbms-bean >
        <ejb-name>EBCCICounterpartyDetails</ejb-name>
        <data-source-name>CMSPool</data-source-name>
        <table-map>
        	<table-name>CMS_CCI_MAP</table-name>
	        <field-map>
	            <cmp-field>cMPGroupCCIMapID</cmp-field>
	            <dbms-column>CCI_MAP_ID</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPGroupCCINo</cmp-field>
	            <dbms-column>CMS_CCI_NO</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPGroupCCINoRef</cmp-field>
	            <dbms-column>CCI_REF_ID</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPSubProfileID</cmp-field>
	            <dbms-column>CMS_LE_SUB_PROFILE_ID</dbms-column>
	        </field-map>      
	        <field-map>
	            <cmp-field>cMPDeletedInd</cmp-field>
	            <dbms-column>IS_DELETED</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>versionTime</cmp-field>
	            <dbms-column>VERSION_TIME</dbms-column>
	        </field-map>
		</table-map>
    </weblogic-rdbms-bean>

    <weblogic-rdbms-bean>
        <ejb-name>EBCCICounterpartyDetailsStaging</ejb-name>
        <data-source-name>CMSPool</data-source-name>
        <table-map>
        	<table-name>CMS_STAGE_CCI_MAP</table-name>
	        <field-map>
	            <cmp-field>cMPGroupCCIMapID</cmp-field>
	            <dbms-column>CCI_MAP_ID</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPGroupCCINo</cmp-field>
	            <dbms-column>CMS_CCI_NO</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPGroupCCINoRef</cmp-field>
	            <dbms-column>CCI_REF_ID</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPSubProfileID</cmp-field>
	            <dbms-column>CMS_LE_SUB_PROFILE_ID</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>cMPDeletedInd</cmp-field>
	            <dbms-column>IS_DELETED</dbms-column>
	        </field-map>
	        <field-map>
	            <cmp-field>versionTime</cmp-field>
	            <dbms-column>VERSION_TIME</dbms-column>
	        </field-map>
		</table-map>
    </weblogic-rdbms-bean>
</weblogic-rdbms-jar>