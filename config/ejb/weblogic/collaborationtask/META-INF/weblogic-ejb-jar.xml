<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBCollaborationTaskProxyManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBCollaborationTaskProxyManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>SBCollaborationTaskBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBCollaborationTaskBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBStagingCollaborationTaskBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBStagingCollaborationTaskBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBCollateralTask</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
                <res-ref-name>jdbc/CMSPool</res-ref-name>              
                <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBCollateralTaskHome</jndi-name>
        <local-jndi-name>EBCollateralTaskLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  
    
    <weblogic-enterprise-bean>
        <ejb-name>EBStagingCollateralTask</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
                <res-ref-name>jdbc/CMSPool</res-ref-name>              
                <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBStagingCollateralTaskHome</jndi-name>
        <local-jndi-name>EBStagingCollateralTaskLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  

    <weblogic-enterprise-bean>
        <ejb-name>EBCCTask</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
                <res-ref-name>jdbc/CMSPool</res-ref-name>              
                <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBCCTaskHome</jndi-name>
        <local-jndi-name>EBCCTaskLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>  
    
    <weblogic-enterprise-bean>
        <ejb-name>EBStagingCCTask</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
            <resource-description>              
                <res-ref-name>jdbc/CMSPool</res-ref-name>              
                <jndi-name>CMSPool</jndi-name>          
            </resource-description>      
        </reference-descriptor>
        <jndi-name>EBStagingCCTaskHome</jndi-name>
        <local-jndi-name>EBStagingCCTaskLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>