<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar
  PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd">
<weblogic-rdbms-jar>
   <weblogic-rdbms-bean>
      <ejb-name>EBCurrency</ejb-name>
      <data-source-name>CMSPool</data-source-name>
      <table-map>
         <table-name>CURRENCY</table-name>
         <field-map>
            <cmp-field>currencyCode</cmp-field>
            <dbms-column>CURRENCY_ISO_CODE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>decimalPlace</cmp-field>
            <dbms-column>DECIMAL_PLACE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>countryCode</cmp-field>
            <dbms-column>CURRENCY_COUNTRY</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>currencyDescription</cmp-field>
            <dbms-column>CURRENCY_DESC</dbms-column>
         </field-map>
      </table-map>
   </weblogic-rdbms-bean>
   <weblogic-rdbms-bean>
      <ejb-name>EBCountry</ejb-name>
      <data-source-name>CMSPool</data-source-name>
      <table-map>
         <table-name>COUNTRY</table-name>
         <field-map>
            <cmp-field>countryCode</cmp-field>
            <dbms-column>COUNTRY_ISO_CODE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>countryName</cmp-field>
            <dbms-column>COUNTRY_NAME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>regionName</cmp-field>
            <dbms-column>REGION_NAME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>currencyCode</cmp-field>
            <dbms-column>CURRENCY_ISO_CODE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>swiftCode</cmp-field>
            <dbms-column>SWIFT_CODE</dbms-column>
         </field-map>
      </table-map>
   </weblogic-rdbms-bean>
</weblogic-rdbms-jar>