<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC
 "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN"
 "http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd">

<weblogic-ejb-jar>

    <weblogic-enterprise-bean>
        <ejb-name>SBPropertyIndexFeedProxy</ejb-name>
        <stateless-session-descriptor>
            <pool>
            </pool>
            <stateless-clustering>
            </stateless-clustering>
        </stateless-session-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <enable-call-by-reference>False</enable-call-by-reference>
        <jndi-name>SBPropertyIndexFeedProxyHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBPropertyIndexFeedEntry</ejb-name>
        <entity-descriptor>
            <entity-cache>
                <idle-timeout-seconds>30</idle-timeout-seconds>
				<cache-between-transactions>True</cache-between-transactions>
            </entity-cache>
            <persistence>
                <delay-updates-until-end-of-tx>True</delay-updates-until-end-of-tx>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>EBPropertyIndexFeedEntryHome</jndi-name>
        <local-jndi-name>EBPropertyIndexFeedEntryLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBPropertyIndexFeedEntryStaging</ejb-name>
        <entity-descriptor>
            <entity-cache>
                <idle-timeout-seconds>30</idle-timeout-seconds>
				<cache-between-transactions>True</cache-between-transactions>
            </entity-cache>
            <persistence>
                <delay-updates-until-end-of-tx>True</delay-updates-until-end-of-tx>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>EBPropertyIndexFeedEntryHomeStaging</jndi-name>
        <local-jndi-name>EBPropertyIndexFeedEntryLocalHomeStaging</local-jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBPropertyIndexFeedGroup</ejb-name>
        <entity-descriptor>
            <entity-cache>
                <idle-timeout-seconds>30</idle-timeout-seconds>
				<cache-between-transactions>True</cache-between-transactions>
            </entity-cache>
            <persistence>
                <delay-updates-until-end-of-tx>True</delay-updates-until-end-of-tx>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>            
        </reference-descriptor>
        <jndi-name>EBPropertyIndexFeedGroupHome</jndi-name>
        <local-jndi-name>EBPropertyIndexFeedGroupLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBPropertyIndexFeedGroupStaging</ejb-name>
        <entity-descriptor>
            <entity-cache>
                <idle-timeout-seconds>30</idle-timeout-seconds>
            </entity-cache>
            <persistence>
                <delay-updates-until-end-of-tx>True</delay-updates-until-end-of-tx>
				<cache-between-transactions>True</cache-between-transactions>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>            
        </reference-descriptor>
        <jndi-name>EBPropertyIndexFeedGroupHomeStaging</jndi-name>
        <local-jndi-name>EBPropertyIndexFeedGroupLocalHomeStaging</local-jndi-name>
    </weblogic-enterprise-bean>

</weblogic-ejb-jar>