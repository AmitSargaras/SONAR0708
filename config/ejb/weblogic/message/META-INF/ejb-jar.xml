<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN' 'http://java.sun.com/dtd/ejb-jar_2_0.dtd'>

<ejb-jar>
	<display-name>Message</display-name>
 <enterprise-beans>
  <session id="session_SBEaiMessageManager">
   <ejb-name>SBEaiMessageManager</ejb-name>
   <home>com.integrosys.cms.host.eai.service.SBEaiMessageManagerHome</home>
   <remote>com.integrosys.cms.host.eai.service.SBEaiMessageManager</remote>
   <ejb-class>com.integrosys.cms.host.eai.service.SBEaiMessageManagerBean</ejb-class>
   <session-type>Stateless</session-type>
   <transaction-type>Container</transaction-type>
      <resource-ref>
         <res-ref-name>jdbc/CMSPool</res-ref-name>
         <res-type>javax.sql.DataSource</res-type>
         <res-auth>Container</res-auth>
         <res-sharing-scope>Shareable</res-sharing-scope>
      </resource-ref>
  </session>
 </enterprise-beans>
<assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>SBEaiMessageManager</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>
