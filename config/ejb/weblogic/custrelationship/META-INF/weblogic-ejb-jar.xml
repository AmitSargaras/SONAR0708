<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBCustRelationshipProxy</ejb-name>
        <reference-descriptor>
		<resource-description>
			<res-ref-name>jdbc/CMSPool</res-ref-name>
			<jndi-name>CMSPool</jndi-name>
		</resource-description>
	</reference-descriptor>
        <jndi-name>SBCustRelationshipProxyHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>SBCustRelationshipBusManager</ejb-name>
        <reference-descriptor>
		<resource-description>
			<res-ref-name>jdbc/CMSPool</res-ref-name>
			<jndi-name>CMSPool</jndi-name>
		</resource-description>
	</reference-descriptor>
        <jndi-name>SBCustRelationshipBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>SBCustRelationshipBusManagerStaging</ejb-name>
        <reference-descriptor>
		<resource-description>
			<res-ref-name>jdbc/CMSPool</res-ref-name>
			<jndi-name>CMSPool</jndi-name>
		</resource-description>
	</reference-descriptor>
        <jndi-name>SBCustRelationshipBusManagerStagingHome</jndi-name>
    </weblogic-enterprise-bean>
        
    <weblogic-enterprise-bean>
        <ejb-name>EBCustRelationship</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
			<resource-description>              
				<res-ref-name>jdbc/CMSPool</res-ref-name>              
				<jndi-name>CMSPool</jndi-name>          
			</resource-description>      
        </reference-descriptor>
        <jndi-name>EBCustRelationshipHome</jndi-name>
        <local-jndi-name>EBCustRelationshipLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBCustRelationshipStaging</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
			<resource-description>              
				<res-ref-name>jdbc/CMSPool</res-ref-name>              
				<jndi-name>CMSPool</jndi-name>          
			</resource-description>      
        </reference-descriptor>
        <jndi-name>EBCustRelationshipStagingHome</jndi-name>
        <local-jndi-name>EBCustRelationshipStagingLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>

	<weblogic-enterprise-bean>
        <ejb-name>EBCustShareholder</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
			<resource-description>              
				<res-ref-name>jdbc/CMSPool</res-ref-name>              
				<jndi-name>CMSPool</jndi-name>          
			</resource-description>      
        </reference-descriptor>
        <jndi-name>EBCustShareholderHome</jndi-name>
        <local-jndi-name>EBCustShareholderLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBCustShareholderStaging</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>          
			<resource-description>              
				<res-ref-name>jdbc/CMSPool</res-ref-name>              
				<jndi-name>CMSPool</jndi-name>          
			</resource-description>      
        </reference-descriptor>
        <jndi-name>EBCustShareholderStagingHome</jndi-name>
        <local-jndi-name>EBCustShareholderStagingLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>