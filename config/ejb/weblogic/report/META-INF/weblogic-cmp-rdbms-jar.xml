<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar
  PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd">
<weblogic-rdbms-jar>
   <weblogic-rdbms-bean>
      <ejb-name>EBReportRequestBean</ejb-name>
      <data-source-name>CMSPool</data-source-name>
      <table-map>
         <table-name>CMS_REPORT_REQUEST</table-name>
         <field-map>
            <cmp-field>eBReportRequestID</cmp-field>
            <dbms-column>REPORT_REQUEST_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>reportID</cmp-field>
            <dbms-column>REPORT_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>reportName</cmp-field>
            <dbms-column>REPORT_NAME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>parameters</cmp-field>
            <dbms-column>PARAMETERS</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>eBUserID</cmp-field>
            <dbms-column>USER_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>requestTime</cmp-field>
            <dbms-column>REQUEST_TIME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>reportGenTime</cmp-field>
            <dbms-column>REPORT_GEN_TIME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>status</cmp-field>
            <dbms-column>STATUS</dbms-column>
         </field-map>
      </table-map>
   </weblogic-rdbms-bean>
</weblogic-rdbms-jar>