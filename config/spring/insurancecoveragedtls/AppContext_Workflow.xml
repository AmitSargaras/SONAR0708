		<property name="insuranceCoverageDtlsBusManager" ref="insuranceCoverageDtlsBusManager" />
		<property name="stagingInsuranceCoverageDtlsBusManager" ref="stagingInsuranceCoverageDtlsBusManager" />
	</bean>
	<bean id="InsuranceCoverageDtlsTrx.MakerCloseRejectedUpdateInsuranceCoverageDtlsOperation"
