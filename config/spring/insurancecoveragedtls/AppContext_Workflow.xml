		class="com.integrosys.cms.app.insurancecoveragedtls.trx.MakerSubmitSaveUpdateInsuranceCoverageDtlsOperation">
		<property name="insuranceCoverageDtlsBusManager" ref="insuranceCoverageDtlsBusManager" />
		<property name="stagingInsuranceCoverageDtlsBusManager" ref="stagingInsuranceCoverageDtlsBusManager" />
	</bean>
	
	<bean id="InsuranceCoverageDtlsTrx.MakerSaveUpdateInsuranceCoverageDtlsOperation"
		class="com.integrosys.cms.app.insurancecoveragedtls.trx.MakerSaveUpdateInsuranceCoverageDtlsOperation">
