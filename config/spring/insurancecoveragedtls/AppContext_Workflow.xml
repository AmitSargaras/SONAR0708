		<property name="stagingInsuranceCoverageDtlsBusManager" ref="stagingInsuranceCoverageDtlsBusManager" />
	</bean>
	
    <bean id="insuranceCoverageDtlsReadController" class="com.integrosys.cms.app.insurancecoveragedtls.trx.InsuranceCoverageDtlsReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.ReadInsuranceCoverageDtlsOperation"/>
                 <entry key="ReadInsuranceCoverageDtlsIDOperation" value-ref="InsuranceCoverageDtlsTrx.ReadInsuranceCoverageDtlsIDOperation"/>
            </map>
        </property>
    </bean>

    <bean id="insuranceCoverageDtlsTrxController" class="com.integrosys.cms.app.insurancecoveragedtls.trx.InsuranceCoverageDtlsTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerCreateInsuranceCoverageDtlsOperation"/>
            	  
            	  <entry key="MakerSaveCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerSaveCreateInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerSubmitSaveCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerSubmitSaveCreateInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerCloseDraftCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerCloseDraftCreateInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerSubmitSaveUpdateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerSubmitSaveUpdateInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerSaveUpdateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerSaveUpdateInsuranceCoverageDtlsOperation"/>
            	  
            	  	
				  <entry key="MakerUpdateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerUpdateInsuranceCoverageDtlsOperation"/>
				  <entry key="MakerDeleteInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerDeleteInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerEditRejectedCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerEditRejectedCreateInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerEditRejectedUpdateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerEditRejectedUpdateInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerEditRejectedDeleteInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerEditRejectedDeleteInsuranceCoverageDtlsOperation"/>
            	  <entry key="MakerCloseRejectedCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerCloseRejectedCreateInsuranceCoverageDtlsOperation"/>
                  <entry key="MakerCloseRejectedUpdateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerCloseRejectedUpdateInsuranceCoverageDtlsOperation"/>
                  <entry key="MakerCloseRejectedDeleteInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.MakerCloseRejectedDeleteInsuranceCoverageDtlsOperation"/>

                  <entry key="CheckerRejectInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.CheckerRejectInsuranceCoverageDtlsOperation"/>
                  <entry key="CheckerApproveUpdateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.CheckerApproveUpdateInsuranceCoverageDtlsOperation"/>
                  <entry key="CheckerApproveCreateInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.CheckerApproveCreateInsuranceCoverageDtlsOperation"/>
                  <entry key="CheckerApproveDeleteInsuranceCoverageDtlsOperation" value-ref="InsuranceCoverageDtlsTrx.CheckerApproveDeleteInsuranceCoverageDtlsOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

    <bean id="insuranceCoverageDtlsTrxControllerFactory" class="com.integrosys.cms.app.insurancecoveragedtls.trx.InsuranceCoverageDtlsTrxControllerFactory">
        <property name="insuranceCoverageDtlsReadController" ref="insuranceCoverageDtlsReadController"/>
        <property name="insuranceCoverageDtlsTrxController" ref="insuranceCoverageDtlsTrxController"/>
    </bean>
</beans>