	<bean id="InsuranceCoverageDtlsTrx.MakerCloseRejectedDeleteInsuranceCoverageDtlsOperation"
		class="com.integrosys.cms.app.insurancecoveragedtls.trx.MakerCloseRejectedDeleteInsuranceCoverageDtlsOperation">
