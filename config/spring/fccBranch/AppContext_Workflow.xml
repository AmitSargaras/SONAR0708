        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadFCCBranchOperation" value-ref="FCCBranchTrx.ReadFCCBranchOperation"/>
                 <entry key="ReadFCCBranchIDOperation" value-ref="FCCBranchTrx.ReadFCCBranchIDOperation"/>
                
                
            </map>
        </property>
    </bean>

    <bean id="fccBranchTrxController" class="com.integrosys.cms.app.fccBranch.trx.FCCBranchTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerEditRejectedUpdateFCCBranchOperation" value-ref="FCCBranchTrx.MakerEditRejectedUpdateFCCBranchOperation"/>
            	   <entry key="MakerEditRejectedCreateFCCBranchOperation" value-ref="FCCBranchTrx.MakerEditRejectedCreateFCCBranchOperation"/>
            	  <entry key="MakerEditRejectedDeleteFCCBranchOperation" value-ref="FCCBranchTrx.MakerEditRejectedDeleteFCCBranchOperation"/>
                  <entry key="MakerUpdateFCCBranchOperation" value-ref="FCCBranchTrx.MakerUpdateFCCBranchOperation"/>
                  <entry key="MakerUpdateDraftCreateFCCBranchOperation" value-ref="FCCBranchTrx.MakerUpdateDraftCreateFCCBranchOperation"/>
                  <entry key="MakerCreateFCCBranchOperation" value-ref="FCCBranchTrx.MakerCreateFCCBranchOperation"/>
                   <entry key="MakerDeleteFCCBranchOperation" value-ref="FCCBranchTrx.MakerDeleteFCCBranchOperation"/>
                  <entry key="MakerCloseRejectedUpdateFCCBranchOperation" value-ref="FCCBranchTrx.MakerCloseRejectedUpdateFCCBranchOperation"/>
                  <entry key="MakerCloseDraftFCCBranchOperation" value-ref="FCCBranchTrx.MakerCloseDraftFCCBranchOperation"/>
                  <entry key="MakerCloseRejectedCreateFCCBranchOperation" value-ref="FCCBranchTrx.MakerCloseRejectedCreateFCCBranchOperation"/>
                  <entry key="MakerCloseRejectedDeleteFCCBranchOperation" value-ref="FCCBranchTrx.MakerCloseRejectedDeleteFCCBranchOperation"/>
                  <entry key="CheckerApproveUpdateFCCBranchOperation" value-ref="FCCBranchTrx.CheckerApproveUpdateFCCBranchOperation"/>
                  <entry key="CheckerApproveCreateFCCBranchOperation" value-ref="FCCBranchTrx.CheckerApproveCreateFCCBranchOperation"/>
                  <entry key="CheckerRejectFCCBranchOperation" value-ref="FCCBranchTrx.CheckerRejectFCCBranchOperation"/>
                  <entry key="CheckerCreateRejectFCCBranchOperation" value-ref="FCCBranchTrx.CheckerCreateRejectFCCBranchOperation"/>
                  <entry key="CheckerEditRejectFCCBranchOperation" value-ref="FCCBranchTrx.CheckerEditRejectFCCBranchOperation"/>
                  <entry key="CheckerDeleteRejectFCCBranchOperation" value-ref="FCCBranchTrx.CheckerDeleteRejectFCCBranchOperation"/>
                  <entry key="MakerSaveFCCBranchOperation" value-ref="FCCBranchTrx.MakerSaveFCCBranchOperation"/>
                  <entry key="MakerSaveUpdateFCCBranchOperation" value-ref="FCCBranchTrx.MakerSaveUpdateFCCBranchOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
