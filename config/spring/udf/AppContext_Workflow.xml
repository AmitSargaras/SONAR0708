            <entry key="CheckerRejectUdfOperation" value-ref="UdfTrx.CheckerRejectUdfOperation"/>
            <entry key="MakerEditRejectedCreateUdfOperation" value-ref="UdfTrx.MakerEditRejectedCreateUdfOperation"/>
            <entry key="MakerEditRejectedUpdateUdfOperation" value-ref="UdfTrx.MakerEditRejectedUpdateUdfOperation"/>
            <entry key="MakerCloseRejectedCreateUdfOperation" value-ref="UdfTrx.MakerCloseRejectedCreateUdfOperation"/>
            <entry key="MakerCloseRejectedUpdateUdfOperation" value-ref="UdfTrx.MakerCloseRejectedUpdateUdfOperation"/>
            <entry key="MakerSaveUpdateUdfOperation" value-ref="UdfTrx.MakerSaveUpdateUdfOperation"/>
            <entry key="MakerSaveUdfOperation" value-ref="UdfTrx.MakerSaveUdfOperation"/>
            <entry key="MakerUpdateDraftCreateUdfOperation" value-ref="UdfTrx.MakerUpdateDraftCreateUdfOperation"/>
   			<entry key="MakerCloseDraftUdfOperation" value-ref="UdfTrx.MakerCloseDraftUdfOperation"/>
   			
   			<entry key="CheckerApproveDeleteUdfOperation" value-ref="UdfTrx.CheckerApproveDeleteUdfOperation"/>
   			<entry key="CheckerApproveEnableUdfOperation" value-ref="UdfTrx.CheckerApproveEnableUdfOperation"/>
   			<entry key="CheckerRejectActivateUdfOperation" value-ref="UdfTrx.CheckerRejectActivateUdfOperation"/>
   			<entry key="MakerDeleteUdfOperation" value-ref="UdfTrx.MakerDeleteUdfOperation"/>
   			<entry key="MakerActivateUdfOperation" value-ref="UdfTrx.MakerActivateUdfOperation"/>
   			<entry key="MakerCloseRejectedDisableUdfOperation" value-ref="UdfTrx.MakerCloseRejectedDisableUdfOperation"/>
   			<entry key="MakerEditRejectedDeleteUdfOperation" value-ref="UdfTrx.MakerEditRejectedDeleteUdfOperation"/>
   			<entry key="MakerEditRejectedEnableUdfOperation" value-ref="UdfTrx.MakerEditRejectedEnableUdfOperation"/>
   			
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

    <bean id="udfTrxControllerFactory" class="com.integrosys.cms.app.udf.trx.UdfTrxControllerFactory">
        <property name="udfReadController" ref="udfReadController"/>
        <property name="udfTrxController" ref="udfTrxController"/>
    </bean> 
