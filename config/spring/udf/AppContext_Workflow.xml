    <bean id="udfReadController" class="com.integrosys.cms.app.udf.trx.UdfReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadUdfOperation" value-ref="UdfTrx.ReadUdfOperation"/>
                 <entry key="ReadUdfIDOperation" value-ref="UdfTrx.ReadUdfIDOperation"/>  
            </map>
        </property>
    </bean> 
    <bean id="udfTrxController" class="com.integrosys.cms.app.udf.trx.UdfTrxController">
         <property name="nameTrxOperationMap">
        <map>
            <entry key="MakerCreateUdfOperation" value-ref="UdfTrx.MakerCreateUdfOperation"/>
            <entry key="CheckerApproveCreateUdfOperation" value-ref="UdfTrx.CheckerApproveCreateUdfOperation"/>
           
            <!--added by santosh-UBS CR  -->
            <entry key="MakerUpdateUdfOperation" value-ref="UdfTrx.MakerUpdateUdfOperation"/>
