            <map>
            	  <entry key="MakerEditRejectedUpdateHolidayOperation" value-ref="HolidayTrx.MakerEditRejectedUpdateHolidayOperation"/>
            	   <entry key="MakerEditRejectedCreateHolidayOperation" value-ref="HolidayTrx.MakerEditRejectedCreateHolidayOperation"/>
            	  <entry key="MakerEditRejectedDeleteHolidayOperation" value-ref="HolidayTrx.MakerEditRejectedDeleteHolidayOperation"/>
