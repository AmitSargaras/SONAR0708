    <bean id="holidayReadController" class="com.integrosys.cms.app.holiday.trx.HolidayReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadHolidayOperation" value-ref="HolidayTrx.ReadHolidayOperation"/>
                 <entry key="ReadHolidayIDOperation" value-ref="HolidayTrx.ReadHolidayIDOperation"/>
                
                
