                  <entry key="MakerCloseRejectedCreateHolidayOperation" value-ref="HolidayTrx.MakerCloseRejectedCreateHolidayOperation"/>
                  <entry key="MakerCloseRejectedDeleteHolidayOperation" value-ref="HolidayTrx.MakerCloseRejectedDeleteHolidayOperation"/>
                  <entry key="CheckerApproveUpdateHolidayOperation" value-ref="HolidayTrx.CheckerApproveUpdateHolidayOperation"/>
                  <entry key="CheckerApproveCreateHolidayOperation" value-ref="HolidayTrx.CheckerApproveCreateHolidayOperation"/>
                  <entry key="CheckerRejectHolidayOperation" value-ref="HolidayTrx.CheckerRejectHolidayOperation"/>
                  <entry key="CheckerCreateRejectHolidayOperation" value-ref="HolidayTrx.CheckerCreateRejectHolidayOperation"/>
                  <entry key="CheckerEditRejectHolidayOperation" value-ref="HolidayTrx.CheckerEditRejectHolidayOperation"/>
                  <entry key="CheckerDeleteRejectHolidayOperation" value-ref="HolidayTrx.CheckerDeleteRejectHolidayOperation"/>
                  <entry key="MakerSaveHolidayOperation" value-ref="HolidayTrx.MakerSaveHolidayOperation"/>
                  <entry key="MakerSaveUpdateHolidayOperation" value-ref="HolidayTrx.MakerSaveUpdateHolidayOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
