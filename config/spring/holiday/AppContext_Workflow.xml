                  <entry key="MakerUpdateDraftCreateHolidayOperation" value-ref="HolidayTrx.MakerUpdateDraftCreateHolidayOperation"/>
                  <entry key="MakerCreateHolidayOperation" value-ref="HolidayTrx.MakerCreateHolidayOperation"/>
