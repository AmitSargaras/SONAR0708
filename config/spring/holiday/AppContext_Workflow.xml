        <property name="holidayTrxController" ref="holidayTrxController"/>
        <property name="holidayInsertFileTrxController" ref="holidayInsertFileTrxController"/>
        <property name="readHolidayInsertFileTrxController" ref="readHolidayInsertFileTrxController"/>
