<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

	<bean id="slsbCheckListProxyManager" class="org.springframework.ejb.access.SimpleRemoteStatelessSessionProxyFactoryBean">
	  <property name="jndiName" value="SBCheckListProxyManagerHome" />
	  <property name="businessInterface" value="com.integrosys.cms.app.checklist.proxy.SBCheckListProxyManager" />
	  <property name="lookupHomeOnStartup" value="false" />
	</bean>

	<bean id="slsbCheckListTemplateProxyManager" class="org.springframework.ejb.access.SimpleRemoteStatelessSessionProxyFactoryBean">
	  <property name="jndiName" value="SBCheckListTemplateProxyManagerHome" />
	  <property name="businessInterface" value="com.integrosys.cms.app.chktemplate.proxy.SBCheckListTemplateProxyManager" />
	  <property name="lookupHomeOnStartup" value="false" />
	</bean>

  	<bean id="checklistProxy" class="com.integrosys.cms.app.checklist.proxy.CheckListProxyManagerImpl">
  		<property name="slsbCheckListProxyManager" ref="slsbCheckListProxyManager" />
  	</bean>
  	
  	<bean id="templateProxy" class="com.integrosys.cms.app.chktemplate.proxy.CheckListTemplateProxyManagerImpl">
  		<property name="slsbCheckListTemplateProxyManager" ref="slsbCheckListTemplateProxyManager" />
  	</bean>
</beans>