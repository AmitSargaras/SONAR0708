<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

  <bean id="RecurrentDocCheckListAction.UpdateDeleteRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.UpdateDeleteRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.ReadStagingRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.ReadStagingRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.ReplaceRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.ReplaceRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.CopyItemCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.CopyItemCommand" />

  <bean id="RecurrentDocCheckListAction.AddRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.AddRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.ApproveRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.ApproveRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="updateShareDocumentRequired" value="${checklist.approve.required.update.sharedoc}"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.RemoveRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.RemoveRecurrentDocCheckListCommand" />

  <bean id="RecurrentDocCheckListAction.UpdateRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.UpdateRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.MaintainRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.MaintainRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="checkListTemplateProxyManager" ref="templateProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.DisableRecurrentDocColTaskCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.DisableRecurrentDocColTaskCommand" />

  <bean id="RecurrentDocCheckListAction.ReplaceItemCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.ReplaceItemCommand" />

  <bean id="RecurrentDocCheckListAction.SubmitRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.SubmitRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="maintainChecklistWithoutApproval" value="${checklist.maintain.suppress.checker.approval}"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.LinkItemCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.LinkItemCommand" />

  <bean id="RecurrentDocCheckListAction.SubmitDeleteRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.SubmitDeleteRecurrentDocCheckListCommand">
   <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.ListRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.ListRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.AddCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.AddCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.CloseRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.CloseRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.LinkRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.LinkRecurrentDocCheckListCommand" />

  <bean id="RecurrentDocCheckListAction.RejectRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.RejectRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="RecurrentDocCheckListAction.FrameFlagSetterCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.FrameFlagSetterCommand" />

  <bean id="RecurrentDocCheckListAction.CopyRecurrentDocCheckListCommand" class="com.integrosys.cms.ui.checklist.recurrentDoc.CopyRecurrentDocCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

</beans>