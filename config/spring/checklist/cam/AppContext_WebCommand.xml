<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

  <bean id="CAMCheckListAction.UpdateDeleteCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.UpdateDeleteCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.ReadStagingCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.ReadStagingCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.ReplaceCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.ReplaceCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.CopyItemCommand" class="com.integrosys.cms.ui.checklist.cam.CopyItemCommand" />

  <bean id="CAMCheckListAction.AddCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.AddCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.ApproveCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.ApproveCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="updateShareDocumentRequired" value="${checklist.approve.required.update.sharedoc}"/>
  </bean>

  <bean id="CAMCheckListAction.RemoveCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.RemoveCAMCheckListCommand" />

  <bean id="CAMCheckListAction.UpdateCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.UpdateCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.MaintainCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.MaintainCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="checkListTemplateProxyManager" ref="templateProxy"/>
  </bean>

  <bean id="CAMCheckListAction.DisableCAMColTaskCommand" class="com.integrosys.cms.ui.checklist.cam.DisableCAMColTaskCommand" />

  <bean id="CAMCheckListAction.ReplaceItemCommand" class="com.integrosys.cms.ui.checklist.cam.ReplaceItemCommand" />

  <bean id="CAMCheckListAction.SubmitCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.SubmitCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="maintainChecklistWithoutApproval" value="${checklist.maintain.suppress.checker.approval}"/>
  </bean>

  <bean id="CAMCheckListAction.LinkItemCommand" class="com.integrosys.cms.ui.checklist.cam.LinkItemCommand" />

  <bean id="CAMCheckListAction.SubmitDeleteCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.SubmitDeleteCAMCheckListCommand">
   <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.ListCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.ListCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.AddCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.AddCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.CloseCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.CloseCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.LinkCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.LinkCAMCheckListCommand" />

  <bean id="CAMCheckListAction.RejectCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.RejectCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CAMCheckListAction.FrameFlagSetterCommand" class="com.integrosys.cms.ui.checklist.cam.FrameFlagSetterCommand" />

  <bean id="CAMCheckListAction.CopyCAMCheckListCommand" class="com.integrosys.cms.ui.checklist.cam.CopyCAMCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

</beans>