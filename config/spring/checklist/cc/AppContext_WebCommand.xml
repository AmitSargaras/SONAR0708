<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

  <bean id="CCCheckListAction.UpdateDeleteCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.UpdateDeleteCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.CloseCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.CloseCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.CopyItemCommand" class="com.integrosys.cms.ui.checklist.cc.CopyItemCommand" />

  <bean id="CCCheckListAction.RemoveCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.RemoveCCCheckListCommand" />

  <bean id="CCCheckListAction.AddCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.AddCCCheckListCommand" />

  <bean id="CCCheckListAction.DispatchCCPageCommand" class="com.integrosys.cms.ui.checklist.cc.DispatchCCPageCommand" />

  <bean id="CCCheckListAction.CopyCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.CopyCCCheckListCommand" />

  <bean id="CCCheckListAction.RejectCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.RejectCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.ApproveCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.ApproveCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="updateShareDocumentRequired" value="${checklist.approve.required.update.sharedoc}"/>
  </bean>

  <bean id="CCCheckListAction.MaintainCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.MaintainCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
	<property name="checkListTemplateProxyManager" ref="templateProxy"/>
  </bean>

  <bean id="CCCheckListAction.LinkCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.LinkCCCheckListCommand" />

  <bean id="CCCheckListAction.ReplaceItemCommand" class="com.integrosys.cms.ui.checklist.cc.ReplaceItemCommand" />

  <bean id="CCCheckListAction.UpdateCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.UpdateCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.SubmitCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.SubmitCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
    <property name="maintainChecklistWithoutApproval" value="${checklist.maintain.suppress.checker.approval}"/>
  </bean>

  <bean id="CCCheckListAction.LinkItemCommand" class="com.integrosys.cms.ui.checklist.cc.LinkItemCommand" />

  <bean id="CCCheckListAction.ListCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.ListCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.SubmitDeleteCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.SubmitDeleteCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.ReplaceCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.ReplaceCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.AddCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.AddCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.ReadStagingCCCheckListCommand" class="com.integrosys.cms.ui.checklist.cc.ReadStagingCCCheckListCommand">
    <property name="checkListProxyManager" ref="checklistProxy"/>
  </bean>

  <bean id="CCCheckListAction.FrameFlagSetterCommand" class="com.integrosys.cms.ui.checklist.cc.FrameFlagSetterCommand" />

</beans>