	</bean>
	<bean id="MakerSavePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerSavePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerEditRejectedCreatePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerEditRejectedCreatePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerEditRejectedUpdatePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerEditRejectedUpdatePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerEditRejectedDeletePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerEditRejectedDeletePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerEditRejectedEnablePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerEditRejectedEnablePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerCloseRejectedCreatePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerCloseRejectedCreatePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerCloseRejectedUpdatePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerCloseRejectedUpdatePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerCloseDraftPincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerCloseDraftPincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerCloseRejectedDisablePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerCloseRejectedDisablePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerSaveUpdatePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerSaveUpdatePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="MakerDeletePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.MakerDeletePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="CheckerApproveUpdatePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.CheckerApproveUpdatePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="CheckerApproveDeletePincodeMappingOperation"
		class="com.integrosys.cms.app.pincodemapping.trx.CheckerApproveDeletePincodeMappingOperation">
		<property name="pincodeMappingBusManager" ref="pincodeMappingBusManager" />
		<property name="stagingPincodeMappingBusManager" ref="stagingPincodeMappingBusManager" />
	</bean>
	<bean id="pincodeMappingReadController"
		class="com.integrosys.cms.app.pincodemapping.trx.PincodeMappingReadController">
		<property name="nameTrxOperationMap">
			<map>
				<entry key="ReadPincodeMappingOperation" value-ref="ReadPincodeMappingOperation" />
				<entry key="ReadPincodeMappingIDOperation" value-ref="ReadPincodeMappingIDOperation" />


			</map>
		</property>
	</bean>

	<bean id="pincodeMappingTrxController"
		class="com.integrosys.cms.app.pincodemapping.trx.PincodeMappingTrxController">
		<property name="nameTrxOperationMap">
			<map>
			<entry key="CheckerApproveCreatePincodeMappingOperation"
					value-ref="CheckerApproveCreatePincodeMappingOperation" />
					<entry key="CheckerRejectPincodeMappingOperation" value-ref="CheckerRejectPincodeMappingOperation" />
					<entry key="MakerSavePincodeMappingOperation" value-ref="MakerSavePincodeMappingOperation" />
					<entry key="MakerEditRejectedCreatePincodeMappingOperation"
					value-ref="MakerEditRejectedCreatePincodeMappingOperation" />
					 <entry key="MakerEditRejectedUpdatePincodeMappingOperation"
						value-ref="MakerEditRejectedUpdatePincodeMappingOperation" />
						<entry key="MakerEditRejectedDeletePincodeMappingOperation"
					value-ref="MakerEditRejectedDeletePincodeMappingOperation" />
					<entry key="MakerEditRejectedEnablePincodeMappingOperation"
					value-ref="MakerEditRejectedEnablePincodeMappingOperation" />
					<entry key="MakerCloseRejectedCreatePincodeMappingOperation"
					value-ref="MakerCloseRejectedCreatePincodeMappingOperation" />
					<entry key="MakerCloseRejectedUpdatePincodeMappingOperation"
					value-ref="MakerCloseRejectedUpdatePincodeMappingOperation" />
					<entry key="MakerCloseRejectedDisablePincodeMappingOperation"
					value-ref="MakerCloseRejectedDisablePincodeMappingOperation" />
					<entry key="MakerCloseDraftPincodeMappingOperation" value-ref="MakerCloseDraftPincodeMappingOperation" />
					<entry key="MakerCreateSavedPincodeMappingOperation" value-ref="MakerCreateSavedPincodeMappingOperation" />
					<entry key="MakerUpdatePincodeMappingOperation" value-ref="MakerUpdatePincodeMappingOperation" />
					<entry key="MakerSaveUpdatePincodeMappingOperation" value-ref="MakerSaveUpdatePincodeMappingOperation" />
					<entry key="MakerDeletePincodeMappingOperation" value-ref="MakerDeletePincodeMappingOperation" />
					<entry key="CheckerApproveDeletePincodeMappingOperation"
					value-ref="CheckerApproveDeletePincodeMappingOperation" />
					<entry key="CheckerApproveUpdatePincodeMappingOperation"
					value-ref="CheckerApproveUpdatePincodeMappingOperation" />
				<!--
				
				
				
				
				
				
			
				
				
				
				<entry key="CheckerApproveUpdatePartyGroupOperation"
					value-ref="CheckerApproveUpdatePartyGroupOperation" />
				
				
				<entry key="MakerActivatePartyGroupOperation" value-ref="MakerActivatePartyGroupOperation" />
				<entry key="CheckerApproveEnablePartyGroupOperation"
					value-ref="CheckerApproveEnablePartyGroupOperation" />
				
				
				<entry key="CheckerRejectActivatePartyGroupOperation"
					value-ref="CheckerRejectActivatePartyGroupOperation" />
				
				
				
				
 -->
 <entry key="MakerCreatePincodeMappingOperation" value-ref="MakerCreatePincodeMappingOperation" />
			</map>
		</property>
		<property name="processTrxManager" ref="processTrxManager" />
	</bean>

	<bean id="pincodeMappingTrxControllerFactory"
		class="com.integrosys.cms.app.pincodemapping.trx.PincodeMappingTrxControllerFactory">
		<property name="pincodeMappingReadController" ref="pincodeMappingReadController" />
		<property name="pincodeMappingTrxController" ref="pincodeMappingTrxController" />
	</bean>
	
</beans>