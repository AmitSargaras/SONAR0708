    <bean id="insuranceCoverageReadController" class="com.integrosys.cms.app.insurancecoverage.trx.InsuranceCoverageReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.ReadInsuranceCoverageOperation"/>
                 <entry key="ReadInsuranceCoverageIDOperation" value-ref="InsuranceCoverageTrx.ReadInsuranceCoverageIDOperation"/>
            </map>
        </property>
    </bean>

    <bean id="insuranceCoverageTrxController" class="com.integrosys.cms.app.insurancecoverage.trx.InsuranceCoverageTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerCreateInsuranceCoverageOperation"/>
            	  
            	  <entry key="MakerSaveCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerSaveCreateInsuranceCoverageOperation"/>
            	  <entry key="MakerSubmitSaveCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerSubmitSaveCreateInsuranceCoverageOperation"/>
            	  <entry key="MakerCloseDraftCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerCloseDraftCreateInsuranceCoverageOperation"/>
            	  <entry key="MakerSubmitSaveUpdateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerSubmitSaveUpdateInsuranceCoverageOperation"/>
            	  <entry key="MakerUpdateSaveInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerUpdateSaveInsuranceCoverageOperation"/>
            	  	
				  <entry key="MakerUpdateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerUpdateInsuranceCoverageOperation"/>
				  <entry key="MakerDeleteInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerDeleteInsuranceCoverageOperation"/>
            	  <entry key="MakerEditRejectedCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerEditRejectedCreateInsuranceCoverageOperation"/>
            	  <entry key="MakerEditRejectedUpdateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerEditRejectedUpdateInsuranceCoverageOperation"/>
            	  <entry key="MakerEditRejectedDeleteInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerEditRejectedDeleteInsuranceCoverageOperation"/>
            	  <entry key="MakerCloseRejectedCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerCloseRejectedCreateInsuranceCoverageOperation"/>
                  <entry key="MakerCloseRejectedUpdateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerCloseRejectedUpdateInsuranceCoverageOperation"/>
                  <entry key="MakerCloseRejectedDeleteInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.MakerCloseRejectedDeleteInsuranceCoverageOperation"/>

                  <entry key="CheckerRejectInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.CheckerRejectInsuranceCoverageOperation"/>
                  <entry key="CheckerApproveUpdateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.CheckerApproveUpdateInsuranceCoverageOperation"/>
                  <entry key="CheckerApproveCreateInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.CheckerApproveCreateInsuranceCoverageOperation"/>
                  <entry key="CheckerApproveDeleteInsuranceCoverageOperation" value-ref="InsuranceCoverageTrx.CheckerApproveDeleteInsuranceCoverageOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
	
	
