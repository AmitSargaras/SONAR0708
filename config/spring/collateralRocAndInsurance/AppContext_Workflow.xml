<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

    <description>
        To be used for transaction controller and transaction operation.
        Transaction operation will have the property to set bus manager, which get from 'AppContext_Services.xml'
    </description>

	<bean id="CollateralRocTrx.ReadCollateralRocOperation" class="com.integrosys.cms.app.collateralrocandinsurance.trx.ReadCollateralRocOperation">
        <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
    </bean>
    
    <bean id="CollateralRocTrx.ReadCollateralRocIDOperation" class="com.integrosys.cms.app.collateralrocandinsurance.trx.ReadCollateralRocIDOperation">
        <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
    </bean>
    
    <bean id="CollateralRocTrx.MakerCreateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerCreateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.CheckerApproveCreateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.CheckerApproveCreateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.CheckerRejectCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.CheckerRejectCollateralRocOperation">
		<property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>

	<bean id="CollateralRocTrx.MakerSaveCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerSaveCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerSaveUpdateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerSaveUpdateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerUpdateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerUpdateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.CheckerApproveUpdateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.CheckerApproveUpdateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerDeleteCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerDeleteCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerUpdateDraftCreateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerUpdateDraftCreateCollateralRocOperation">
		<property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerEditRejectedCreateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerEditRejectedCreateCollateralRocOperation">
		<property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerEditRejectedUpdateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerEditRejectedUpdateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerEditRejectedDeleteCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerEditRejectedDeleteCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerCloseRejectedCreateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerCloseRejectedCreateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerCloseRejectedUpdateCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerCloseRejectedUpdateCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerCloseRejectedDeleteCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerCloseRejectedDeleteCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean>
	
	<bean id="CollateralRocTrx.MakerCloseDraftCollateralRocOperation"
		class="com.integrosys.cms.app.collateralrocandinsurance.trx.MakerCloseDraftCollateralRocOperation">
		 <property name="collateralRocBusManager" ref="collateralRocBusManager"/>
        <property name="stagingCollateralRocBusManager" ref="stagingCollateralRocBusManager"/>
	</bean> 
	
    <bean id="collateralRocReadController" class="com.integrosys.cms.app.collateralrocandinsurance.trx.CollateralRocReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadCollateralRocOperation" value-ref="CollateralRocTrx.ReadCollateralRocOperation"/>
                 <entry key="ReadCollateralRocIDOperation" value-ref="CollateralRocTrx.ReadCollateralRocIDOperation"/>  
            </map>
        </property>
    </bean>

    <bean id="collateralRocTrxController" class="com.integrosys.cms.app.collateralrocandinsurance.trx.CollateralRocTrxController">
        <property name="nameTrxOperationMap">
            <map>
            <entry key="MakerCreateCollateralRocOperation" value-ref="CollateralRocTrx.MakerCreateCollateralRocOperation"/>
            <entry key="CheckerApproveCreateCollateralRocOperation" value-ref="CollateralRocTrx.CheckerApproveCreateCollateralRocOperation"/>
            <entry key="CheckerRejectCollateralRocOperation" value-ref="CollateralRocTrx.CheckerRejectCollateralRocOperation"/>
            <entry key="MakerSaveCollateralRocOperation" value-ref="CollateralRocTrx.MakerSaveCollateralRocOperation"/>
            <entry key="MakerUpdateCollateralRocOperation" value-ref="CollateralRocTrx.MakerUpdateCollateralRocOperation"/>
            <entry key="CheckerApproveUpdateCollateralRocOperation" value-ref="CollateralRocTrx.CheckerApproveUpdateCollateralRocOperation"/>
            <entry key="MakerDeleteCollateralRocOperation" value-ref="CollateralRocTrx.MakerDeleteCollateralRocOperation"/>
            <entry key="MakerUpdateDraftCreateCollateralRocOperation" value-ref="CollateralRocTrx.MakerUpdateDraftCreateCollateralRocOperation"/>
            <entry key="MakerEditRejectedCreateCollateralRocOperation" value-ref="CollateralRocTrx.MakerEditRejectedCreateCollateralRocOperation"/>
            <entry key="MakerEditRejectedUpdateCollateralRocOperation" value-ref="CollateralRocTrx.MakerEditRejectedUpdateCollateralRocOperation"/>
            <entry key="MakerEditRejectedDeleteCollateralRocOperation" value-ref="CollateralRocTrx.MakerEditRejectedDeleteCollateralRocOperation"/>
            <entry key="MakerCloseRejectedCreateCollateralRocOperation" value-ref="CollateralRocTrx.MakerCloseRejectedCreateCollateralRocOperation"/>
            <entry key="MakerCloseRejectedUpdateCollateralRocOperation" value-ref="CollateralRocTrx.MakerCloseRejectedUpdateCollateralRocOperation"/>
            <entry key="MakerCloseRejectedDeleteCollateralRocOperation" value-ref="CollateralRocTrx.MakerCloseRejectedDeleteCollateralRocOperation"/>
            <entry key="MakerCloseDraftCollateralRocOperation" value-ref="CollateralRocTrx.MakerCloseDraftCollateralRocOperation"/> 
            <entry key="MakerSaveUpdateCollateralRocOperation" value-ref="CollateralRocTrx.MakerSaveUpdateCollateralRocOperation"/> 
             
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

    <bean id="collateralRocTrxControllerFactory" class="com.integrosys.cms.app.collateralrocandinsurance.trx.CollateralRocTrxControllerFactory">
        <property name="collateralRocReadController" ref="collateralRocReadController"/>
        <property name="collateralRocTrxController" ref="collateralRocTrxController"/>
    </bean>
</beans>