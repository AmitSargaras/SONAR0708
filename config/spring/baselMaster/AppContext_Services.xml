<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
			
	<bean id="baselProxy" class="com.integrosys.cms.app.baselmaster.proxy.BaselProxyManagerImpl">
		<property name="baselBusManager" ref="baselBusManager" />
		<property name="stagingBaselBusManager" ref="stagingBaselBusManager" />
		<property name="trxControllerFactory" ref="baselTrxControllerFactory" />
		<property name="stagingBaselFileMapperIdBusManager" ref="stagingBaselFileMapperIdBusManager" />
		<property name="baselFileMapperIdBusManager" ref="baselFileMapperIdBusManager" />		
		
	</bean>
	<bean id="baselBusManager" class="com.integrosys.cms.app.baselmaster.bus.BaselBusManagerImpl">
		<property name="baselMasterDao" ref="baselMasterDao" />
		<property name="baselJdbc" ref="baselJdbc" /> 
	</bean>
	<bean id="stagingBaselBusManager" class="com.integrosys.cms.app.baselmaster.bus.BaselBusManagerStagingImpl">
		<property name="baselMasterDao" ref="baselMasterDao" />
		<property name="baselJdbc" ref="baselJdbc" /> 
	</bean>
	<bean id="stagingBaselFileMapperIdBusManager" class="com.integrosys.cms.app.baselmaster.bus.FileMapperIdBusManagerStagingImpl">
		<property name="baselMasterDao" ref="baselMasterDao" />
		<property name="baselJdbc" ref="baselJdbc" /> 
	</bean>
	<bean id="baselFileMapperIdBusManager" class="com.integrosys.cms.app.baselmaster.bus.FileMapperIdBusManagerImpl">
		<property name="baselMasterDao" ref="baselMasterDao" />
		<property name="baselJdbc" ref="baselJdbc" /> 
	</bean>
	
	
			
</beans>