    </bean>
    <bean id="caseBranchTrxControllerFactory" class="com.integrosys.cms.app.caseBranch.trx.CaseBranchTrxControllerFactory">
        <property name="caseBranchReadController" ref="caseBranchReadController"/>
        <property name="caseBranchTrxController" ref="caseBranchTrxController"/>
        <property name="caseBranchInsertFileTrxController" ref="caseBranchInsertFileTrxController"/>
        <property name="readCaseBranchInsertFileTrxController" ref="readCaseBranchInsertFileTrxController"/>
