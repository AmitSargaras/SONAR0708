        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerEditRejectedUpdateCaseBranchOperation" value-ref="CaseBranchTrx.MakerEditRejectedUpdateCaseBranchOperation"/>
            	   <entry key="MakerEditRejectedCreateCaseBranchOperation" value-ref="CaseBranchTrx.MakerEditRejectedCreateCaseBranchOperation"/>
            	  <entry key="MakerEditRejectedDeleteCaseBranchOperation" value-ref="CaseBranchTrx.MakerEditRejectedDeleteCaseBranchOperation"/>
                  <entry key="MakerUpdateCaseBranchOperation" value-ref="CaseBranchTrx.MakerUpdateCaseBranchOperation"/>
                  <entry key="MakerUpdateDraftCreateCaseBranchOperation" value-ref="CaseBranchTrx.MakerUpdateDraftCreateCaseBranchOperation"/>
                  <entry key="MakerCreateCaseBranchOperation" value-ref="CaseBranchTrx.MakerCreateCaseBranchOperation"/>
                   <entry key="MakerDeleteCaseBranchOperation" value-ref="CaseBranchTrx.MakerDeleteCaseBranchOperation"/>
                  <entry key="MakerCloseRejectedUpdateCaseBranchOperation" value-ref="CaseBranchTrx.MakerCloseRejectedUpdateCaseBranchOperation"/>
                  <entry key="MakerCloseDraftCaseBranchOperation" value-ref="CaseBranchTrx.MakerCloseDraftCaseBranchOperation"/>
                  <entry key="MakerCloseRejectedCreateCaseBranchOperation" value-ref="CaseBranchTrx.MakerCloseRejectedCreateCaseBranchOperation"/>
                  <entry key="MakerCloseRejectedDeleteCaseBranchOperation" value-ref="CaseBranchTrx.MakerCloseRejectedDeleteCaseBranchOperation"/>
                  <entry key="CheckerApproveUpdateCaseBranchOperation" value-ref="CaseBranchTrx.CheckerApproveUpdateCaseBranchOperation"/>
                  <entry key="CheckerApproveCreateCaseBranchOperation" value-ref="CaseBranchTrx.CheckerApproveCreateCaseBranchOperation"/>
                  <entry key="CheckerRejectCaseBranchOperation" value-ref="CaseBranchTrx.CheckerRejectCaseBranchOperation"/>
                  <entry key="CheckerCreateRejectCaseBranchOperation" value-ref="CaseBranchTrx.CheckerCreateRejectCaseBranchOperation"/>
                  <entry key="CheckerEditRejectCaseBranchOperation" value-ref="CaseBranchTrx.CheckerEditRejectCaseBranchOperation"/>
                  <entry key="CheckerDeleteRejectCaseBranchOperation" value-ref="CaseBranchTrx.CheckerDeleteRejectCaseBranchOperation"/>
                  <entry key="MakerSaveCaseBranchOperation" value-ref="CaseBranchTrx.MakerSaveCaseBranchOperation"/>
                  <entry key="MakerSaveUpdateCaseBranchOperation" value-ref="CaseBranchTrx.MakerSaveUpdateCaseBranchOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
