	</bean>
	
    <bean id="limitsOfAuthorityMasterReadController" class="com.integrosys.cms.app.limitsOfAuthorityMaster.trx.ReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadOperation" value-ref="LimitsOfAuthorityMasterTrx.ReadOperation"/>
                 <entry key="ReadIDOperation" value-ref="LimitsOfAuthorityMasterTrx.ReadIDOperation"/>  
            </map>
        </property>
    </bean> 
    <bean id="limitsOfAuthorityMasterTrxController" class="com.integrosys.cms.app.limitsOfAuthorityMaster.trx.TrxController">
      <property name="nameTrxOperationMap">
        <map>
            <entry key="MakerCreateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerCreateOperation"/>
            <entry key="CheckerApproveCreateOperation" value-ref="LimitsOfAuthorityMasterTrx.CheckerApproveCreateOperation"/>
           <entry key="CheckerApproveDeleteOperation" value-ref="LimitsOfAuthorityMasterTrx.CheckerApproveDeleteOperation"/>
           
           <entry key="MakerUpdateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerUpdateOperation"/>
           <entry key="CheckerApproveUpdateOperation" value-ref="LimitsOfAuthorityMasterTrx.CheckerApproveUpdateOperation"/>
           <entry key="CheckerRejectOperation" value-ref="LimitsOfAuthorityMasterTrx.CheckerRejectOperation"/>
           <entry key="MakerEditRejectedCreateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerEditRejectedCreateOperation"/>
           <entry key="MakerEditRejectedUpdateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerEditRejectedUpdateOperation"/>
           <entry key="MakerCloseRejectedCreateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerCloseRejectedCreateOperation"/>
           <entry key="MakerCloseRejectedUpdateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerCloseRejectedUpdateOperation"/>
           <entry key="MakerSaveUpdateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerSaveUpdateOperation"/>
           <entry key="MakerSaveOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerSaveOperation"/>
           <entry key="MakerUpdateDraftCreateOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerUpdateDraftCreateOperation"/>
  		   <entry key="MakerCloseDraftOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerCloseDraftOperation"/>
  		   <entry key="MakerDeleteOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerDeleteOperation"/>
           <entry key="MakerCloseRejectedDeleteOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerCloseRejectedDeleteOperation"/>
           <entry key="MakerEditRejectedDeleteOperation" value-ref="LimitsOfAuthorityMasterTrx.MakerEditRejectedDeleteOperation"/>
            
        </map>
      </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

    <bean id="limitsOfAuthorityMasterTrxControllerFactory" class="com.integrosys.cms.app.limitsOfAuthorityMaster.trx.TrxControllerFactory">
        <property name="readController" ref="limitsOfAuthorityMasterReadController"/>
        <property name="trxController" ref="limitsOfAuthorityMasterTrxController"/>
    </bean> 
</beans>