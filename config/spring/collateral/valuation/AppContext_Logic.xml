<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
  
  <bean id="cashValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.CashValuator" />
  
  <bean id="goldValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.GoldValuator">
  	<property name="goldFeedProfileSingleton" ref="goldFeedProfileSingleton" />
  </bean>
  
  <bean id="guaranteeValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.GuaranteeValuator" />
  
  <bean id="cgcValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.CGCValuator" >
      <property name="cgcValuationDAO" ref="cgcValuationJdbc" />
  </bean>

  <bean id="insuranceValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.InsuranceValuator" />
  
  <bean id="marketableValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.MarketableRunningTotalValuator" >
      <property name="marketableProfileSingleton" ref="mktFeedSingleton" />
  </bean>
  
  <bean id="pdcValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.PDCValuator" />
  
  <bean id="propertyValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.PropertyValuator">
  	<property name="propertyProfileSingleton" ref="propertyProfileSingleton" />
  </bean>

  <bean id="propertyIndexValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.PropertyIndexValuator">
  	  <constructor-arg value="true" />
  	  <constructor-arg value="true" />
      <property name="propertyProfileSingleton" ref="propertyProfileSingleton" />
  </bean>

  <bean id="straightLineValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.StrtLineValuator" >
      <property name="assetLifeProfileSingleton" ref="assetLifeProfileSingleton" />
  </bean>

  <bean id="vehicleValuator" class="com.integrosys.cms.app.collateral.bus.valuation.valuator.VehicleValuator">
  	<property name="vehicleFeedProfileSingleton" ref="vehicleFeedProfileSingleton" />
    <property name="assetLifeProfileSingleton" ref="assetLifeProfileSingleton" />
    <property name="commonCodeRegionSingleton" ref="commonCodeRegionSingleton" />
  </bean>
  
  <bean id="assetLifeProfileSingleton" class="com.integrosys.cms.app.collateral.bus.valuation.support.AssetLifeProfileSingleton" init-method="init" lazy-init="true">
      <property name="strtLineValuationDao" ref="straightLineValuationJdbc" />
  </bean>

  <bean id="commonCodeRegionSingleton" class="com.integrosys.cms.app.collateral.bus.valuation.support.CommonCodeRegionSingleton" init-method="init" lazy-init="true">
      <property name="vehicleValuationDao" ref="vehicleValuationJdbc" />
  </bean>

  <bean id="goldFeedProfileSingleton" class="com.integrosys.cms.app.collateral.bus.valuation.support.GoldFeedProfileSingleton" init-method="init" lazy-init="true">
  	<property name="goldValuationDao" ref="goldValuationJdbc" />
  </bean>
  
  <bean id="vehicleFeedProfileSingleton" class="com.integrosys.cms.app.collateral.bus.valuation.support.VehicleFeedProfileSingleton" init-method="init" lazy-init="true">
  	<property name="vehicleValuationDao" ref="vehicleValuationJdbc" />
  </bean>
  
  <bean id="stockPriceCapSingleton" class="com.integrosys.cms.app.collateral.bus.valuation.support.StockPriceCapSingleton" init-method="init" lazy-init="true">
      <property name="marketableValuationDao" ref="marketableValuationJdbc" />
  </bean>

  <bean id="mktFeedSingleton" class="com.integrosys.cms.app.collateral.bus.valuation.support.MktFeedSingleton" init-method="init" lazy-init="true">
      <property name="marketableValuationDao" ref="marketableValuationJdbc" />
  </bean>

