		<property name="checkListProxyManager" ref="checklistProxy" />
	</bean>
	<bean id="ImageTagAction.MakerPreapreResubmitImageTagCmd" class="com.integrosys.cms.ui.imageTag.MakerPreapreResubmitImageTagCmd">
		<property name="imageTagProxyManager" ref="imageTagProxy" />
		<property name="collateralProxy" ref="collateralProxy" />
		<property name="limitProxy" ref="limitProxy" />
		<property name="checkListProxyManager" ref="checklistProxy" />
	</bean>
	<bean id="ImageTagAction.GetImageFiltersSubFolderCommand" class="com.integrosys.cms.ui.imageTag.GetImageFiltersSubFolderCommand">
		<property name="imageTagProxyManager" ref="imageTagProxy" />
		<property name="imageUploadProxyManager" ref="imageUploadProxy" />
	</bean>
	<bean id="ImageTagAction.GetImageFiltersDocNameCommand" class="com.integrosys.cms.ui.imageTag.GetImageFiltersDocNameCommand">
		<property name="imageTagProxyManager" ref="imageTagProxy" />
		<property name="imageUploadProxyManager" ref="imageUploadProxy" />
	</bean>
	<bean id="ImageTagAction.GetImageFiltersSubFolderPrintCommand" class="com.integrosys.cms.ui.imageTag.GetImageFiltersSubFolderPrintCommand">
		<property name="imageTagProxyManager" ref="imageTagProxy" />
		<property name="imageUploadProxyManager" ref="imageUploadProxy" />
	</bean>
	<bean id="ImageTagAction.GetImageFiltersDocNamePrintCommand" class="com.integrosys.cms.ui.imageTag.GetImageFiltersDocNamePrintCommand">
		<property name="imageTagProxyManager" ref="imageTagProxy" />
		<property name="imageUploadProxyManager" ref="imageUploadProxy" />
	</bean>
	<!-- Added for DMS Integration -->
	<bean id="ImageTagAction.RetriveImageCommand" class="com.integrosys.cms.ui.imageTag.RetriveImageCommand">
		<property name="contentManagerFactory" ref="contentManagerFactory" />
	</bean>
		<bean id="ImageTagAction.RetriveImageViewCheckerCommand" class="com.integrosys.cms.ui.imageTag.RetriveImageViewCheckerCommand">
		<property name="contentManagerFactory" ref="contentManagerFactory" />
	</bean>
	<bean id="ImageTagAction.RetriveUntagImageCommand" class="com.integrosys.cms.ui.imageTag.RetriveUntagImageCommand">
		<property name="contentManagerFactory" ref="contentManagerFactory" />
	</bean>
	<bean id="contentManagerFactory" class="com.integrosys.cms.app.contentManager.factory.ContentManagerFactory" >
	</bean>
	<bean id="ImageTagAction.DownloadImageCommand" class="com.integrosys.cms.ui.imageTag.DownloadImageCommand">
		<property name="contentManagerFactory" ref="contentManagerFactory" />
	</bean>
	<bean id="ImageTagAction.RefreshImageStatusDateCmd"
		class="com.integrosys.cms.ui.imageTag.RefreshImageStatusDateCmd">
		<property name="imageTagProxyManager" ref="imageTagProxy" />
		<property name="checkListProxyManager" ref="checklistProxy" />
	</bean>
</beans>

