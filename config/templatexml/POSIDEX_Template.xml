<?xml version="1.0" encoding="UTF-8"?>
	<posidexTemplateOut>
			<query>
			select prof.lsp_short_name party_name,
			(addr.lra_addr_line_1||decode(addr.lra_addr_line_2,null,'',','||addr.lra_addr_line_2)||decode(addr.lra_addr_line_3,null,'',','||addr.lra_addr_line_3)) address,
			(select city_name from cms_city where id=addr.lra_city_text) city,
			addr.lra_post_code pincode,
			addr.lra_telephone_text teleno,
			prof.lsp_le_id partyid,
			prof.pan pan_no,
      		prof.total_sanctioned_limit,
			prof.total_funded_limit,
			prof.total_non_funded_limit,
			(CASE WHEN (TO_NUMBER(cam.cms_appr_officer_grade) BETWEEN 1 AND 7 ) THEN 'GOOD'
		      WHEN (TO_NUMBER(cam.cms_appr_officer_grade) BETWEEN 8 AND 10) THEN 'BAD'
		      END)
		      CUSTOMER_TYPE,
		    (CASE WHEN ( (TO_NUMBER(cam.cms_appr_officer_grade) BETWEEN 1 AND 10) AND (TO_NUMBER(cam.cms_appr_officer_grade) != 7)  ) THEN 'HDB'||' '||TO_NUMBER(cam.cms_appr_officer_grade)||' - RAM helpdesk'
		      WHEN (TO_NUMBER(cam.cms_appr_officer_grade) = 7 ) THEN 'HDB'||' '||TO_NUMBER(cam.cms_appr_officer_grade)||' - Refer to Wholesale Credit-RAM Helpdesk'
		      END) REMARKS
			from sci_le_sub_profile prof, sci_le_reg_addr addr,sci_lsp_lmt_profile cam
		</query>
	<whereClause>
		<mandatoryClause>
				1=1 
				and addr.lra_type_value='CORPORATE'
				and addr.cms_le_main_profile_id = prof.cms_le_main_profile_id
				and cam.cms_customer_id = prof.cms_le_sub_profile_id
				and prof.status='ACTIVE'
				and prof.lsp_sgmnt_code_value 
				in('SEG0000001',
				'SEG0000003',
				'SEG0000006',
				'SEG0000007',
