    <class name="com.integrosys.cms.app.creditriskparam.bus.exemptFacility.OBExemptFacility" auto-complete="false">
        <map-to xml="OBExemptFacility"/>
        <field name="facilityCode" type="java.lang.String">
            <bind-xml name="facilityCode" node="element"></bind-xml>
        </field>
        <field name="facilityStatusExempted" type="java.lang.String">
            <bind-xml name="facilityStatusExempted" node="element"></bind-xml>
        </field>
        <field name="facilityStatusConditionalPerc" type="java.lang.Double">
            <bind-xml name="facilityStatusConditionalPerc" node="element"></bind-xml>
        </field>
    </class>
</mapping>