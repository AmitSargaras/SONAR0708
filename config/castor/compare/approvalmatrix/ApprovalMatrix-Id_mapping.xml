<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Approval Matrix Entry</description>
    <class name="com.integrosys.cms.app.approvalmatrix.bus.OBApprovalMatrixEntry"
            auto-complete="false">
        <map-to xml="OBApprovalMatrixEntry"/>
        <field name="approvalMatrixEntryRef" type="long">
            <bind-xml name="approvalMatrixEntryRef" node="element"/>
        </field>
        
        <field name="riskGrade" type="int">
            <bind-xml name="riskGrade" node="element"></bind-xml>
        </field>
         <field name="level1" type="java.lang.String">
            <bind-xml name="level1" node="element"></bind-xml>
        </field>
        <field name="level2" type="java.lang.String">
            <bind-xml name="level2" node="element"></bind-xml>
        </field>
        <field name="level3" type="java.lang.String">
            <bind-xml name="level3" node="element"></bind-xml>
        </field>
        <field name="level4" type="java.lang.String">
            <bind-xml name="level4" node="element"></bind-xml>
        </field>
    </class>
</mapping>