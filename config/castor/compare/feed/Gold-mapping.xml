<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Gold Feed Entry</description>
    <class name="com.integrosys.cms.app.feed.bus.gold.OBGoldFeedEntry"
        auto-complete="false">
        <map-to xml="OBGoldFeedEntry"/>
        <field name="GoldFeedEntryRef" type="long">
            <bind-xml name="GoldFeedEntryRef" node="element"/>
        </field>
        <field name="GoldGradeNum" type="java.lang.String">
            <bind-xml name="GoldGradeNum" node="element"></bind-xml>
        </field>
        <field name="UnitMeasurementNum" type="java.lang.String">
            <bind-xml name="UnitMeasurementNum" node="element"></bind-xml>
        </field>
        <field name="CurrencyCode" type="java.lang.String">
            <bind-xml name="CurrencyCode" node="element"></bind-xml>
        </field>
        <field name="UnitPrice" type="java.math.BigDecimal">
            <bind-xml name="UnitPrice" node="element"></bind-xml>
        </field>
    </class>
</mapping>