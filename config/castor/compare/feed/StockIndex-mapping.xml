<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Stock Index Feed Entry</description>
    <class name="com.integrosys.cms.app.feed.bus.stockindex.OBStockIndexFeedEntry"
        auto-complete="false">
        <map-to xml="OBStockIndexFeedEntry"/>
        <field name="StockIndexFeedEntryRef" type="long">
            <bind-xml name="StockIndexFeedEntryRef" node="element"/>
        </field>
        <field name="UnitPrice" type="java.lang.Double">
            <bind-xml name="UnitPrice" node="element"></bind-xml>
        </field>
        <field name="Ric" type="java.lang.String">
            <bind-xml name="Ric" node="element"></bind-xml>
        </field>
        <field name="IsinCode" type="java.lang.String">
            <bind-xml name="IsinCode" node="element"></bind-xml>
        </field>
        <field name="Name" type="java.lang.String">
            <bind-xml name="Name" node="element"></bind-xml>
        </field>
        <!--<field name="LastUpdatedDate" type="date">-->
        <!--    <bind-xml name="LastUpdatedDate" node="element"></bind-xml>-->
        <!--</field>-->
    </class>
</mapping>