<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<description>Common Security Types Mapping</description>

	<class name="com.integrosys.cms.host.eai.security.bus.property.PropertySecurity">
		<map-to xml="Property"/>
		<field name="State" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="State"/>
		</field>
		<field name="District" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="District"/>
		</field>
		<field name="Mukim" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="Mukim"/>
		</field>
		<field name="TitleType" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="TitleType"/>
		</field>
		<field name="TitleNumber" type="string">
			<bind-xml name="TitleNumber" node="element"/>
		</field>
		<field name="TitleNumberPrefix" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="TitleNumberPrefix"/>
		</field>
	</class>

	<class name="com.integrosys.cms.host.eai.security.sharedsecurity.bus.VehicleDetailSearchItem">
		<map-to xml="AssetVehicle"/>
		<field name="VehicleType" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="VehicleType"/>
		</field>
		<field name="RegistrationNo" type="string">
			<bind-xml name="RegistrationNo" node="element"/>
		</field>
		<field name="ChassisNumber" type="string">
			<bind-xml name="ChassisNumber" node="element"/>
		</field>
		<field name="EngineNumber" type="string">
			<bind-xml name="EngineNumber" node="element"/>
		</field>
	</class>
	
	<class name="com.integrosys.cms.host.eai.security.sharedsecurity.bus.AirCraftDetailSearchItem">
		<map-to xml="AssetAircraft"/>
		<field name="AircraftType" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="AircraftType"/>
		</field>
		<field name="ManufactureYear" type="long">
			<bind-xml name="ManufactureYear" node="element"/>
		</field>
		<field name="Model" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="Model"/>
		</field>
	</class>
	
	<class name="com.integrosys.cms.host.eai.security.sharedsecurity.bus.VesselDetailSearchItem">
		<map-to xml="AssetVessel"/>
		<field name="VesselType" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="VesselType"/>
		</field>
		<field name="BuiltYear" type="long">
			<bind-xml name="BuiltYear" node="element"/>
		</field>
		<field name="RegisteredCountry" type="string">
			<bind-xml name="PortRegistration" node="element"/>
		</field>
	</class>
	
	<class name="com.integrosys.cms.host.eai.security.sharedsecurity.bus.PlantEquipDetailSearchItem">
		<map-to xml="AssetPlantEquip"/>
		<field name="PlantEquipType" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="PlantEquipType"/>
		</field>
		<field name="SerialNo" type="string">
			<bind-xml name="SerialNo" node="element"/>
		</field>
		<field name="ManufactureYear" type="long">
			<bind-xml name="ManufactureYear" node="element"/>
		</field>
		<field name="Model" type="com.integrosys.cms.host.eai.StandardCode">
			<bind-xml name="Model"/>
		</field>
	</class>

</mapping>