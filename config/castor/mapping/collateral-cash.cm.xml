<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<description>Collateral Mapping - CO002 - Cash</description>
	<class name="com.integrosys.cms.host.eai.security.bus.cash.CashSecurity">
		<map-to xml="CashDetail" />
		<field name="LOSSecurityId" type="string">
			<bind-xml name="LOSSecurityId" />
		</field>
		<field name="InterestCapital" type="string">
			<bind-xml name="InterestCapital" />
		</field>
		<field name="SecurityIssuer" type="string">
			<bind-xml name="SecurityIssuer" />
		</field>
		<field name="CCRefNo" type="string">
			<bind-xml name="CCRefNo" />
		</field>
		<field name="PrivateCaveatGteeExpDate" type="string">
			<bind-xml name="PrivateCaveatGteeExpDate" />
		</field>
		<field name="FDDesc" type="string">
			<bind-xml name="FDDesc" />
		</field>
		<field name="ChangeIndicator" type="string">
			<bind-xml name="ChangeIndicator" />
		</field>
		<field name="UpdateStatusIndicator" type="string">
			<bind-xml name="UpdateStatusIndicator" />
		</field>
	</class>

	<class name="com.integrosys.cms.host.eai.security.bus.cash.CashDeposit">
		<map-to xml="DepositDetail" />
		<field name="SecurityId" type="string">
			<bind-xml name="LOSSecurityId" />
		</field>
		<field name="cashDepositId" type="long">
			<bind-xml name="CMSDepositId" node="element" />
		</field>
		<field name="ReferenceNo" type="string">
			<bind-xml name="ReferenceNo" />
		</field>
		<field name="AmountCurrency" type="string">
			<bind-xml name="AmountCurrency" />
		</field>
		<field name="Amount" type="double">
			<bind-xml name="Amount" node="element" />
		</field>
		<field name="MaturityDate" type="string">
			<bind-xml name="MaturityDate" />
		</field>
		<field name="OwnOtherBankInd" type="string">
			<bind-xml name="OwnOtherBankInd" />
		</field>
		<field name="GroupAcctNo" type="string">
			<bind-xml name="GroupAcctNo" />
		</field>
		<field name="Tenure" type="long">
			<bind-xml name="Tenure" node="element" />
		</field>
		<field name="TenurePeriodType" type="string">
			<bind-xml name="TenurePeriodType" />
		</field>
		<field name="HoldCode" type="string">
			<bind-xml name="HoldCode" node="element" />
		</field>
		<field name="ChangeIndicator" type="string">
			<bind-xml name="ChangeIndicator" />
		</field>
		<field name="UpdateStatusIndicator" type="string">
			<bind-xml name="UpdateStatusIndicator" />
		</field>
	</class>
</mapping>