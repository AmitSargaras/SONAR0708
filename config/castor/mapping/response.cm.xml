<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<description>Response Mapping - Acknowledgement</description>

	<include href="common-response.cm.xml" />

	<class extends="com.integrosys.cms.host.eai.EAIBody" name="com.integrosys.cms.host.eai.response.ResponseMessageBody" >
		<map-to xml="MsgBody"/>
		<field name="Response" type="com.integrosys.cms.host.eai.response.Response">
            <bind-xml name="Response" />
		</field>
	</class>

    <class name="com.integrosys.cms.host.eai.response.Response">
		<map-to xml="Response"/>
		<field name="ResponseCode" type="string">
            <bind-xml name="ResponseCode" node="element" />
		</field>
		<field name="ResponseMessage" type="string">
            <bind-xml name="ResponseMessage" node="element" />
		</field>
	</class>

</mapping>