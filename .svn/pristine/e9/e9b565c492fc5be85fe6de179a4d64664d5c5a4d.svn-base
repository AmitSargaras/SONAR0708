<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.fileUpload.bus">
	<class  entity-name="actualfileUpload" name="OBFileUpload" table="CMS_FILE_UPLOAD" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_FILEUPLOAD_SEQ</param>
			</generator>
		</id>		
			<property name="fileName" column="FILENAME" />
			<property name="uploadBy" column="UPLOADBY" />
			<property name="approveBy" column="APPROVEBY" />
			<property name="uploadTime" column="UPLOADTIME" />
			<property name="totalRecords" column="TOTALRECORDS" />
			<property name="approveRecords" column="APPROVERECORDS" />
			<property name="fileType" column="FILETYPE" />
			
	</class>
	
	
	<class  entity-name="stagefileUpload" name="OBFileUpload" table="STAGE_FILE_UPLOAD" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_FILEUPLOAD_SEQ</param>
			</generator>
		</id>
			<property name="fileName" column="FILENAME" />
			<property name="uploadBy" column="UPLOADBY" />
			<property name="approveBy" column="APPROVEBY" />
			<property name="uploadTime" column="UPLOADTIME" />
			<property name="totalRecords" column="TOTALRECORDS" />
			<property name="approveRecords" column="APPROVERECORDS" />
			<property name="fileType" column="FILETYPE" />
			
	</class>
	

</hibernate-mapping>
