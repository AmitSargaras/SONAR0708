        <property name="stagingRecurrentFileMapperIdBusManager" ref="stagingRecurrentFileMapperIdBusManager"/>
	</bean>
	<bean id="RecurrentTrx.FileInsertReadOperation"
		class="com.integrosys.cms.app.Recurrent.trx.FileInsertReadOperation">
		<property name="RecurrentFileMapperIdBusManager" ref="RecurrentFileMapperIdBusManager"/>
        <property name="stagingRecurrentFileMapperIdBusManager" ref="stagingRecurrentFileMapperIdBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerCloseRejectedInsertMasterOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerCloseRejectedInsertMasterOperation">
		 <property name="RecurrentFileMapperIdBusManager" ref="RecurrentFileMapperIdBusManager"/>
        <property name="stagingRecurrentFileMapperIdBusManager" ref="stagingRecurrentFileMapperIdBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerCreateRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerCreateRecurrentOperation">
		<property name="RecurrentFileMapperIdBusManager" ref="RecurrentFileMapperIdBusManager"/>
        <property name="stagingRecurrentFileMapperIdBusManager" ref="stagingRecurrentFileMapperIdBusManager"/>
	</bean>
    
    <bean id="RecurrentInsertFileTrxController" class="com.integrosys.cms.app.Recurrent.trx.InsertFileTrxController">
        <property name="nameTrxOperationMap">
            <map>        	 
                  <entry key="MakerRecurrentInsertFileOperation" value-ref="RecurrentTrx.MakerRecurrentInsertFileOperation" />
                  <entry key="CheckerApproveInsertMasterOperation" value-ref="RecurrentTrx.CheckerApproveInsertMasterOperation" />
                  <entry key="CheckerRejectInsertMasterOperation" value-ref="RecurrentTrx.CheckerRejectInsertMasterOperation" />
                  <entry key="MakerCloseRejectedInsertMasterOperation" value-ref="RecurrentTrx.MakerCloseRejectedInsertMasterOperation"/>
                   <entry key="CheckerCreateRecurrentOperation" value-ref="RecurrentTrx.CheckerCreateRecurrentOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
    
    <bean id="readRecurrentInsertFileTrxController" class="com.integrosys.cms.app.Recurrent.trx.ReadInsertFileTrxController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="FileInsertReadOperation" value-ref="RecurrentTrx.FileInsertReadOperation"/>                   
            </map>
        </property>
    </bean>
    <bean id="RecurrentTrxControllerFactory" class="com.integrosys.cms.app.Recurrent.trx.RecurrentTrxControllerFactory">
        <property name="RecurrentReadController" ref="RecurrentReadController"/>
        <property name="RecurrentTrxController" ref="RecurrentTrxController"/>
        <property name="RecurrentInsertFileTrxController" ref="RecurrentInsertFileTrxController"/>
        <property name="readRecurrentInsertFileTrxController" ref="readRecurrentInsertFileTrxController"/>
    </bean>
</beans>