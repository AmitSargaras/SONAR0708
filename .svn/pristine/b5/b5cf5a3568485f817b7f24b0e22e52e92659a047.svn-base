        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerDeleteRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerDeleteRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerEditRejectedUpdateRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerEditRejectedUpdateRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerEditRejectedCreateRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerEditRejectedCreateRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerEditRejectedDeleteRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerEditRejectedDeleteRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerCloseRejectedUpdateRecurrentOperation"
