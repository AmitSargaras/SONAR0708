		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerCloseDraftRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerCloseDraftRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerCloseRejectedDeleteRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerCloseRejectedDeleteRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.MakerCloseRejectedCreateRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerCloseRejectedCreateRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerApproveUpdateRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerApproveUpdateRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerApproveCreateRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerApproveCreateRecurrentOperation">
