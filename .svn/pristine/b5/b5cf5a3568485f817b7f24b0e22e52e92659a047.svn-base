        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerRejectRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerRejectRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerCreateRejectRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerCreateRejectRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerEditRejectRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerEditRejectRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerDeleteRejectRecurrentOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerDeleteRejectRecurrentOperation">
		 <property name="RecurrentBusManager" ref="RecurrentBusManager"/>
        <property name="stagingRecurrentBusManager" ref="stagingRecurrentBusManager"/>
	</bean>

    <bean id="RecurrentReadController" class="com.integrosys.cms.app.Recurrent.trx.RecurrentReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadRecurrentOperation" value-ref="RecurrentTrx.ReadRecurrentOperation"/>
                 <entry key="ReadRecurrentIDOperation" value-ref="RecurrentTrx.ReadRecurrentIDOperation"/>
                
                
            </map>
        </property>
    </bean>

    <bean id="RecurrentTrxController" class="com.integrosys.cms.app.Recurrent.trx.RecurrentTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerEditRejectedUpdateRecurrentOperation" value-ref="RecurrentTrx.MakerEditRejectedUpdateRecurrentOperation"/>
            	   <entry key="MakerEditRejectedCreateRecurrentOperation" value-ref="RecurrentTrx.MakerEditRejectedCreateRecurrentOperation"/>
            	  <entry key="MakerEditRejectedDeleteRecurrentOperation" value-ref="RecurrentTrx.MakerEditRejectedDeleteRecurrentOperation"/>
                  <entry key="MakerUpdateRecurrentOperation" value-ref="RecurrentTrx.MakerUpdateRecurrentOperation"/>
                  <entry key="MakerUpdateDraftCreateRecurrentOperation" value-ref="RecurrentTrx.MakerUpdateDraftCreateRecurrentOperation"/>
                  <entry key="MakerCreateRecurrentOperation" value-ref="RecurrentTrx.MakerCreateRecurrentOperation"/>
                   <entry key="MakerDeleteRecurrentOperation" value-ref="RecurrentTrx.MakerDeleteRecurrentOperation"/>
                  <entry key="MakerCloseRejectedUpdateRecurrentOperation" value-ref="RecurrentTrx.MakerCloseRejectedUpdateRecurrentOperation"/>
                  <entry key="MakerCloseDraftRecurrentOperation" value-ref="RecurrentTrx.MakerCloseDraftRecurrentOperation"/>
                  <entry key="MakerCloseRejectedCreateRecurrentOperation" value-ref="RecurrentTrx.MakerCloseRejectedCreateRecurrentOperation"/>
                  <entry key="MakerCloseRejectedDeleteRecurrentOperation" value-ref="RecurrentTrx.MakerCloseRejectedDeleteRecurrentOperation"/>
                  <entry key="CheckerApproveUpdateRecurrentOperation" value-ref="RecurrentTrx.CheckerApproveUpdateRecurrentOperation"/>
                  <entry key="CheckerApproveCreateRecurrentOperation" value-ref="RecurrentTrx.CheckerApproveCreateRecurrentOperation"/>
                  <entry key="CheckerRejectRecurrentOperation" value-ref="RecurrentTrx.CheckerRejectRecurrentOperation"/>
                  <entry key="CheckerCreateRejectRecurrentOperation" value-ref="RecurrentTrx.CheckerCreateRejectRecurrentOperation"/>
                  <entry key="CheckerEditRejectRecurrentOperation" value-ref="RecurrentTrx.CheckerEditRejectRecurrentOperation"/>
                  <entry key="CheckerDeleteRejectRecurrentOperation" value-ref="RecurrentTrx.CheckerDeleteRejectRecurrentOperation"/>
                  <entry key="MakerSaveRecurrentOperation" value-ref="RecurrentTrx.MakerSaveRecurrentOperation"/>
                  <entry key="MakerSaveUpdateRecurrentOperation" value-ref="RecurrentTrx.MakerSaveUpdateRecurrentOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
	<bean id="RecurrentTrx.MakerRecurrentInsertFileOperation"
		class="com.integrosys.cms.app.Recurrent.trx.MakerRecurrentInsertFileOperation">
		 <property name="RecurrentFileMapperIdBusManager" ref="RecurrentFileMapperIdBusManager"/>
        <property name="stagingRecurrentFileMapperIdBusManager" ref="stagingRecurrentFileMapperIdBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerApproveInsertMasterOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerApproveInsertMasterOperation">
		 <property name="RecurrentFileMapperIdBusManager" ref="RecurrentFileMapperIdBusManager"/>
        <property name="stagingRecurrentFileMapperIdBusManager" ref="stagingRecurrentFileMapperIdBusManager"/>
	</bean>
	<bean id="RecurrentTrx.CheckerRejectInsertMasterOperation"
		class="com.integrosys.cms.app.Recurrent.trx.CheckerRejectInsertMasterOperation">
