                       <entry key="MakerEditRejectedDeleteValuationAgencyOperation" value-ref="MakerEditRejectedDeleteValuationAgencyOperation"/>
                       <entry key="MakerEditRejectedUpdateValuationAgencyOperation" value-ref="MakerEditRejectedUpdateValuationAgencyOperation"/>
                       <entry key="MakerEditRejectedCreateValuationAgencyOperation" value-ref="MakerEditRejectedCreateValuationAgencyOperation"/>
                       <entry key="MakerUpdateValuationAgencyOperation" value-ref="MakerUpdateValuationAgencyOperation"/>
                       <entry key="CheckerApproveUpdateValuationAgencyOperation" value-ref="CheckerApproveUpdateValuationAgencyOperation"/>
                       <entry key="MakerDisableValuationAgencyOperation" value-ref="MakerDisableValuationAgencyOperation"/>
                       <entry key="MakerEnableValuationAgencyOperation" value-ref="MakerEnableValuationAgencyOperation"/>
               		   <entry key="CheckerDisableRejectValuationAgencyOperation" value-ref="CheckerDisableRejectValuationAgencyOperation"/>
