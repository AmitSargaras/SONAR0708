                       <entry key="MakerSaveUpdateValuationAgencyOperation" value-ref="MakerSaveUpdateValuationAgencyOperation"/>
                       <entry key="MakerSaveValuationAgencyOperation" value-ref="MakerSaveValuationAgencyOperation"/>
                       <entry key="MakerCloseDraftValuationAgencyOperation" value-ref="MakerCloseDraftValuationAgencyOperation"/>
                       <entry key="MakerUpdateDraftCreateValuationAgencyOperation" value-ref="MakerUpdateDraftCreateValuationAgencyOperation"/>
      
               
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
