<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Facility Relationship</description>
    <class name="com.integrosys.cms.app.limit.bus.OBFacilityRelationship"
        auto-complete="false">
        <map-to xml="OBFacilityRelationship"/>
        <field name="AccountRelationshipEntryCode" type="java.lang.String">
            <bind-xml name="AccountRelationshipEntryCode" node="element"/>
        </field>
        <field name="ShareHolderPercentage" type="java.lang.Double">
            <bind-xml name="ShareHolderPercentage" node="element"></bind-xml>
        </field>
        <field name="GuaranteePercentage" type="java.lang.Double">
            <bind-xml name="GuaranteePercentage" node="element"></bind-xml>
        </field>
        <field name="GuaranteeAmount" type="com.integrosys.base.businfra.currency.Amount">
            <bind-xml name="GuaranteeAmount" node="element"></bind-xml>
        </field>
        <field name="ReceiveMailCode" type="java.lang.Boolean">
            <bind-xml name="ReceiveMailCode" node="element"></bind-xml>
        </field>
        <field name="HoldMailCodeEntryCode" type="java.lang.String">
            <bind-xml name="HoldMailCodeEntryCode" node="element"></bind-xml>
        </field>
        <field name="NameAssociateWithFacilityOrder" type="java.lang.Integer">
            <bind-xml name="NameAssociateWithFacilityOrder" node="element"></bind-xml>
        </field>
        <field name="NameConjunctionPosition" type="java.lang.Character">
            <bind-xml name="NameConjunctionPosition" node="element"></bind-xml>
        </field>
        <field name="NameConjunction" type="java.lang.String">
            <bind-xml name="NameConjunction" node="element"></bind-xml>
        </field>
        <field name="ProfitRatio" type="java.lang.Double">
            <bind-xml name="ProfitRatio" node="element"></bind-xml>
        </field>
        <field name="DividendRatio" type="java.lang.Double">
            <bind-xml name="DividendRatio" node="element"></bind-xml>
        </field>
    </class>
</mapping>