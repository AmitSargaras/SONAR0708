<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="DocumentLocationForm" type="com.integrosys.cms.ui.documentlocation.DocumentLocationForm">
        </form-bean>    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/DocumentLocation" type="com.integrosys.cms.ui.documentlocation.DocumentLocationAction"
            name="DocumentLocationForm" validate="false" scope="request">
            <forward name="after_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/doc_loc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_list_WF" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/doc_loc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_prepare_update" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/update_doc_loc.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_prepare_update_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/update_doc_loc.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="update_not_required" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/update_not_required.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="chk_view" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/chk_view.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_prepare_close" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/view_doc_location.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="to_track_maker" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/view_doc_location.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="to_track_checker" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/view_doc_location.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_submit" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_submit_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="after_close" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="approve" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="reject" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="wip" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="wip_checklist" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/wip_checklist.html"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>
            <forward name="view" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/documentlocation/view_doc_location.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.doc.Location"/>
            </forward>            
        </action>
    
    </action-mappings>
    
    <!--<message-resources parameter="ApplicationResources-checklist-doclocation"/>-->
</struts-config>