<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="com.integrosys.cms.batch.reports">
	<class name="OBReportRequest" table="CMS_REPORT_REQUEST">
		<id name="reportRequestID" column="REPORT_REQUEST_ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">REPORT_REQUEST_SEQ</param>
			</generator>
		</id>

		<property name="userID" column="USER_ID"/>
		<property name="reportID" column="REPORT_ID"/>
		<property name="reportName" column="REPORT_NAME"/>
		<property name="parameters" column="PARAMETERS"/>
		<property name="requestTime" column="REQUEST_TIME" type="timestamp"/>
		<property name="reportGenTime" column="REPORT_GEN_TIME" type="timestamp"/>
		<property name="status" column="STATUS"/>
	</class>
</hibernate-mapping>
