######################
# MQ Settings
######################

mq.queue.manager=QM_CLM
mq.host.ip=172.16.106.128
mq.port=1414
mq.listener.concurrent.consumers=1

# obsolete config for MQ classes prior to Spring adoption
mq.server.isremote=true
mq.ccsid=819

# channel config is only required for client conn, must be authenticated by mq user (MCA)
mq.in.channel.name=QCLM_CLNT
mq.out.channel.name=

# default queues for CLIMS EAI (XML)
mq.in.queue=QCLM
mq.out.queue=QSOA

# queues for ARBS host (MBASE)
mq.arbs.in.queue=QARBS.IN
mq.arbs.out.queue=QARBS.OUT

# queues for TSPR host (XML)
mq.tspr.in.queue=QTSPR.IN
mq.tspr.out.queue=QTSPR.OUT

# queues for Trade host (XML)
mq.trade.in.queue=trade.in
mq.trade.out.queue=trade.out
