<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="eventMonitorDaoJdbcProxyCreator" class="org.springframework.aop.framework.autoproxy.BeanNameAutoProxyCreator">
		<property name="interceptorNames">
			<list>
				<idref bean="matchAllTxInterceptor" />
			</list>
		</property>
		<property name="beanNames">
			<list>
				<idref bean="collateralEvaluationDueDao" />
				<idref bean="assetPostDatedChequeCollateralMaturityDao" />
				<idref bean="cashDepositCollateralMaturityDao" />
				<idref bean="marketableSecCollateralMaturityDao" />
				<idref bean="nonMarketableSecCollateralMaturityDao" />
				<idref bean="marketIndexDao" />
				<idref bean="coverageExcessDao" />
				<idref bean="expiredDocumentDao" />
			</list>
		</property>
	</bean>

</beans>