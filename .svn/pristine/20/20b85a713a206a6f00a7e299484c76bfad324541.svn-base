<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.collateralNewMaster.bus">
	<class  entity-name="actualCollateralNewMaster" name="OBCollateralNewMaster" table="CMS_COLLATERAL_NEW_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_COL_NEW_MASTER_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

	
		<property name="newCollateralCode" column="NEW_COLLATERAL_CODE" />
		<property name="newCollateralMainType" column="NEW_COLLATERAL_MAIN_TYPE" />
		<property name="newCollateralSubType" column="NEW_COLLATERAL_SUB_TYPE" />
		<property name="newCollateralDescription" column="NEW_COLLATERAL_DESCRIPTION" />
		<property name="insurance" column="INSURANCE" />
		<property name="revaluationFrequencyCount" column="REVALUATION_FREQUENCY_COUNT" />
		<property name="revaluationFrequencyDays" column="REVALUATION_FREQUENCY_DAYS" />
	
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATE_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
		
		<property name="newCollateralCategory" column="NEW_COLLATERAL_CATEGORY"></property>
		<property name="isApplicableForCersaiInd" column="CERSAI_IND"></property>
		
	</class>
	<class  entity-name="stageCollateralNewMaster" name="OBCollateralNewMaster" table="CMS_STG_COLLATERAL_NEW_MASTER" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_COL_NEW_MASTER_SEQ</param>
			</generator>
		</id>
	
		 	
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="newCollateralCode" column="NEW_COLLATERAL_CODE" />
		<property name="newCollateralMainType" column="NEW_COLLATERAL_MAIN_TYPE" />
		<property name="newCollateralSubType" column="NEW_COLLATERAL_SUB_TYPE" />
		<property name="newCollateralDescription" column="NEW_COLLATERAL_DESCRIPTION" />
		<property name="insurance" column="INSURANCE" />
		<property name="revaluationFrequencyCount" column="REVALUATION_FREQUENCY_COUNT" />
		<property name="revaluationFrequencyDays" column="REVALUATION_FREQUENCY_DAYS" />
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATE_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
		
		<property name="newCollateralCategory" column="NEW_COLLATERAL_CATEGORY"></property>
		<property name="isApplicableForCersaiInd" column="CERSAI_IND"></property>
	</class>
</hibernate-mapping>
