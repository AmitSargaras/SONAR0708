<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.npaTraqCodeMaster.bus">
	<class entity-name="npaDataLog" name="OBNpaDataLog" table="CMS_NPADATA_LOG"
		lazy="false">
		<id name="id" column="ID" type="long">
			<generator
				class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_NPADATA_LOG_SEQ</param>
			</generator>
		</id>
		<property name="reportingDate" column="REPORTING_DATE" />
		<property name="system" column="SYSTEM" />
		<property name="partyID" column="PARTY_ID" />
		<property name="collateralType" column="COLLATERAL_TYPE" />
		<property name="valuationDate" column="VALUATION_DATE" />
		<property name="valuationAmount" column="VALUATION_AMOUNT" />
		<property name="originalValue" column="ORIGINAL_VALUE" />
		<property name="startDate" column="START_DATE" />
		<property name="maturityDate" column="MATURITY_DATE" />
		<property name="fileName" column="FILE_NAME" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>
