<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

    <description>
        To be used for transaction controller and transaction operation.
        Transaction operation will have the property to set bus manager, which get from 'AppContext_Services.xml'
    </description>
 
	<bean id="LeiDateValidationTrx.ReadLeiDateValidationOperation" class="com.integrosys.cms.app.leiDateValidation.trx.ReadLeiDateValidationOperation">
        <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
    </bean>
    
    <bean id="LeiDateValidationTrx.ReadLeiDateValidationIDOperation" class="com.integrosys.cms.app.leiDateValidation.trx.ReadLeiDateValidationIDOperation">
        <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
    </bean>
    
     <bean id="LeiDateValidationTrx.MakerCreateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerCreateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.CheckerApproveCreateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.CheckerApproveCreateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	
	 <bean id="LeiDateValidationTrx.MakerUpdateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerUpdateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.CheckerApproveUpdateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.CheckerApproveUpdateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.CheckerRejectLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.CheckerRejectLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerEditRejectedUpdateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerEditRejectedUpdateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerEditRejectedCreateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerEditRejectedCreateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerCloseRejectedCreateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerCloseRejectedCreateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerCloseRejectedUpdateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerCloseRejectedUpdateLeiDateValidationOperation">
		 <property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerSaveUpdateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerSaveUpdateLeiDateValidationOperation">
		<property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerSaveLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerSaveLeiDateValidationOperation">
		<property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerUpdateDraftCreateLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerUpdateDraftCreateLeiDateValidationOperation">
		<property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	<bean id="LeiDateValidationTrx.MakerCloseDraftLeiDateValidationOperation"
		class="com.integrosys.cms.app.leiDateValidation.trx.MakerCloseDraftLeiDateValidationOperation">
		<property name="leiDateValidationBusManager" ref="leiDateValidationBusManager"/>
        <property name="stagingLeiDateValidationBusManager" ref="stagingLeiDateValidationBusManager"/>
	</bean>
	
	
    <bean id="leiDateValidationReadController" class="com.integrosys.cms.app.leiDateValidation.trx.LeiDateValidationReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadLeiDateValidationOperation" value-ref="LeiDateValidationTrx.ReadLeiDateValidationOperation"/>
                 <entry key="ReadLeiDateValidationIDOperation" value-ref="LeiDateValidationTrx.ReadLeiDateValidationIDOperation"/>  
            </map>
        </property>
    </bean> 
    <bean id="leiDateValidationTrxController" class="com.integrosys.cms.app.leiDateValidation.trx.LeiDateValidationTrxController">
         <property name="nameTrxOperationMap">
        <map>
            <entry key="MakerCreateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerCreateLeiDateValidationOperation"/>
            <entry key="CheckerApproveCreateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.CheckerApproveCreateLeiDateValidationOperation"/>
                       <entry key="MakerUpdateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerUpdateLeiDateValidationOperation"/>
            <entry key="CheckerApproveUpdateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.CheckerApproveUpdateLeiDateValidationOperation"/>
            <entry key="CheckerRejectLeiDateValidationOperation" value-ref="LeiDateValidationTrx.CheckerRejectLeiDateValidationOperation"/>
            <entry key="MakerEditRejectedCreateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerEditRejectedCreateLeiDateValidationOperation"/>
            <entry key="MakerEditRejectedUpdateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerEditRejectedUpdateLeiDateValidationOperation"/>
            <entry key="MakerCloseRejectedCreateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerCloseRejectedCreateLeiDateValidationOperation"/>
            <entry key="MakerCloseRejectedUpdateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerCloseRejectedUpdateLeiDateValidationOperation"/>
            <entry key="MakerSaveUpdateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerSaveUpdateLeiDateValidationOperation"/>
            <entry key="MakerSaveLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerSaveLeiDateValidationOperation"/>
            <entry key="MakerUpdateDraftCreateLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerUpdateDraftCreateLeiDateValidationOperation"/>
   			<entry key="MakerCloseDraftLeiDateValidationOperation" value-ref="LeiDateValidationTrx.MakerCloseDraftLeiDateValidationOperation"/>
   			
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
	
    <bean id="leiDateValidationTrxControllerFactory" class="com.integrosys.cms.app.leiDateValidation.trx.LeiDateValidationTrxControllerFactory">
        <property name="leiDateValidationReadController" ref="leiDateValidationReadController"/>
        <property name="leiDateValidationTrxController" ref="leiDateValidationTrxController"/>
    </bean> 
</beans>