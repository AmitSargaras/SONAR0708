	<delimiter>|||</delimiter>
	<secondaryDelimiter>~~~</secondaryDelimiter>
	<dataStartIndex>1</dataStartIndex>
	<fileFormat>dat</fileFormat>
	<isFooter>1</isFooter>
	<mappedColumns>	
		<column>
			<dataType>ALPHABETIC_WITH_SPACIAL</dataType>
			<name>Master Name</name>
			<length>100</length>
			<mappedFieldName>masterName</mappedFieldName>
		</column>		
		<column>		
			<dataType>DATFILEXTENTION</dataType>
			<name>File Name</name>			
			<length>100</length>
			<mappedFieldName>fileName</mappedFieldName>
		</column>			
		<column>
			<dataType>NUMERIC</dataType>
			<name>Record Count</name>
			<length>5</length>
			<mappedFieldName>recordCount</mappedFieldName>
		</column>
	</mappedColumns>
	<defaultColumns>
		<column>
			<name>rowIndex</name>
			<defaultValue>1</defaultValue>
			<mappedFieldName>rowIndex</mappedFieldName>
		</column>
	</defaultColumns>
</syncMasterTemplateIn>