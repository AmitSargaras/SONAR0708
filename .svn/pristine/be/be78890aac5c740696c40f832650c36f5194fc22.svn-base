<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBCCCertificateProxyManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBCCCertificateProxyManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>SBCCCertificateBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBCCCertificateBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBStagingCCCertificateBusManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBStagingCCCertificateBusManagerHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBCCCertificate</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor><reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>
        <jndi-name>EBCCCertificateHome</jndi-name>
        <local-jndi-name>EBCCCertificateLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBCCCertificateItem</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor><reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>        
        <jndi-name>EBCCCertificateItemHome</jndi-name>
        <local-jndi-name>EBCCCertificateItemLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBStagingCCCertificate</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor><reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>
        <jndi-name>EBStagingCCCertificateHome</jndi-name>
        <local-jndi-name>EBStagingCCCertificateLocalHome</local-jndi-name>
    </weblogic-enterprise-bean> 
    
    <weblogic-enterprise-bean>
        <ejb-name>EBStagingCCCertificateItem</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor><reference-descriptor>          <resource-description>              <res-ref-name>jdbc/CMSPool</res-ref-name>              <jndi-name>CMSPool</jndi-name>          </resource-description>      </reference-descriptor>        
        <jndi-name>EBStagingCCCertificateItemHome</jndi-name>
        <local-jndi-name>EBStagingCCCertificateItemLocalHome</local-jndi-name>
    </weblogic-enterprise-bean>    
</weblogic-ejb-jar>