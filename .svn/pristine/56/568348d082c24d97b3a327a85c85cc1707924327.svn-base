<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.creditApproval.bus">
	<class  entity-name="actualCreditApproval" name="OBCreditApproval" table="CMS_CREDIT_APPROVAL" lazy="false">

		<id name="id" column="ID" type="long">
				<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_CREDIT_APPROVAL_SEQ</param>
			</generator>
		</id>
			<property name="versionTime" column="VERSION_TIME" type="long"></property>
			<property name="approvalCode" column="APPROVAL_CODE" />
			<property name="approvalName" column="APPROVAL_NAME" />
			<property name="maximumLimit" column="MAXIMUM_LIMIT" />
			<property name="minimumLimit" column="MINIMUM_LIMIT" />
			<property name="segmentId" column="SEGMENT" />
			<property name="email" column="EMAIL" />
			<property name="senior" column="SENIOR" />
			<property name="risk" column="RISK" />
			<property name="deferralPowers" column="DEFERRAL_POWERS" />
			<property name="waivingPowers" column="WAIVING_POWERS" />
			<property name="regionId" column="REGION_ID" type="long"/>
			<property name="createBy" column="CREATE_BY" />
			<property name="creationDate" column="CREATION_DATE" />
			<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
			<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
			<property name="deprecated" column="DEPRECATED" />
			<property name="status" column="STATUS" />
			<property name="cpsId" column="CPS_ID"/>
	        <property name="employeeId" column="EMPLOYEE_ID" />
		
	</class>
	
		<class  entity-name="stageCreditApproval" name="OBCreditApproval" table="CMS_STAGE_CREDIT_APPROVAL" lazy="false">

		<id name="id" column="ID" type="long">
				<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_CREDIT_APPROVAL_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			<property name="approvalCode" column="APPROVAL_CODE" />
			<property name="approvalName" column="APPROVAL_NAME" />
			<property name="maximumLimit" column="MAXIMUM_LIMIT" />
			<property name="minimumLimit" column="MINIMUM_LIMIT" />
			<property name="segmentId" column="SEGMENT" />
			<property name="email" column="EMAIL" />
			<property name="senior" column="SENIOR" />
			<property name="risk" column="RISK" />
			<property name="deferralPowers" column="DEFERRAL_POWERS" />
			<property name="waivingPowers" column="WAIVING_POWERS" />
			<property name="regionId" column="REGION_ID" type="long"/>
			<property name="createBy" column="CREATE_BY" />
			<property name="creationDate" column="CREATION_DATE" />
			<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
			<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
			<property name="deprecated" column="DEPRECATED" />
			<property name="status" column="STATUS" />
			<property name="cpsId" column="CPS_ID" />
			  <property name="employeeId" column="EMPLOYEE_ID" />

		
	</class>
	
</hibernate-mapping>
