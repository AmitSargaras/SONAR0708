<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBCommonCodeEntryManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBCommonCodeEntryManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    
    <weblogic-enterprise-bean>
        <ejb-name>SBCommonCodeEntriesProxyManager</ejb-name>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>SBCommonCodeEntriesProxyManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBCommonCodeEntry</ejb-name>
        <entity-descriptor>
			<entity-cache>
				<max-beans-in-cache>2000</max-beans-in-cache>
			</entity-cache>           
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>EBCommonCodeEntryHome</jndi-name>
    </weblogic-enterprise-bean>
    
    
    <weblogic-enterprise-bean>
        <ejb-name>EBCommonCodeEntryStage</ejb-name>
        <entity-descriptor>
			<entity-cache>
				<max-beans-in-cache>2000</max-beans-in-cache>
			</entity-cache>           
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
            <resource-description>
                <res-ref-name>jdbc/CMSPool</res-ref-name>
                <jndi-name>CMSPool</jndi-name>
            </resource-description>
        </reference-descriptor>
        <jndi-name>EBCommonCodeEntryStageHome</jndi-name>
    </weblogic-enterprise-bean>
    
    
</weblogic-ejb-jar>