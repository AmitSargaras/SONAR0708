<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="actualBankingMethodCust" 
		name="com.integrosys.cms.app.customer.bus.OBBankingMethod" table="CMS_BANKING_METHOD_CUST" lazy="false">
            <id name="bankId"  type="long" column="CMS_BANKING_METHOD_ID">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_BANKING_METHOD_SEQ</param>
			   </generator>
            </id>
            <!-- <property name="customerId" column="CUSTOMER_ID" /> -->
    		<property name="LEID" column="CMS_LE_SUB_PROFILE_ID"  type="long"/>
			<property name="bankType" column="CMS_BANKING_METHOD_NAME"/>
			<property name="customerIDForBankingMethod" column="CUSTOMER_ID"/>
			<property name="status" column="STATUS"/>
    </class>    
    
    
    <class entity-name="stagingBankingMethodCust" 
		name="com.integrosys.cms.app.customer.bus.OBBankingMethod" table="CMS_STAGE_BANKING_METHOD_CUST" lazy="false">
            <id name="bankId"   type="long" column="CMS_BANKING_METHOD_ID">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CHK_STG_BANKING_METHOD_SEQ</param>
			    </generator>
            </id>
            <!-- <property name="customerId" column="CUSTOMER_ID" /> -->
    		<property name="LEID" column="CMS_LE_SUB_PROFILE_ID"  type="long"/>
			<property name="bankType" column="CMS_BANKING_METHOD_NAME"/>
			<property name="customerIDForBankingMethod" column="CUSTOMER_ID"/>
			<property name="status" column="STATUS"/>
    </class>
</hibernate-mapping>