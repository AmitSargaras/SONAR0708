<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="ExcludedFacilityForm" type="com.integrosys.cms.ui.excludedfacility.ExcludedFacilityForm">
        </form-bean>
    </form-beans>
    
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/excludedfacility" type="com.integrosys.cms.ui.excludedfacility.ExcludedFacilityAction" name="ExcludedFacilityForm" validate="false" scope="request">
  
            <forward name="list_excluded_facility_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_list_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
            <forward name="maker_prepare_create_excluded_facility_category" path="/template/common_template.jsp">
			<set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
			<set-property property="rightFrame" value="/excludedFacility/maker_create_excluded_facility.jsp"/>
			</forward>
            
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
             <forward name="checker_excluded_facility_category_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/checker_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
             <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
             <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
            <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_resubmit_delete_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
            <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_read_save_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
             <forward name="maker_add_edit_excluded_facility_category_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_add_edit_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
             <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_read_delete_excluded_facility.jsp"/>
               <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
               <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
            <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_close_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            <forward name="checker_list_excluded_facility_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/checker_list_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
             <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/excludedFacility/maker_view_excluded_facility.jsp"/>
                <set-property property="title" value="menu.manual_feeds.excluded_facility_category.name"/>
            </forward>
            
        </action>
        </action-mappings>
</struts-config>
