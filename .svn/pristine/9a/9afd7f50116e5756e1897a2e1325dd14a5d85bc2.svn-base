<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.function.bus">
	<class entity-name="actualTeamFunctionGrp" name="OBTeamFunctionGrp"
		table="CMS_TEAM_FUNCTION_GRP" lazy="false">
		<id name="teamFunctionGrpID" type="long" column="ID">
			<generator
				class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">TEAM_FUNCTION_GRP_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long" />
		<property name="teamId" column="TEAM_ID" type="long" />
		<property name="teamTypeId" column="TEAM_TYPE_ID" type="long"/>
		<property name="functionGrpId" column="FUNCTION_GRP_ID" type="long"/>
		<property name="groupId" column="GROUP_ID" type="long" />
		<property name="status" column="STATUS" />
	</class>
	
	<class entity-name="stagingTeamFunctionGrp" name="OBTeamFunctionGrp"
		table="STAGE_TEAM_FUNCTION_GRP" lazy="false">
		<id name="teamFunctionGrpID" type="long" column="ID">
			<generator
				class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">TEAM_FUNCTION_GRP_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long" />
		<property name="teamId" column="TEAM_ID" type="long" />
		<property name="teamTypeId" column="TEAM_TYPE_ID" type="long"/>
		<property name="functionGrpId" column="FUNCTION_GRP_ID" type="long"/>
		<property name="groupId" column="GROUP_ID" type="long" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>