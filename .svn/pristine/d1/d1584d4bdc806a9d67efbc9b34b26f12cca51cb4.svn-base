<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="LiquidationForm" type="com.integrosys.cms.ui.liquidation.LiquidationForm">
        </form-bean>
    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
	    <action path="/Liquidation" type="com.integrosys.cms.ui.liquidation.LiquidationAction" name="LiquidationForm" validate="false" scope="request">
			<forward name="list" path="/template/common_template.jsp">
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			    <set-property property="rightFrame" value="/liquidation/maker_list_liquidation.jsp"/>
			</forward>
			<forward name="view_npl_info" path="/template/common_template.jsp">
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			    <set-property property="rightFrame" value="/liquidation/view_npl_details.jsp"/>
			</forward>
			<forward name="after_return" path="/template/common_template.jsp">
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			    <set-property property="rightFrame" value="/liquidation/maker_list_liquidation.jsp"/>
			</forward>
			<forward name="after_prepare" path="/template/common_template.jsp">
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			    <set-property property="rightFrame" value="/liquidation/maker_list_liquidation.jsp"/>
			</forward>
			<forward name="view_recovery_expense" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/view_recovery_expense.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="view_recovery_income" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/view_recovery_income.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="view_recovery" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/view_recovery.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			
			<forward name="prepare_add_recovery_expense" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/maker_add_edit_expense.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="prepare_add_recovery_income" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/maker_add_edit_income.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="prepare_add_recovery" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/maker_add_edit_recovery.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			
			<forward name="maker_edit_liquidation_page" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/maker_list_liquidation.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="maker_edit_liquidation_confirm" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="checker_edit_liquidation" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/checker_edit_liquidation.jsp"/>
			    <set-property property="roleDesc" value="role.sac.checker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="common_approve_page" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
			    <set-property property="roleDesc" value="role.sac.checker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="common_reject_page" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
			    <set-property property="roleDesc" value="role.sac.checker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="wip" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/common/wip.html"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="maker_close_liquidation" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/view_liquidation.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="view" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/view_liquidation.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="after_to_track" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/liquidation/view_liquidation.jsp"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
			<forward name="common_close_page" path="/template/common_template.jsp">
			    <set-property property="rightFrame" value="/common/ack_close.jsp"/>
			    <set-property property="roleDesc" value="role.sac.maker"/>
			    <set-property property="title" value="title.mainLiquidation.edit"/>
			</forward>
	    </action>    
    </action-mappings>
    
    <message-resources key="message-liquidation" parameter="ApplicationResources-liquidation"/>
</struts-config>