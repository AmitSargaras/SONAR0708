<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.caseCreationUpdate.bus">
	<class  entity-name="actualCaseCreationUpdate" name="OBCaseCreation" table="cms_caseCreationUpdate" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CASECREATION_UPDATE_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
		
		<property name="description" column="DESCRIPTION" />
		<property name="branchCode" column="BRANCHCODE" />
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATE_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="limitProfileId" column="LIMIT_PROFILE_ID" />
		<property name="currRemarks" column="CURR_REMARKS" />
		<property name="prevRemarks" column="PREV_REMARKS" />
		
	</class>
	<class  entity-name="stageCaseCreationUpdate" name="OBCaseCreation" table="cms_stage_caseCreationUpdate" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_CASE_UPDATE_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
		
		<property name="description" column="DESCRIPTION" />
		<property name="branchCode" column="BRANCHCODE" />
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATE_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="limitProfileId" column="LIMIT_PROFILE_ID" />
		<property name="currRemarks" column="CURR_REMARKS" />
		<property name="prevRemarks" column="PREV_REMARKS" />
		
	</class>
</hibernate-mapping>
