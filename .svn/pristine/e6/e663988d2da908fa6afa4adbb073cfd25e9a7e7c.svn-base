
drop table CMS_UDF;

CREATE TABLE CMS_UDF 
(
  ID NUMBER NOT NULL 
, VERSION_TIME NUMBER 
, MODULEID NUMBER NOT NULL 
, MODULENAME VARCHAR2(50 BYTE) NOT NULL 
, FIELDNAME VARCHAR2(50 BYTE) NOT NULL 
, FIELDTYPEID NUMBER NOT NULL 
, FIELDTYPE VARCHAR2(50 BYTE) NOT NULL 
, OPTIONS VARCHAR2(4000 BYTE) 
, SEQUENCE NUMBER 
, STATUS VARCHAR2(10 BYTE) DEFAULT 'Created' NOT NULL 
, CONSTRAINT CMS_UDF_MASTER_PK PRIMARY KEY 
  (
    ID 
  )
  ENABLE 
) ;


