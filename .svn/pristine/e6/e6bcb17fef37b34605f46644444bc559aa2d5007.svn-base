<hibernate-mapping>
  <class name="com.integrosys.cms.host.eai.security.bus.insurance.InsuranceSecurity" table="CMS_INSURANCE" polymorphism="explicit">
    <id name="CMSSecurityId" column="CMS_COLLATERAL_ID" type="long" />

    <property name="insuranceDescription" column="DESCRIPTION" />
    <component name="insuranceName" class="com.integrosys.cms.host.eai.StandardCode">
    	<!-- <property name="standardCodeNumber" column="?" /> -->
    	<property name="standardCodeValue" column="INSURER_NAME" />
    </component>
    <component name="insuranceType" class="com.integrosys.cms.host.eai.StandardCode">
    	<!-- <property name="standardCodeNumber" column="?" /> -->
    	<property name="standardCodeValue" column="INSURANCE_TYPE" />
    </component>

    <property name="insuredAmount" column="INSURED_AMOUNT" type="double" />
    <property name="insuredAmountCurrency" column="INSURED_AMT_CURR" />
    <property name="JDOEffectiveDate" column="EFFECTIVE_DATE" type="timestamp" />
    <property name="policyNo" column="POLICY_NO" />
    <property name="JDOExpiryDate" column="EXPIRY_DATE" type="timestamp" />
    <property name="externalLegalCounsel" column="EXT_LEGAL_COUNSEL" />
    <property name="accelerationClause" column="ACCELERATION_CLAUSE" />
    <property name="localCurrencyInCM" column="LOCAL_CCY_IN_CM" />
    
    <component name="coreMarket" class="com.integrosys.cms.host.eai.StandardCode">
    	<!-- <property name="standardCodeNumber" column="?" /> -->
    	<property name="standardCodeValue" column="CORE_MARKET" />
    </component>
    
    <component name="bankCustArrangeIns" class="com.integrosys.cms.host.eai.StandardCode">
    	<!-- <property name="standardCodeNumber" column="?" /> -->
    	<property name="standardCodeValue" column="ARR_INSURER" />
    </component>

    <property name="JDOISDADate" column="ISDA_DATE" type="timestamp" />
    <property name="JDOTreasuryDate" column="TREASURY_DATE" type="timestamp" />
    <property name="bankInterestNoted" column="BANK_INT_NOTED" />
    <property name="insurancePremium" column="INSURANCE_PREMIUM" type="double" />
    <property name="insurancePremiumCurrency" column="INSURANCE_PREMIUM_CURRENCY" />
    <property name="issuanceDate" column="ISSUANCE_DATE" type="timestamp" />
  </class>

