<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="concReportNewForm"
            type="com.integrosys.cms.ui.report.ConcReportNewForm"/>
        <!-- system report -->
        <form-bean name="SystemReportSearchForm"
            type="com.integrosys.cms.ui.report.SystemReportSearchForm">
        </form-bean>


        <!-- MISReports ActionForm. -->
        <form-bean name="MISReportSearchForm"
            type="com.integrosys.cms.ui.report.MISReportSearchForm">
        </form-bean>

        <!-- ConcentrationReports ActionForm. -->
        <form-bean name="ConcentrationReportSearchForm"
            type="com.integrosys.cms.ui.report.ConcentrationReportSearchForm">
        </form-bean>  
        
        <form-bean name="CCIReportSearchForm"
            type="com.integrosys.cms.ui.report.CCIReportSearchForm">
        </form-bean>         
        
        <!-- Maintain Concentration Reports parameter -->
        <form-bean name="MaintainConcReportForm" type="com.integrosys.cms.ui.concentrationreport.MaintainConcReportForm">
        </form-bean>
         
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/conc_report_new"
            type="com.integrosys.cms.ui.report.ConcReportNewAction"
            name="concReportNewForm"
            validate="false" scope="request">

            <forward name="new" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/new_conc_report.jsp"/>
                <set-property property="title" value="title.conc.risk.profile"/>
            </forward>
            <forward name="new_lms" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/lms_new_conc_report.jsp"/>
                <set-property property="roleDesc" value="lam.maker"/>
                <set-property property="title" value="title.concreport.edit"/>
            </forward>
            <forward name="generateResult" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit_report_generation.jsp"/>
                <set-property property="title" value="title.conc.risk.profile"/>
            </forward>
        </action>
        
        <!-- system reports -->
        <action path="/SystemReportSearch" type="com.integrosys.cms.ui.report.SystemReportSearchAction"
            name="SystemReportSearchForm" validate="true" scope="request">
            <forward name="prepare_search" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/search_system_reports.jsp"/>
                <set-property property="title" value="title.systemadminchecker.sysreport"/>
            </forward>
            <forward name="search_reports" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/list_system_reports.jsp"/>
                <set-property property="title" value="title.systemadminchecker.sysreport"/>
            </forward>
            <forward name="get_report" path="/reports/display_report.jsp">
            </forward>
        </action>

        <!-- MISSearchReports Action. -->
        <action path="/MISReportSearch" type="com.integrosys.cms.ui.report.MISReportSearchAction"
            name="MISReportSearchForm" validate="true" scope="request">
            <forward name="prepare_search" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/search_mis_reports.jsp"/>
                <set-property property="title" value="title.misreport"/>
            </forward>
            <forward name="search_reports" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/list_mis_reports.jsp"/>
                <set-property property="title" value="title.misreport"/>
            </forward>
            <forward name="get_report" path="/reports/display_report.jsp">
            </forward>
            
            <forward name="generate_monthly_basel_report" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselReport/downloadReports.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="download_monthly_basel_report" path="/baselReport/popup.jsp" />
            
            <forward name="generate_daily_basel_open_report" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselReport/downloadReports.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="download_daily_basel_open_report" path="/baselReport/popup.jsp" />
            
            <forward name="generate_daily_basel_delete_report" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselReport/downloadReports.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="download_daily_basel_delete_report" path="/baselReport/popup.jsp" />
        </action>
          
        <!-- ConcentrationSearchReports Action. -->
        <action path="/ConcentrationReportSearch" type="com.integrosys.cms.ui.report.ConcentrationReportSearchAction"
            name="ConcentrationReportSearchForm" validate="true" scope="request">
            <forward name="prepare_search" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/search_concentration_reports.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="search_reports" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/list_concentration_reports.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="get_report" path="/reports/display_report.jsp">
            </forward>
        </action>
        
        <action path="/CCIReportSearch" type="com.integrosys.cms.ui.report.CCIReportSearchAction"
            name="CCIReportSearchForm" validate="true" scope="request">
            <forward name="prepare_search" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/search_cci_reports.jsp"/>
                <set-property property="title" value="title.report.ccireport"/>
            </forward>
            <forward name="search_reports" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/reports/list_cci_reports.jsp"/>
                <set-property property="title" value="title.report.ccireport"/>
            </forward>
            <forward name="get_report" path="/reports/display_report.jsp">
            </forward>
        </action>
          
        <action path="/MaintainConcReport"
            type="com.integrosys.cms.ui.concentrationreport.MaintainConcReportAction"
            name="MaintainConcReportForm" scope="request">
            <forward name="maker_add_edit_concreport_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/concentrationreport/concreport_maker_edit.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="concreport_maker_edit_successful" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="checker_concreport_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/concentrationreport/concreport_checker.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="maker_close_concreport_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/concentrationreport/concreport_checker.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/concentrationreport/concreport_maker_view.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="checker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/concentrationreport/concreport_maker_view.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>
            <forward name="after_to_track" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/concentrationreport/concreport_checker.jsp"/>
                <set-property property="title" value="title.concreport"/>
            </forward>

        </action>
        
    </action-mappings>
    
    <message-resources key="message-reports" parameter="ApplicationResources-reports"/>
</struts-config>