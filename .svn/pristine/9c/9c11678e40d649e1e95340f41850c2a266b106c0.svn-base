<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="NewTatForm" type="com.integrosys.cms.ui.newTat.NewTatForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/NewTat" type="com.integrosys.cms.ui.newTat.NewTatAction" name="NewTatForm" validate="false" scope="request">
            <forward name="list" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/list.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="list_ok" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/list.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="list_cancel" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/list.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
              <forward name="maker_list_tat" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/list.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="pagination" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/list.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
              <forward name="show_audit_trail" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/audit_trail_details.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="show_case_rm" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/deferral_approve_new_tat_view.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="show_case_branch" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_branch_view.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="show_case_cpu" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_cpu_view.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
              <forward name="refresh_facility_Name" path="/newTat/facilityNameList.jsp"/>
            
            <forward name="initiate_new_tat" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/customer_search_new_tat.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="list_customer" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/customer_search_new_tat.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="submit_documents" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/list.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="confirm_submit_documents" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
               <forward name="close_case" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="work_in_process" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/work_in_process.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="party_closed" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/party_closed.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            
             <forward name="deferral_approve" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="deferral_clearance" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="prepare_submit_documents" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/maintain_new_tat.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
              <forward name="confirm_submit_documents_error" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/maintain_new_tat.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="prepare_deferral_approve" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/deferral_approve_new_tat.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="prepare_deferral_clearance" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/deferral_approve_new_tat.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="prepare_document_receive" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_branch.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="prepare_document_scan" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_branch.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="confirm_document_receive" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="confirm_document_scan" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            
            <forward name="prepare_limit_release" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_cpu.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="prepare_deferral_raised" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_cpu.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="prepare_clims_updated" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_cpu.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="confirm_limit_release" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="confirm_deferral_raised" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
            <forward name="confirm_clims_updated" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/confirmation.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
             <forward name="refresh_facility_details" path="/newTat/facdetaillist.jsp"/>
             <forward name="confirm_limit_release_error" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTat/update_new_tat_cpu.jsp"/>
                <set-property property="title" value="maintain.newTat"/>
            </forward>
        </action>
		
    </action-mappings>
    
    <message-resources key="message-newTat" parameter="ApplicationResources.properties"/>
</struts-config>