        <property name="geographyDAO" ref="geographyDAO"/>
        <property name="geographyJdbc" ref="geographyJdbc"/>
    </bean>
    
    --><!--    Country Beans Start	-->
       
   	<bean id="countryProxy" class="com.integrosys.cms.ui.geography.country.CountryProxyManagerImpl">
