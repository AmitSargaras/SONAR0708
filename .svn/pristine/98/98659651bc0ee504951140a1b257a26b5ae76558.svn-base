<binding name="sibs_cif_address_req">
    <include path="sibs_header_req.xml" />
    <mapping name="Definition" class="com.integrosys.simulator.host.SIBS.CIFAddress.OBSIBSCustomerAddressInquiryRes" extends="com.integrosys.simulator.host.SIBS.OBMessageHeader"> 
        <structure name="HeaderDefinition">
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
	       <collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.CIFAddress.OBSIBSCustomerAddressInquiryRes">
		        	<value name="FixColumn" field="customerNumber"/>
		        	<value name="FixColumn" field="addressSequence"/>
		        	<value name="FixColumn" field="addressType"/>
			        <value name="FixColumn" field="addressLine1"/>
			        <value name="FixColumn" field="addressLine2"/>
			        <value name="FixColumn" field="addressLine3"/>
			        <value name="FixColumn" field="addressLine4"/>
			        <value name="FixColumn" field="addressLine5"/>
			        <value name="FixColumn" field="foreignAddress"/>
			        <value name="FixColumn" field="insufficientAddress"/>
			        <value name="FixColumn" field="blockHouseBuildingNumber"/>
			        <value name="FixColumn" field="storeyNumber"/>
			        <value name="FixColumn" field="unitNumber"/>
			        <value name="FixColumn" field="poBoxNumber"/>
			        <value name="FixColumn" field="buildingPostOfficeName"/>
			        <value name="FixColumn" field="streetName"/>
			        <value name="FixColumn" field="city"/>
			        <value name="FixColumn" field="state"/>
			        <value name="FixColumn" field="postalCode"/>
			        <value name="FixColumn" field="country"/>
			        <value name="FixColumn" field="liveThereSinceDate" format="dateFmt"/>
			        <value name="FixColumn" field="addressRemark"/>
					<value name="FixColumn" field="strLastMaintainDate"/>
			        <value name="FixColumn" field="strTime"/>
			     </structure>
		     </collection>
	     </structure>
    </mapping> 	
</binding>