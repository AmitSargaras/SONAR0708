<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.productMaster.bus">
	<class  entity-name="actualProductMaster" name="OBProductMaster" table="CMS_PRODUCT_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_PRODUCT_MASTER_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>

		<property name="productCode" column="PRODUCT_CODE" />
		<property name="productName" column="PRODUCT_NAME" />
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" update= "false"/>
	</class>
	<class  entity-name="stageProductMaster" name="OBProductMaster" table="CMS_STAGE_PRODUCT_MASTER" lazy="false">
			<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_PRODUCT_MASTER_SEQ</param>
			</generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long"></version>
			
		<property name="productCode" column="PRODUCT_CODE" />
		<property name="productName" column="PRODUCT_NAME" />
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
	</class>
</hibernate-mapping>
