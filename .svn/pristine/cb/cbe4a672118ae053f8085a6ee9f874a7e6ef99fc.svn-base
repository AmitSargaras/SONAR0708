<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.integrosys.cms.batch.bhavcopy">
  <class name="OBBhavcopy" table="CMS_BHAVCOPY" lazy="false" >
    <id name="scCode" column="SC_CODE" type="long"/>
    <property name="scName"           column="SC_NAME" />
    <property name="scGroup"    column="SC_GROUP" />
    <property name="openValue"   column="OPEN_VALUE" type="double"/>
    <property name="closeValue"    column="CLOSE_VALUE" type="double"/>
    <property name="lastValue"     column="LAST_VALUE" type="double"/>
  </class>

</hibernate-mapping>
