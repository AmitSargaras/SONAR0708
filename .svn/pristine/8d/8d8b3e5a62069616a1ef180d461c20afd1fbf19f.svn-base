<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.systemBank.bus">
	<class  entity-name="actualOBSystemBank" name="OBSystemBank" table="CMS_SYSTEM_BANK" lazy="false">
		<id name="id" column="ID" type="long">
			 <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_SYSTEM_BANK_SEQ</param>
			 </generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="systemBankCode" column="system_bank_code" />
		<property name="systemBankName" column="system_bank_name" />
		<property name="contactNumber" column="contact_number" />
		<property name="faxNumber" column="fax_number" />
		<property name="contactMail" column="contact_mail" />
		<property name="address" column="address" />
		<property name="revisedContactMail" column="revised_contact_mail" />
<!--	<property name="cityTown" column="city_town" />-->
<!--	<property name="state" column="state" />-->
<!--	<property name="region" column="region" />-->
<!--	<property name="country" column="country" />-->
 <many-to-one name="country" column="COUNTRY" entity-name="actualCountry" lazy="false" />
		<many-to-one name="region" column="REGION" entity-name="actualRegion" lazy="false" />
		<many-to-one name="state" column="STATE" entity-name="actualState" lazy="false" />
		<many-to-one name="cityTown" column="CITY_TOWN" entity-name="actualCity" lazy="false" />
		
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		<property name="masterId" column="MASTER_ID" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATE_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
	</class>
	
	<class  entity-name="stageOBSystemBank" name="OBSystemBank" table="CMS_STAGE_SYSTEM_BANK" lazy="false">
		<id name="id" column="ID" type="long">
			 <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_SYSTEM_BANK_SEQ</param>
			 </generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="systemBankCode" column="system_bank_code" />
		<property name="systemBankName" column="system_bank_name" />
		<property name="contactNumber" column="contact_number" />
		<property name="contactMail" column="contact_mail" />
		<property name="address" column="address" />
		<property name="revisedContactMail" column="revised_contact_mail" />
<!--	<property name="cityTown" column="city_town" />-->
<!--	<property name="state" column="state" />-->
<!--	<property name="region" column="region" />-->
<!--	<property name="country" column="country" />-->
<many-to-one name="country" column="COUNTRY" entity-name="actualCountry" lazy="false" />
		<many-to-one name="region" column="REGION" entity-name="actualRegion" lazy="false" />
		<many-to-one name="state" column="STATE" entity-name="actualState" lazy="false" />
		<many-to-one name="cityTown" column="CITY_TOWN" entity-name="actualCity" lazy="false" />
		<property name="faxNumber" column="fax_number" />
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		<property name="masterId" column="MASTER_ID" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createBy" column="CREATE_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
		<property name="cpsId"     		column="CPS_ID" />
	</class>
</hibernate-mapping>
