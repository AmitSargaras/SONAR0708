<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.batch.feeds.property">
  <class name="OBPropertyFile" table="SI_TEMP_OTHR_PF003" lazy="false" >
    <id name="compositeID"              column="COMPOSITE_ID" type="string" />
    <property name="cifID"              column="CIF_ID" type="string" />
    <property name="sourceSecurityId"   column="SOURCE_SECURITY_ID" type="string" />
    <property name="valuerName"         column="VALUER_NAME" type="string" />
    <!--<property name="valuationDate"      column="VALUATION_DATE" type="timestamp" />-->
    <!--<property name="reValuationDate"    column="RE_VALUATION_DATE" type="timestamp" />-->
    <property name="valuerCode"         column="VALUER_CODE" type="string" />
    <property name="omv"                column="OMV" type="double" />
    <property name="fsv"                column="FSV" type="double" />
    <property name="reservedPrice"      column="RESERVED_PRICE" type="double" />
    <property name="valuationCurrency"  column="VALUATION_CURRENCY" type="string" />
    <property name="collateralID"       column="CMS_COLLATERAL_ID" type="long"/>
  </class>

</hibernate-mapping>



