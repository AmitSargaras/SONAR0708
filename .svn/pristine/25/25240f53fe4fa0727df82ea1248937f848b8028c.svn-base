<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar
  PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd">
<weblogic-rdbms-jar>
   <weblogic-rdbms-bean>
      <ejb-name>EBCollateralMetaData</ejb-name>
      <data-source-name>CMSPool</data-source-name>
      <table-map>
         <table-name>SECURITY_FIELD</table-name>
         <field-map>
            <cmp-field>fieldName</cmp-field>
            <dbms-column>FIELD_NAME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>securitySubTypeId</cmp-field>
            <dbms-column>SECURITY_SUB_TYPE_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>isUpdatable</cmp-field>
            <dbms-column>IS_SSC_UPDATABLE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>applicableCountry</cmp-field>
            <dbms-column>APPLICABLE_COUNTRY</dbms-column>
         </field-map>
      </table-map>
   </weblogic-rdbms-bean>
</weblogic-rdbms-jar>