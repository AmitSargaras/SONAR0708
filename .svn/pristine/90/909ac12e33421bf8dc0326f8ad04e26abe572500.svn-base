<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
			
	<bean id="ecbfCustomerInterfaceLogDAO" class="com.integrosys.cms.app.ecbf.counterparty.ECBFCustomerInterfaceLogDAOImpl">
		<property name="sessionFactory" ref="ofaSessionFactory" />
	</bean>
	
	<bean id="ecbfCustomerInterfaceLogJDBC" class="com.integrosys.cms.app.ecbf.counterparty.ECBFCustomerInterfaceLogJDBCImpl">
		<property name="dataSource" ref="ofaPool" />
	</bean>
	
	<bean id="ecbfLimitInterfaceLogDAO" class="com.integrosys.cms.app.ecbf.limit.ECBFLimitInterfaceLogDAOImpl">
		<property name="sessionFactory" ref="ofaSessionFactory" />
	</bean>
			
</beans>