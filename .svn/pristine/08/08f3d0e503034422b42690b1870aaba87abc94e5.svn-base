        <structure name="HeaderDefinition">
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
	        <collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.accountVerification.OBSIBSAccountVerification">
			        <value name="FixColumn" field="accountNumber"/>
