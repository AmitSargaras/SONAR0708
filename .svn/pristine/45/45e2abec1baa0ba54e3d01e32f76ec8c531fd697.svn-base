<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
			
			
		<bean id="productMasterBusManager" class="com.integrosys.cms.app.productMaster.bus.ProductMasterBusManagerImpl">
		<property name="productMasterDao" ref="productMasterDao" />
		<property name="productMasterJdbc" ref="productMasterJdbc" />
		</bean>

		<bean id="productMasterProxy" class="com.integrosys.cms.app.productMaster.proxy.ProductMasterProxyManagerImpl">
		<property name="productMasterBusManager" ref="productMasterBusManager" />
		<property name="stagingProductMasterBusManager" ref="stagingProductMasterBusManager" />
		<property name="trxControllerFactory" ref="productMasterTrxControllerFactory" />
		</bean>
		
		<bean id="stagingProductMasterBusManager" class="com.integrosys.cms.app.productMaster.bus.ProductMasterBusManagerStagingImpl">
		<property name="productMasterDao" ref="productMasterDao" />
		<property name="productMasterJdbc" ref="productMasterJdbc" />
		</bean>	
</beans>