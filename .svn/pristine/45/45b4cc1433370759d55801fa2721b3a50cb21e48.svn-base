<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Facility Multi-Tier Financing</description>
    <class name="com.integrosys.cms.app.limit.bus.OBFacilityMultiTierFinancing"
        auto-complete="false">
        <map-to xml="OBFacilityMultiTierFinancing"/>
        <field name="TierTerm" type="java.lang.Short">
            <bind-xml name="TierTerm" node="element"/>
        </field>
        <field name="TierTermCode" type="java.lang.Character">
            <bind-xml name="TierTermCode" node="element"></bind-xml>
        </field>
        <field name="Rate" type="java.lang.Double">
            <bind-xml name="Rate" node="element"></bind-xml>
        </field>
    </class>
</mapping>