<binding name="sibs_collateral_charge_req">
    <include path="sibs_header_req.xml" />
    <mapping name="Definition" class="com.integrosys.simulator.host.SIBS.collateralcharge.OBSIBSCollateralCharge" extends="com.integrosys.simulator.host.SIBS.OBMessageHeader"> 
        <structure name="HeaderDefinition">
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
				<collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.collateralcharge.OBSIBSCollateralCharge">
					<value name="FixColumn" field="applicationNo" />
					<value name="FixColumn" field="facilityCode" />
					<value name="FixColumn" field="accountSeq" />
					<value name="FixColumn" field="collateralID" />
					<value name="FixColumn" field="chargeType" />
					<value name="FixColumn" field="chargeAmt" />
					<value name="FixColumn" field="chargePresentationNo" />
					<value name="FixColumn" field="chargePresentationDate" format="dateFmt"/>
					<value name="FixColumn" field="chargeRegisteredDate" format="dateFmt"/>
					<value name="FixColumn" field="folio" />
					<value name="FixColumn" field="jilid" />
					<value name="FixColumn" field="partyChargeInd" />
			     </structure>
		     </collection>
	     </structure>
    </mapping> 	
</binding>						