		class="com.integrosys.cms.app.directorMaster.trx.CheckerApproveCreateDirectorMasterOperation">
		 <property name="directorMasterBusManager" ref="directorMasterBusManager"/>
        <property name="stagingDirectorMasterBusManager" ref="stagingDirectorMasterBusManager"/>
