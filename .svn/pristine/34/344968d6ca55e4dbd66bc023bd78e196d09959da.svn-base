        <property name="stagingDirectorMasterBusManager" ref="stagingDirectorMasterBusManager"/>
	</bean>
	
	
    <bean id="directorMasterReadController" class="com.integrosys.cms.app.directorMaster.trx.DirectorMasterReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadDirectorMasterOperation" value-ref="DirectorMasterTrx.ReadDirectorMasterOperation"/>
                 <entry key="ReadDirectorMasterIDOperation" value-ref="DirectorMasterTrx.ReadDirectorMasterIDOperation"/>
                
                
            </map>
        </property>
    </bean>

    <bean id="directorMasterTrxController" class="com.integrosys.cms.app.directorMaster.trx.DirectorMasterTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerEditRejectedUpdateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerEditRejectedUpdateDirectorMasterOperation"/>
            	   <entry key="MakerEditRejectedCreateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerEditRejectedCreateDirectorMasterOperation"/>
            	  
                  <entry key="MakerUpdateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerUpdateDirectorMasterOperation"/>
                  <entry key="MakerCreateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerCreateDirectorMasterOperation"/>
                  
                  <entry key="MakerCloseRejectedUpdateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerCloseRejectedUpdateDirectorMasterOperation"/>
                  <entry key="MakerCloseRejectedCreateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerCloseRejectedCreateDirectorMasterOperation"/>
                 
                  <entry key="MakerCloseRejectedDisableDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerCloseRejectedDisableDirectorMasterOperation"/>
                  
                  <entry key="MakerCloseRejectedEnableDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerCloseRejectedEnableDirectorMasterOperation"/>
                 
                  <entry key="CheckerApproveUpdateDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerApproveUpdateDirectorMasterOperation"/>
                  <entry key="CheckerApproveCreateDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerApproveCreateDirectorMasterOperation"/>
                  <entry key="CheckerRejectDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerRejectDirectorMasterOperation"/>
                  <entry key="CheckerCreateRejectDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerCreateRejectDirectorMasterOperation"/>
                  <entry key="CheckerEditRejectDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerEditRejectDirectorMasterOperation"/>
                  
                   <entry key="MakerDisableDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerDisableDirectorMasterOperation"/>
                   <entry key="MakerEditRejectedDisableDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerEditRejectedDisableDirectorMasterOperation"/>
                   <entry key="MakerEnableDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerEnableDirectorMasterOperation"/>
                   <entry key="MakerEditRejectedEnableDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerEditRejectedEnableDirectorMasterOperation"/>
                   
                   <entry key="CheckerDisableRejectDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerDisableRejectDirectorMasterOperation"/>
                   <entry key="CheckerEnableRejectDirectorMasterOperation" value-ref="DirectorMasterTrx.CheckerEnableRejectDirectorMasterOperation"/>
                   <entry key="MakerSaveDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerSaveDirectorMasterOperation"/>
                  <entry key="MakerSaveUpdateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerSaveUpdateDirectorMasterOperation"/>
                  <entry key="MakerUpdateDraftCreateDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerUpdateDraftCreateDirectorMasterOperation"/>
                  <entry key="MakerCloseDraftDirectorMasterOperation" value-ref="DirectorMasterTrx.MakerCloseDraftDirectorMasterOperation"/>
                                    
                 
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

    <bean id="directorMasterTrxControllerFactory" class="com.integrosys.cms.app.directorMaster.trx.DirectorMasterTrxControllerFactory">
        <property name="directorMasterReadController" ref="directorMasterReadController"/>
        <property name="directorMasterTrxController" ref="directorMasterTrxController"/>
    </bean>
</beans>