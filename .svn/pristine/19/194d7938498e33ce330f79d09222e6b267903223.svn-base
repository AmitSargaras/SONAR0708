<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans type="org.apache.struts.action.ActionFormBean">
		<!-- Mutual Funds list ActionForm. -->
		<form-bean name="generalParamListForm"
			type="com.integrosys.cms.ui.generalparam.GeneralParamListForm" />

		<!-- GeneralParam item ActionForm. -->
		<!--<form-bean name="generalParamItemForm"
			type="com.integrosys.cms.ui.generalparam.item.GeneralParamItemForm" />

	--></form-beans>

	<action-mappings
		type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
		
		<action path="/general_param_list"
			type="com.integrosys.cms.ui.generalparam.GeneralParamListAction"
			name="generalParamListForm" validate="false" scope="request">

			<forward name="list" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/generalParam/MakerListGeneralParam.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			
			<forward name="checker_view" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/generalParam/CheckerViewListGeneralParam.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="checker_paginate" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/generalParam/CheckerViewListGeneralParam.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			
			<forward name="add" path="/feed_mutual_funds_item.do?event=prepare" />
			<forward name="save" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_save.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="submit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="list2ForChecker"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/generalParam/CheckerListGeneralParam.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="list2ForMaker"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/generalParam/CheckerListGeneralParam.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="approve"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_approve.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="reject"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_reject.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="close"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_close.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="workInProgress"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/wip.html" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="view" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/generalParam/ViewGeneralParam.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
		</action>
		<!-- GeneralParam item Action. 
		<action path="/feed_mutual_funds_item"
			type="com.integrosys.cms.ui.feed.mutualfunds.item.GeneralParamItemAction"
			name="generalParamItemForm" validate="false" scope="request">

			<forward name="prepare"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/feed/mutual_funds/item/prepare.jsp" />
				<set-property property="title"
					value="title.manual.update.feed.mutualfunds" />
			</forward>
			<forward name="save"
				path="/feed_mutual_funds_list.do?event=listStaging" />

		</action>		
		
	--></action-mappings>

	<message-resources key="message-manualfeeds"
		parameter="ApplicationResources-manualfeeds" />

	<!-- <controller>
		<set-property property="processorClass" value="org.springframework.web.struts.DelegatingRequestProcessor"/>
		</controller> -->

	<!--<message-resources parameter="ApplicationResources-manualfeeds"/>-->
	<!-- 
		<plug-in className="org.springframework.web.struts.ContextLoaderPlugIn">
		<set-property property="contextConfigLocation" value="/WEB-INF/context/action-manualfeeds-servlet.xml"/>
		</plug-in>
		=======
		<message-resources key="message-manualfeeds" parameter="ApplicationResources-manualfeeds"/>
		
	-->
</struts-config>