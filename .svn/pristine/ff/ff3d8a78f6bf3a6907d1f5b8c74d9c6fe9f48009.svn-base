			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
	
	<description>
		Validator class to validate the EAI Message.
	</description>

	<bean id="validationMandatoryFieldFactory" class="com.integrosys.cms.host.eai.support.ValidationMandatoryFieldFactory">
		<constructor-arg>
			<set value-type="com.integrosys.cms.host.eai.support.ValidationMandatoryFieldKey">
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.ApprovedSecurity
					PROPERTY_securityReferenceNote
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.insurance.InsuranceSecurity
					PROPERTY_effectiveDate
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.cash.CashSecurity
					PROPERTY_securityIssuer
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.asset.SpecificChargeDetail
					PROPERTY_registrationNo
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.limit.bus.FacilityMaster
					PROPERTY_offerAcceptedDate
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.limit.bus.FacilityMaster
					PROPERTY_offerDate
				</value>
				<value>
					SOURCE_CLOS
					CLASS_com.integrosys.cms.host.eai.limit.bus.ChargeDetail
					PROPERTY_partyCharge
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.asset.VehicleDetail
					PROPERTY_PBTPBRInd
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.SecurityInsurancePolicy
					PROPERTY_insurerName
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.SecurityInsurancePolicy
					PROPERTY_insuranceType
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.SecurityInsurancePolicy
					PROPERTY_insuredAmount
				</value>
				<value>
					SOURCE_RLOS
					CLASS_com.integrosys.cms.host.eai.security.bus.SecurityInsurancePolicy
					PROPERTY_effectiveDate
				</value>
			</set>
