<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.limitsOfAuthorityMaster.bus">
	<class entity-name="actualLimitsOfAuthorityMaster" name="OBLimitsOfAuthorityMaster"
		table="CMS_LOA_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			 <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_LOA_MASTER_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		
		<property name="employeeGrade" column="EMPLOYEE_GRADE"/>
		<property name="rankingOfSequence" column="RANKING_OF_SEQUENCE" type="int"/>
		
		<property name="segment" column="SEGMENT" />
		<property name="limitReleaseAmt" column="LIMIT_RELEASE_AMT" type="big_decimal"/>
		<property name="totalSanctionedLimit" column="TOTAL_SANCTIONED_LIMIT" type="big_decimal"/>
		<property name="propertyValuation" column="PROPERTY_VALUATION_AMT" type="big_decimal"/>
		<property name="fdAmount" column="FD_AMOUNT" type="big_decimal"/>
		<property name="drawingPower" column="DRAWING_POWER" type="big_decimal"/>
		<property name="sblcSecurityOmv" column="SBLC_SECURITY_OMV" type="big_decimal"/>
		<property name="facilityCamCovenant" column="FACILITY_CAM_COVENANT"/>
	    
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
	</class>

	<class entity-name="stageLimitsOfAuthorityMaster" name="OBLimitsOfAuthorityMaster"
		table="CMS_STAGE_LOA_MASTER" lazy="false">
		<id name="id" column="ID" type="long">
			 <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STAGE_LOA_MASTER_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		
		<property name="employeeGrade" column="EMPLOYEE_GRADE"/>
		<property name="rankingOfSequence" column="RANKING_OF_SEQUENCE" type="int"/>
		
		<property name="segment" column="SEGMENT" />
		<property name="limitReleaseAmt" column="LIMIT_RELEASE_AMT" type="big_decimal"/>
		<property name="totalSanctionedLimit" column="TOTAL_SANCTIONED_LIMIT" type="big_decimal"/>
		<property name="propertyValuation" column="PROPERTY_VALUATION_AMT" type="big_decimal"/>
		<property name="fdAmount" column="FD_AMOUNT" type="big_decimal"/>
		<property name="drawingPower" column="DRAWING_POWER" type="big_decimal"/>
		<property name="sblcSecurityOmv" column="SBLC_SECURITY_OMV" type="big_decimal"/>
		<property name="facilityCamCovenant" column="FACILITY_CAM_COVENANT"/>
		
		
		<property name="status" column="STATUS" />
		<property name="deprecated" column="DEPRECATED" />
		<property name="creationDate" column="CREATION_DATE" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
		<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
	</class>
</hibernate-mapping>
