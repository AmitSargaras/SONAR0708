#!/bin/bash

. ../set_mig_env.sh $*

if [ ! "$IS_PROCEED" = "true" ]; then
	return
fi

"$DB2_HOME"/bin/db2 LOAD FROM "$FILE_PATH" OF ASC MODIFIED BY TIMESTAMPFORMAT=\"DDMMYYYY\" DUMPFILE="$DUMP_PATH" IMPLIEDDECIMAL STRIPTBLANKS NULLINDCHAR=Y METHOD L \( 1 1, 2 20, 21 25, 26 65, 66 70, 71 73, 74 76, 77 78, 79 83, 84 84, 85 184, 185 185, 186 205, 206 227, 228 235, 236 236, 237 244, 245 245\) MESSAGES "$LOG_PATH" TEMPFILES PATH "$TEMPFILE_PATH" REPLACE INTO "$DB_SCHEMA".MIG_CO012_IN \(RECORD_TYPE, SECURITY_ID, CMS_SECURITY_SUBTYPE_ID, SECURITY_REF_NOTE, SOURCE_SECURITY_TYPE, SOURCE_SECURITY_SUB_TYPE, CURRENCY, LOC_COUNTRY, LOC_ORG_CODE, CUSTODIAN_TYPE, SECURITY_CUSTODIAN, CHARGE_TYPE, CHARGE_NATURE, CHARGE_AMOUNT, LEGAL_CHARGE_DATE, IS_LEGAL_ENFORCE, IS_LEGAL_ENFORCE_DATE, COLLATERAL_STATUS\) NONRECOVERABLE INDEXING MODE AUTOSELECT