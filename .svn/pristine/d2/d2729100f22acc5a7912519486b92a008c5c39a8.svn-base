<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="NoCollateralForm" type="com.integrosys.cms.ui.collateral.nocollateral.nocollateral.NoCollateralaForm">
        </form-bean>
    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
		<action path="/NoCollateral"
			type="com.integrosys.cms.ui.collateral.nocollateral.NoCollateralAction"
			name="NoCollateralForm"
			validate="false"
			scope="request">
		    <forward name="read" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_read.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="read_return" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_read.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
			<forward name="prepare_create" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_update.jsp"/>
	            <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
			    <set-property property="title" value="title.security"/>
			</forward>
		    <forward name="prepare_update" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_update.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="process_update" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_update.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="update" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_save.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="update_return" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_update.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="approve" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_approve.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="reject" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_reject.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="submit" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_submit.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="save" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_save.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="close" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_close.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="close_return" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_close.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="process" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_process.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="process_return" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_process.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="prepare_close" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_close.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="track" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_close.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="track_return" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/collateral/nocollateral/nocollaterala/NoCollatera_close.jsp"/>
				<set-property property="title" value="title.security"/>
		    </forward>
		    <forward name="wip" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/wip.html"/>
				<set-property property="title" value="title.security"/>
		    </forward>
	        <forward name="delete" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_delete.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
            <forward name="add_pledge_prepare" path="/CleanPledge.do?event=add_pledge_prepare"/>
            <forward name="edit_pledge_prepare" path="/CleanPledge.do?event=edit_pledge_prepare"/>
            <forward name="view_pledge" path="/CleanPledge.do?event=view"/>
            
            <forward name="refresh_branch_detail" path="/collateral/common/custodiandetaillist.jsp"/>
            
            <forward name="refresh_city_id" path="/collateral/common/refresh_maker_create_city.jsp"/>
            	        
		</action>
    
    </action-mappings>
    
    <message-resources key="message-col-nocollateral" parameter="ApplicationResources-col-nocollateral"/>
</struts-config>