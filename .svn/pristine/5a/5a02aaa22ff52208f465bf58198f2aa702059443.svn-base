<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="DocumentCheckListForm" type="com.integrosys.cms.ui.checklist.document.DocumentCheckListForm">
        </form-bean>
    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/DocumentCheckList" type="com.integrosys.cms.ui.checklist.document.DocumentCheckListAction"
            name="DocumentCheckListForm" validate="false" scope="request">
            <forward name="after_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/document_list_all.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
             <forward name="imagePath"	path="/imageTag/imagePath.jsp" />
            <forward name="view_checklist" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/view_checklist_obj.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
            <forward name="view_checklist_image" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/view_checklist_image.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
            <forward name="after_main_borrower_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/main_borrower_doc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
            <forward name="after_non_borrower_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/non_borrower_doc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
            <forward name="after_co_borrower_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/co_borrower_doc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
            <forward name="after_pledgor_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/pledgor_doc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
            <forward name="after_securities_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/securities_doc_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.document.held"/>
            </forward>
			<forward name="after_print_letter_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/checklist/document/reminder_letter_list.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.list.letter.instruction"/>
            </forward>
            <forward name="after_print_main_borrower_list" path="/checklist/document/main_borrower_doc_list.jsp"/>
            <forward name="after_print_non_borrower_list" path="/checklist/document/non_borrower_doc_list.jsp"/>
            <forward name="after_print_co_borrower_list" path="/checklist/document/co_borrower_doc_list.jsp"/>
            <forward name="after_print_pledgor_list" path="/checklist/document/pledgor_doc_list.jsp"/>
            <forward name="after_print_securities_list" path="/checklist/document/securities_doc_list.jsp"/>
            <forward name="after_print_list_all" path="/checklist/document/print_document_list_all.jsp"/>
            <forward name="after_print_list_all_req" path="/checklist/document/document_list_all_req.jsp"/>
			<forward name="after_print_letter_list_doc" path="/checklist/document/reminder_letter_list_req.jsp"/>
			
			<forward name="print_image" path="/checklist/document/print_image_page.jsp"/>
                <forward name="downloadImage" path="/checklist/document/popup.jsp" />
                
            <forward name="image_gallary" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/checklist/document/image_gallary.jsp"/>
                	<set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                 	<set-property property="title" value="title.list.document.held"/>
            	</forward>
        </action>
    
    </action-mappings>
    
    <message-resources key="message-checklist-documentheld" parameter="ApplicationResources-checklist-documentheld"/>
</struts-config>