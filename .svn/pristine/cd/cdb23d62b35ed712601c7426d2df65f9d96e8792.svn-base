<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<description>Shared Security Mapping - SS001R</description>

	<include href="common-response.cm.xml" />

	<class extends="com.integrosys.cms.host.eai.EAIBody" name="com.integrosys.cms.host.eai.security.sharedsecurity.SharedSecurityResponseMsgBody" >
		<map-to xml="MsgBody"/>
		<field name="SharedSecurityResultItem" type="com.integrosys.cms.host.eai.security.sharedsecurity.bus.SharedSecurityResultItem" collection="vector">
			<bind-xml name="SecurityResult"/>
		</field>
	</class>

	<class name="com.integrosys.cms.host.eai.security.sharedsecurity.bus.SharedSecurityResultItem" collection="vector">
		<map-to xml="SecurityResult"/>
		<field name="CmsSecurityId" type="long">
			<bind-xml name="CMSSecurityId" node="element"/>
		</field>
		<field name="Country" type="string">
			<bind-xml name="Country" node="element"/>
		</field>
		<field name="PledgorLegalName" type="string">
			<bind-xml name="PledgorLegalName" node="element"/>
		</field>
		<field name="IdNumber" type="string">
			<bind-xml name="IdNumber" node="element"/>
		</field>
		<field name="Property" type="com.integrosys.cms.host.eai.security.bus.property.PropertySecurity">
			<bind-xml name="Property"/>
		</field>
		<field name="Vehicle" type="com.integrosys.cms.host.eai.security.sharedsecurity.bus.VehicleDetailSearchItem">
			<bind-xml name="AssetVehicle"/>
		</field>
		<field name="Aircraft" type="com.integrosys.cms.host.eai.security.sharedsecurity.bus.AirCraftDetailSearchItem">
			<bind-xml name="AssetAircraft"/>
		</field>
		<field name="Vessel" type="com.integrosys.cms.host.eai.security.sharedsecurity.bus.VesselDetailSearchItem">
			<bind-xml name="AssetVessel"/>
		</field>
		<field name="PlantEquip" type="com.integrosys.cms.host.eai.security.sharedsecurity.bus.PlantEquipDetailSearchItem">
			<bind-xml name="AssetPlantEquip"/>
		</field>
	</class>

	<include href="sharedsecurity-securitytypes.cm.xml" />
	
</mapping>