<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.aurionpro.clims.rest.interfaceLog">
	<class  entity-name="commonCodeInterfaceLog" name="com.aurionpro.clims.rest.interfaceLog.OBRestInterfaceLog" table="CC_REST_INTERFACE_LOG" lazy="false">
		
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CC_REST_INTERFACE_SEQ</param>
			</generator>
		</id>	
		<property name="requestId" column="REQUEST_ID" />
        <property name="channelCode"    	column="CHANNEL_CODE"  />		
		<property name="interfaceStatus"  	column="INTERFACE_STATUS"  />
		<property name="interfaceFailedReason"    	column="INTERFACE_FAILED_REASON"   />
		<property name="requestText"    	column="REQUEST_TEXT"     />
		<property name="requestDate"    	column="REQUEST_DATE"    		 />
		<property name="responseText"    	column="RESPONSE_TEXT"    />
		<property name="responseDate"    	column="RESPONSE_DATE"  />
	</class>
</hibernate-mapping>
 