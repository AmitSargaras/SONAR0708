<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="CommodityPriceForm" type="com.integrosys.cms.ui.commodityglobal.commodityprice.CommodityPriceForm">
        </form-bean>
    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
	    <action path="/CommodityPrice"
	        type="com.integrosys.cms.ui.commodityglobal.commodityprice.CommodityPriceAction"
	        name="CommodityPriceForm" validate="false" scope="request">
	        <forward name="select" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/commodityglobal/commodityprice/select.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/commodityglobal/commodityprice/update_commodityprice.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="process" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/commodityglobal/commodityprice/chk_view_commodityprice.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="read" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/commodityglobal/commodityprice/view_commodityprice.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="prepare_close" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/commodityglobal/commodityprice/view_commodityprice.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="to_track" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/commodityglobal/commodityprice/view_commodityprice.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="submit" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="update" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_save.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="approve" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="reject" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="close" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_close.jsp"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	        <forward name="wip" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/wip.html"/>
	            <set-property property="title" value="title.commodityglobal.commodityprice"/>
	        </forward>
	    </action>
    
    </action-mappings>
    
    <!--<message-resources parameter="ApplicationResources-param-commodity-price"/>-->
</struts-config>