				<set-property property="leftFrame"
					value="/template/common_left_frame.jsp" />
				<set-property property="rightFrame"
					value="/bfl/special_issue_clean_type_bfl.jsp" />
				<set-property property="title"
					value="title.bfl.specialissuecleantypebfl" />
			</forward>
			<forward name="after_confirm_special_issue_clean_type_bfl"
				path="/template/common_frame_template.jsp">
				<set-property property="leftFrame"
					value="/template/common_left_frame.jsp" />
