				<set-property property="title"
					value="title.bfl.senddraftbfl" />
			</forward>
			<forward name="after_recv_draft_bfl_ack"
				path="/template/common_frame_template.jsp">
				<set-property property="leftFrame"
					value="/template/common_left_frame.jsp" />
				<set-property property="rightFrame"
					value="/bfl/recv_draft_bfl_ack.jsp" />
				<set-property property="title"
					value="title.bfl.recvdraftbflack" />
			</forward>
			<forward name="after_confirm_recv_draft_bfl_ack"
