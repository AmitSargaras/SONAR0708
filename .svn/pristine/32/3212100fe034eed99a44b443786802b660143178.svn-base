		<action path="/TatDuration"
			type="com.integrosys.cms.ui.tatduration.TatDurationAction" name="TatDurationForm"
			validate="false" scope="request">
			
			<forward name="view_app_list" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/tat/view_appType_tat.jsp"/>
                <set-property property="title" value="menu.maintaintat.tatduration"/>
            </forward>
            
            <forward name="view_stage_list" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/tat/view_tat_param_stage.jsp"/>
                <set-property property="title" value="menu.maintaintat.tatduration"/>
            </forward>
            
            <forward name="after_submit_tat_duration" path="/template/common_template.jsp">
				<set-property property="title" value="title.common.confirmation" />
				<set-property property="rightFrame" value="/common/ack_submit.jsp" />
			</forward>
           
           <forward name="checker_prepare_update" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/tat/tat_param_duration_checker_view.jsp"/>
                <set-property property="title" value="menu.maintaintat.tatduration"/>
            </forward>
            
            <forward name="after_checker_process_tat_duration" path="/template/common_template.jsp">
				<set-property property="title" value="title.common.confirmation" />
				<set-property property="rightFrame" value="/common/ack_submit.jsp" />
			</forward>
			
           <forward name="view_property_index" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/tat/tat_param_duration_checker_view.jsp"/>
                <set-property property="title" value="menu.maintaintat.tatduration"/>
			</forward>

            <forward name="workInProgress" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.maintaintat.tatduration"/>
            </forward>
		</action>	
			
	</action-mappings>

	<message-resources key="message-tat"
		parameter="ApplicationResources-tat" />
</struts-config>