<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
  <description>ECO SECTOR LIMIT</description>
  <class name="com.integrosys.cms.app.creditriskparam.bus.sectorlimit.OBEcoSectorLimitParameter" auto-complete="false">
    <map-to xml="OBEcoSectorLimitParameter"/>
    <field name="loanPurposeCode" type="java.lang.String">
      <bind-xml name="loanPurposeCode" node="element"></bind-xml>
    </field>
    <field name="sectorCode" type="java.lang.String">
      <bind-xml name="sectorCode" node="element"></bind-xml>
    </field>
    <field name="limitPercentage" type="java.lang.Double">
      <bind-xml name="limitPercentage" node="element"></bind-xml>
    </field>
    <field name="conventionalBankPercentage" type="java.lang.Double">
      <bind-xml name="conventionalBankPercentage" node="element"></bind-xml>
    </field>
    <field name="islamicBankPercentage" type="java.lang.Double">
      <bind-xml name="islamicBankPercentage" node="element"></bind-xml>
    </field>
    <field name="investmentBankPercentage" type="java.lang.Double">
      <bind-xml name="investmentBankPercentage" node="element"></bind-xml>
    </field>
  </class>
</mapping>
