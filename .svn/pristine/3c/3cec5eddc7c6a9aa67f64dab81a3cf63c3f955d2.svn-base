    <bean id="npaTraqCodeMasterReadController" class="com.integrosys.cms.app.npaTraqCodeMaster.trx.NpaTraqCodeMasterReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.ReadNpaTraqCodeMasterOperation"/>
                 <entry key="ReadNpaTraqCodeMasterIDOperation" value-ref="NpaTraqCodeMasterTrx.ReadNpaTraqCodeMasterIDOperation"/>  
            </map>
        </property>
    </bean> 
    <bean id="npaTraqCodeMasterTrxController" class="com.integrosys.cms.app.npaTraqCodeMaster.trx.NpaTraqCodeMasterTrxController">
         <property name="nameTrxOperationMap">
        <map>
            <entry key="MakerCreateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerCreateNpaTraqCodeMasterOperation"/>
            <entry key="CheckerApproveCreateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.CheckerApproveCreateNpaTraqCodeMasterOperation"/>
           
            <entry key="MakerUpdateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerUpdateNpaTraqCodeMasterOperation"/>
            <entry key="CheckerApproveUpdateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.CheckerApproveUpdateNpaTraqCodeMasterOperation"/>
            <entry key="CheckerRejectNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.CheckerRejectNpaTraqCodeMasterOperation"/>
            <entry key="MakerEditRejectedCreateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerEditRejectedCreateNpaTraqCodeMasterOperation"/>
            <entry key="MakerEditRejectedUpdateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerEditRejectedUpdateNpaTraqCodeMasterOperation"/>
            <entry key="MakerCloseRejectedCreateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerCloseRejectedCreateNpaTraqCodeMasterOperation"/>
            <entry key="MakerCloseRejectedUpdateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerCloseRejectedUpdateNpaTraqCodeMasterOperation"/>
            <entry key="MakerSaveUpdateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerSaveUpdateNpaTraqCodeMasterOperation"/>
            <entry key="MakerSaveNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerSaveNpaTraqCodeMasterOperation"/>
            <entry key="MakerUpdateDraftCreateNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerUpdateDraftCreateNpaTraqCodeMasterOperation"/>
   			<entry key="MakerCloseDraftNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerCloseDraftNpaTraqCodeMasterOperation"/>
   			
   			<entry key="MakerDeleteNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerDeleteNpaTraqCodeMasterOperation"/>
   			<entry key="MakerEditRejectedDeleteNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerEditRejectedDeleteNpaTraqCodeMasterOperation"/>
   			<entry key="MakerCloseRejectedDeleteNpaTraqCodeMasterOperation" value-ref="NpaTraqCodeMasterTrx.MakerCloseRejectedDeleteNpaTraqCodeMasterOperation"/>
   			
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

