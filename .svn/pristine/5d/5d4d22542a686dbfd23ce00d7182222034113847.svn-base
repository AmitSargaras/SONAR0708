<binding name="sibs_collateral_trade_in_req">
    <include path="sibs_header_req.xml" />
    <mapping name="Definition" class="com.integrosys.simulator.host.SIBS.collateraltradein.OBSIBSCollateralTradeIn" extends="com.integrosys.simulator.host.SIBS.OBMessageHeader"> 
        <structure name="HeaderDefinition">
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
			<collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.collateraltradein.OBSIBSCollateralTradeIn">			        
			        <value name="FixColumn" field="collateralId"/>
			        <value name="FixColumn" field="makeOfGoods"/>
			        <value name="FixColumn" field="modelNumber"/>
			        <value name="FixColumn" field="yearOfManufacture"/>
			        <value name="FixColumn" field="registrationNumber"/>
			        <value name="FixColumn" field="tradeInValue"/>
			        <value name="FixColumn" field="tradeInDeposit"/>
				</structure>
		     </collection>
	     </structure>
    </mapping>  	
</binding>