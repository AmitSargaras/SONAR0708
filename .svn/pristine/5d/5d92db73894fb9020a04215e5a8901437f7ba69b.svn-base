<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainBaselForm" type="com.integrosys.cms.ui.baselmaster.MaintainBaselForm">
        </form-bean>
    </form-beans>
    
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/baselmaster" type="com.integrosys.cms.ui.baselmaster.MaintainBaselMasterAction" name="MaintainBaselForm" validate="false" scope="request">
        
        
            <forward name="list_basel_master_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_list_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
             <forward name="prepare_create_basel" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_add_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
             <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/ack_submit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="checker_basel_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/checker_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/view_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
             <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_read_delete_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="maker_veiw_edit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_edit_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_resubmit_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
             <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_close_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="maker_view_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/baselMaster/maker_view_todo_basel.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
            
             <forward name="common_resubmit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_resubmit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.basel_system_master.name"/>
            </forward>
        
        </action>
        </action-mappings>
</struts-config>
