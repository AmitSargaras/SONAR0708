<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="SRPCountryForm" type="com.integrosys.cms.ui.srp.country.SRPCountryForm"></form-bean>
        
        <form-bean name="SRPGlobalForm" type="com.integrosys.cms.ui.srp.global.SRPGlobalForm"></form-bean>
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/SRPCountry"
            type="com.integrosys.cms.ui.srp.country.SRPCountryAction"
            name="SRPCountryForm" scope="request">
            <forward name="maker_prepare_srpcountry_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/country/srpcountry_maker_prepare.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="maker_add_edit_srpcountry_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/country/srpcountry_maker_edit.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="srpcountry_maker_edit_successful" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="srpcountry_maker_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/country/srpcountry_view.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="checker_srpcountry_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/country/srpcountry_checker.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="view" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/country/srpcountry_view.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>
            <forward name="after_to_track" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/country/srpcountry_view.jsp"/>
                <set-property property="title" value="title.srpcountry"/>
            </forward>

        </action>

        <action path="/SRPGlobal"
            type="com.integrosys.cms.ui.srp.global.SRPGlobalAction"
            name="SRPGlobalForm" scope="request">
            <forward name="maker_prepare_srpglobal_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/global/srpglobal_maker_edit_prepare.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="maker_add_edit_srpglobal_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/global/srpglobal_maker_edit.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="srpglobal_maker_edit_successful" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="srpglobal_maker_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/global/srpglobal_view.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="checker_srpglobal_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/global/srpglobal_checker.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="view" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/global/srpglobal_view.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>
            <forward name="after_to_track" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/srp/global/srpglobal_view.jsp"/>
                <set-property property="title" value="title.srpglobal"/>
            </forward>

        </action>
    </action-mappings>
    
    <message-resources key="message-srp" parameter="ApplicationResources-srp"/>
</struts-config>