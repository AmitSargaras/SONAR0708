<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <class name="com.integrosys.cms.app.checklist.bus.OBShareDoc" auto-complete="false">
        <description>Default mapping for class com.integrosys.cms.app.checklist.bus.OBShareDoc</description>
        <map-to xml="OBShareDoc"/>
        <field name="CheckListId" type="long">
            <bind-xml name="CheckListId" node="element"/>
        </field>
        <field name="Details" type="java.lang.String">
            <bind-xml name="Details" node="element"/>
        </field>         
    </class>
	
	<class name="com.integrosys.cms.app.checklist.bus.OBCheckListItem" auto-complete="false">
        <description>Default mapping for class com.integrosys.cms.app.checklist.bus.OBCheckListItem</description>
        <map-to xml="OBCheckListItem"/>
        <field name="IsMandatoryInd" type="boolean">
            <bind-xml name="IsMandatoryInd" node="element"/>
        </field>
        <field name="IsInVaultInd" type="boolean">
            <bind-xml name="IsInVaultInd" node="element"/>
        </field>
        <field name="IsExtCustInd" type="boolean">
            <bind-xml name="IsExtCustInd" node="element"/>
        </field>
        <field name="IsAuditInd" type="boolean">
            <bind-xml name="IsAuditInd" node="element"/>
        </field>
        <field name="ParentItemID" type="long">
            <bind-xml name="ParentItemID" node="element"/>
        </field>
        <field name="ItemStatus" type="java.lang.String">
            <bind-xml name="ItemStatus" node="element"/>
        </field>
        <field name="ActionParty" type="java.lang.String">
            <bind-xml name="ActionParty" node="element"/>
        </field>
        <field name="CreditApprover" type="java.lang.String">
