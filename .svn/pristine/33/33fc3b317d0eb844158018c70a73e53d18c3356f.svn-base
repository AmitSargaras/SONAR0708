        </field>         
       <!-- <field name="ShareDocList" type="com.integrosys.cms.app.checklist.bus.IShareDoc" collection="array" >
        	<bind-xml name="ShareDocList" node="element"/>
        </field>-->
        <field name="ParentCheckListItemRef" type="long">
            <bind-xml name="ParentCheckListItemRef" node="element"/>
        </field>
        <field name="CustodianDocStatus" type="java.lang.String">
            <bind-xml name="CustodianDocStatus" node="element"/>
        </field>
    </class>
	
</mapping>