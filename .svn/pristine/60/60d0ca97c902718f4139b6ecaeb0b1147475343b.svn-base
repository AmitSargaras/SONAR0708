# Eoncms Db2 path and settings
DB2_HOME=/opt/IBM/db2/V9.1
DB_NAME=CMSTEST
DB_USER=cmstest
DB_PASSWD=cmstest
DB_SCHEMA=CMSTEST

# EonRlos database settings - used in locking and unlock database
RLOS_DB_NAME=EONRLOS
RLOS_USERNAME=rlos
RLOS_PASSWORD=rlos
RLOS_SCHEMA=RLOS

# Tablespace path
DB_DATAPATH=/db2/EONCMS

#Source file/ Source message path
MSG_DIR=source_file

# Log path
LOAD_LOG=/db2/migration/mig_sibs/scripts/load_log
DUMPPATH=$LOAD_LOG
TEMPFILE_PATH=$LOAD_LOG

# For QC reports
QC_PATH=qc_reports
QC_LOG_PATH=$QC_PATH/log

# DB  backup dump file path
DB_DUMPPATH=/db2/migration/dbbackup
DB_RESTOREPATH=/db2/migration/dbrestore


export DB2_HOME;