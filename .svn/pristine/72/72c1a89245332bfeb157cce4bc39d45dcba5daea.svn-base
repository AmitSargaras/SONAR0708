<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar
  PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd">
<weblogic-rdbms-jar>
   <weblogic-rdbms-bean>
      <ejb-name>EBDiaryItem</ejb-name>
      <data-source-name>CMSPool</data-source-name>
      <table-map>
         <table-name>CMS_DIARY_ITEM</table-name>
         <field-map>
            <cmp-field>cMPItemID</cmp-field>
            <dbms-column>ITEM_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>cMPTeamTypeID</cmp-field>
            <dbms-column>TEAM_TYPE_MEMBERSHIP_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>allowedCountry</cmp-field>
            <dbms-column>ALLOWED_COUNTRY</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>isObsoleteStr</cmp-field>
            <dbms-column>IS_OBSOLETE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>customerReference</cmp-field>
            <dbms-column>LE_ID</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>customerName</cmp-field>
            <dbms-column>CUSTOMER_NAME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>description</cmp-field>
            <dbms-column>DESCRIPTION</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>narration</cmp-field>
            <dbms-column>NARRATION</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>dueDate</cmp-field>
            <dbms-column>ITEM_DUE_DATE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>expiryDate</cmp-field>
            <dbms-column>ITEM_EXPIRY_DATE</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>lastUpdatedBy</cmp-field>
            <dbms-column>LAST_UPDATED_BY</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>lastUpdatedTime</cmp-field>
            <dbms-column>LAST_UPDATED_TIME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>versionTime</cmp-field>
            <dbms-column>VERSION_TIME</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>customerSegment</cmp-field>
            <dbms-column>CUSTOMER_SEGMENT</dbms-column>
         </field-map>
         <field-map>
            <cmp-field>fAM</cmp-field>
            <dbms-column>FAM</dbms-column>
         </field-map>
      </table-map>
   </weblogic-rdbms-bean>
</weblogic-rdbms-jar>