<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.integrosys.cms.batch.OBSchedulerLog"
		table="CMS_SCHEDULER_LOG" lazy="false">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">CMS_SCHEDULER_LOG_SEQ</param>
			</generator>
		</id>
		<version name="versionTime" column="VERSION_TIME" type="long"></version>
		<property name="schedulerCode" column="SCHEDULER_CODE" />
		<property name="schedulerName" column="SCHEDULER_NAME" />
		<property name="startDate" column="START_DATE" />
		<property name="endDate" column="END_DATE" />
		<property name="status" column="STATUS" />
		<property name="message" column="MESSAGE" />
	</class>
</hibernate-mapping>