<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Tat Doc and Tat Doc Draft</description>
    <class name="com.integrosys.cms.app.tatdoc.bus.OBTatDoc"
        auto-complete="false">
        <map-to xml="OBTatDoc"/>
        <field name="TatDocID" type="long">
            <bind-xml name="TatDocID" node="element"/>
        </field>
        <field name="LimitProfileID" type="long">
            <bind-xml name="LimitProfileID" node="element"/>
        </field>
        <field name="SolicitorInstructionDate" type="java.util.Date">
            <bind-xml name="SolicitorInstructionDate" node="element"></bind-xml>
        </field>
        <field name="DocReceviceForPADate" type="java.util.Date">
            <bind-xml name="DocReceviceForPADate" node="element"></bind-xml>
        </field>
        <field name="DocPAExcuteDate" type="java.util.Date">
            <bind-xml name="DocPAExcuteDate" node="element"></bind-xml>
        </field>
        <field name="PreDisbursementRemarks" type="java.lang.String">
            <bind-xml name="PreDisbursementRemarks" node="element"></bind-xml>
        </field>
        <field name="SolicitorAdviseReleaseDate" type="java.util.Date">
            <bind-xml name="SolicitorAdviseReleaseDate" node="element"></bind-xml>
        </field>
        <field name="DisbursementDocCompletionDate" type="java.util.Date">
            <bind-xml name="DisbursementDocCompletionDate" node="element"></bind-xml>
        </field>
        <field name="DisbursementDate" type="java.util.Date">
            <bind-xml name="DisbursementDate" node="element"></bind-xml>
        </field>
        <field name="DisbursementRemarks" type="java.lang.String">
            <bind-xml name="DisbursementRemarks" node="element"></bind-xml>
        </field>
        <field name="DocCompletionDate" type="java.util.Date">
            <bind-xml name="DocCompletionDate" node="element"></bind-xml>
        </field>
        <field name="PostDisbursementRemarks" type="java.lang.String">
            <bind-xml name="PostDisbursementRemarks" node="element"></bind-xml>
        </field>
    </class>
    <class name="com.integrosys.cms.app.tatdoc.bus.OBTatDocDraft"
        auto-complete="false">
        <map-to xml="OBTatDoc"/>
        <field name="DocDraftStage" type="java.lang.String">
            <bind-xml name="DocDraftStage" node="element"/>
        </field>
        <field name="DraftNumber" type="short">
            <bind-xml name="DraftNumber" node="element"/>
        </field>
        <field name="DraftDate" type="java.util.Date">
            <bind-xml name="DraftDate" node="element"></bind-xml>
        </field>  
    </class>
</mapping>