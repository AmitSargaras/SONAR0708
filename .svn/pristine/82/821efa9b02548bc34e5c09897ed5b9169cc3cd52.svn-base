<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.udf.bus">
	<class entity-name="actualUdf" name="OBUdf"  table="CMS_UDF" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_UDF_SEQ</param>
			 </generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long" />
			<property name="moduleId" column="MODULEID" type="long" />
			<property name="fieldName" column="FIELDNAME"/>
			<property name="fieldTypeId" column="FIELDTYPEID" type="long" />
			<property name="options" column="OPTIONS"/>
			<property name="fieldType" column="FIELDTYPE"/>
			<property name="moduleName" column="MODULENAME"/>
			<property name="sequence" column="SEQUENCE"  />
			<property name="mandatory" column="MANDATORY"  />
			<property name="numericLength" column="NUMERIC_LENGTH"  />
			<property name="status" column="STATUS"  />
			<property name="deprecated" column="DEPRECATED" />
			<property name="createBy" column="CREATED_BY" />
			<property name="creationDate" column="CREATION_DATE" />
			<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
			<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
	</class>
	
	<class entity-name="stageUdf" name="OBUdf"  table="CMS_STAGE_UDF" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STAGE_UDF_SEQ</param>
			 </generator>
		</id>
			<version name="versionTime" column="VERSION_TIME" type="long" />
			<property name="moduleId" column="MODULEID" type="long" />
			<property name="fieldName" column="FIELDNAME"/>
			<property name="fieldTypeId" column="FIELDTYPEID" type="long" />
			<property name="options" column="OPTIONS"/>
			<property name="fieldType" column="FIELDTYPE"/>
			<property name="moduleName" column="MODULENAME"/>
			<property name="sequence" column="SEQUENCE"  />
			<property name="mandatory" column="MANDATORY"  />
			<property name="numericLength" column="NUMERIC_LENGTH"  />
			<property name="status" column="STATUS"  />
			 <property name="deprecated" column="DEPRECATED" />
			<property name="createBy" column="CREATED_BY" />
			<property name="creationDate" column="CREATION_DATE" />
			<property name="lastUpdateBy" column="LAST_UPDATE_BY" />
			<property name="lastUpdateDate" column="LAST_UPDATE_DATE" />
	</class>
</hibernate-mapping>