<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
		<form-bean name="PreDealForm" type="com.integrosys.cms.ui.predeal.PreDealForm">
        </form-bean>
    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
		<action path="/PreDeal" type="com.integrosys.cms.ui.predeal.PreDealAction" 
			name="PreDealForm" scope="request">
			<forward name="maker_prepare_search" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_search.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>
			<forward name="maker_search" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_search_result.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>		
			<forward name="maker_view_ear_mark" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_view_ear_mark.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>
			<forward name="maker_prepare_new_mark" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_prepare_new_ear_mark.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  		
			<forward name="maker_prepare_release" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_prepare_release.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>                
			<forward name="maker_prepare_delete" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_prepare_delete.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>                
			<forward name="maker_prepare_transfer" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_prepare_transfer.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  
			<forward name="error_page" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_cap_not_set.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>				
			<forward name="maker_prepare_close_delete" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_maker_close_delete.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>                
			<forward name="maker_prepare_close_release" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_maker_close_release.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>                
			<forward name="maker_prepare_close_transfer" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_maker_close_transfer.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>     
			<forward name="maker_prepare_close_create" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_maker_close_new.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 		
			<forward name="maker_prepare_update_rejected_create" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_maker_update_rejected_create.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>
			<forward name="maker_prepare_view_concentration" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_maker_view_conc.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>		
			<forward name="maker_submit_release" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>                
			<forward name="maker_submit_delete" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>                
			<forward name="maker_submit_transfer" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>   
			<forward name="maker_presubmit_new_ear_mark" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_within_policy.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 		
			<forward name="maker_preupdate_rejected_create" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_within_policy.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 		
			<forward name="max_cap_breach" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_not_within_policy.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 				
			<forward name="quota_cap_breach" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_within_policy.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 			
			<forward name="maker_submit_new_ear_mark" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>   
			<forward name="maker_update_rejected_create" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>   		
			<forward name="wip" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/wip.html"/>
				<set-property property="title" value="title.pre.deal"/>
			</forward>	
			<forward name="maker_submit_close" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_close.jsp"/>
				<set-property property="title" value="title.pre.deal"/>
			</forward>        
			<forward name="checker_approve" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  
			<forward name="checker_reject" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  
			<forward name="checker_process_new" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_checker_process_new.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>   		
			<forward name="checker_process_delete" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_checker_process_delete.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  
			<forward name="checker_process_release" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_checker_process_release.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 
			<forward name="checker_process_transfer" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_checker_process_transfer.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  	
			<forward name="to_track_new" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_to_track_new.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>   		
			<forward name="to_track_delete" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_to_track_delete.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  
			<forward name="to_track_release" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_to_track_release.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward> 
			<forward name="to_track_transfer" path="/template/common_template.jsp">
			   <set-property property="rightFrame" value="/predeal/pre_deal_to_track_transfer.jsp"/>
			   <set-property property="title" value="title.pre.deal"/>
			</forward>  			
      	</action>	
    
    </action-mappings>
    
    <message-resources key="message-predealbooking" parameter="ApplicationResources-predealbooking"/>
</struts-config>