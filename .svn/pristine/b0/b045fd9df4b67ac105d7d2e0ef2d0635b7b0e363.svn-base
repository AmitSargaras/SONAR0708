            <bind-xml name="AmountApplied" node="element"/>
        </field>
        <field name="IncrementalLimit" type="java.math.BigDecimal">
            <bind-xml name="IncrementalLimit" node="element"></bind-xml>
        </field>
        <field name="ApplicationDate" type="java.util.Date">
            <bind-xml name="ApplicationDate" node="element"></bind-xml>
        </field>
        <field name="DateOfferAccepted" type="java.util.Date">
            <bind-xml name="DateOfferAccepted" node="element"></bind-xml>
        </field>
        <field name="DateApproved" type="java.util.Date">
            <bind-xml name="DateApproved" node="element"></bind-xml>
        </field>
        <field name="ApprovedByEntryCode" type="java.lang.String">
            <bind-xml name="ApprovedByEntryCode" node="element"></bind-xml>
        </field>
        <field name="CancelRejectCodeEntryCode" type="java.lang.String">
            <bind-xml name="CancelRejectCodeEntryCode" node="element"></bind-xml>
        </field>
        <field name="CancelRejectDate" type="java.util.Date">
