        </field>  
        <field name="RequestReasonEntryCode" type="java.lang.String">
            <bind-xml name="RequestReasonEntryCode" node="element"></bind-xml>
        </field> 
        <field name="DocumentationStatusEntryCode" type="java.lang.String">
            <bind-xml name="DocumentationStatusEntryCode" node="element"></bind-xml>
        </field> 
        <field name="DocumentationDate" type="java.util.Date">
            <bind-xml name="DocumentationDate" node="element"></bind-xml>
        </field>         
        <field name="LawyerCodeEntryCode" type="java.lang.String">
            <bind-xml name="LawyerCodeEntryCode" node="element"></bind-xml>
