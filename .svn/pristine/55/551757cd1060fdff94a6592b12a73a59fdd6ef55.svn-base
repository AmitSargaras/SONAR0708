<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>COUNTRY LIMIT</description>	
    <class name="com.integrosys.cms.app.creditriskparam.bus.countrylimit.OBCountryLimit" auto-complete="false">
        <map-to xml="OBCountryLimit"/>       
		<field name="CountryCode" type="java.lang.String">
            <bind-xml name="CountryCode" node="element"></bind-xml>
        </field>
		<field name="CountryRatingCode" type="java.lang.String">
            <bind-xml name="CountryRatingCode" node="element"></bind-xml>
        </field>		
		<field name="CountryLimitAmount" type="com.integrosys.base.businfra.currency.Amount">
            <bind-xml name="CountryLimitAmount" node="element"/>
        </field>   		
    </class>
</mapping>