#!/bin/bash

. ../set_mig_env.sh $*

if [ ! "$IS_PROCEED" = "true" ]; then
	return
fi

"$DB2_HOME"/bin/db2 LOAD FROM "$FILE_PATH" OF ASC MODIFIED BY TIMESTAMPFORMAT=\"DDMMYYYY\" DUMPFILE="$DUMP_PATH" IMPLIEDDECIMAL STRIPTBLANKS NULLINDCHAR=Y METHOD L \( 1 1, 2 44, 45 63, 64 66, 67 85, 86 104, 105 106, 107 116, 117 119\) MESSAGES "$LOG_PATH" TEMPFILES PATH "$TEMPFILE_PATH" REPLACE INTO "$DB_SCHEMA".MIG_CA003_3_OFF \(RECORD_TYPE, LMT_ID, LLP_BCA_REF_NUM, LMT_FAC_CODE, LMT_FAC_SEQ, SEQ_NUM, RELATIONSHIP_CODE_VALUE, OFFICER_CODE_VALUE, OFFICER_TYPE_CODE_VALUE\) NONRECOVERABLE INDEXING MODE AUTOSELECT

