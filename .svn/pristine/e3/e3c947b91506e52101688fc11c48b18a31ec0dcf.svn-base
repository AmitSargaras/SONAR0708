<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="ReportForm" type="com.integrosys.cms.ui.poi.report.ReportForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/poiReport" type="com.integrosys.cms.ui.poi.report.ReportAction" name="ReportForm" validate="false" scope="request">
            <forward name="list_report" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/listReports.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="prepare_filter" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/reportsFilters.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="submit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/reportsFilters.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="list" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/CustomerList.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="refresh_rm_id" path="/poiReport/relationshipMgrList.jsp"/>
            <forward name="list_guarantor" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/guarantorList.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="select_guarantor" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/reportsFilters.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>

            <forward name="list_branch" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/BranchList.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="paginate_branch_list" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/BranchList.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>

            <forward name="select_branch" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/reportsFilters.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="list_user" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/UserList.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="select_user" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/reportsFilters.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>

            <forward name="generate_report" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/downloadReports.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
             <forward name="generate_report_sms" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/poiReport/downloadReportsSMS.jsp"/>
                <set-property property="title" value="title.reports"/>
            </forward>
            <forward name="downloadReport" path="/poiReport/popup.jsp" />
        </action>
    </action-mappings>
    
    <!-- <message-resources key="message-Report" parameter="ApplicationResources-reports"/> -->
</struts-config>