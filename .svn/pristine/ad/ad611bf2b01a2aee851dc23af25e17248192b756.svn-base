<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.lei.json.dao">
	<class  entity-name="actualLEIValidationLog" name="OBLEIValidationLog" table="LEI_VALIDATION_LOG" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_LEI_INTERFACE_SEQ</param>
			</generator>
		</id>
		
        <property name="leiCode"    		column="LEICODE"  			/>		
        <property name="partyId"    		column="PARTYID"  			/>
        <property name="cmsMainProfileID"   column="CMSMAINPROFILEID"   />		
		<property name="interfaceName"    	column="INTERFACENAME"      />				
        <property name="requestMessage"		column="REQUESTMESSAGE"     />
		<property name="responseMessage" 	column="RESPONSEMESSAGE"    />
		<property name="requestDateTime"    column="REQUESTDATETIME"    />
		<property name="responseDateTime"   column="RESPONSEDATETIME"   />
		<property name="errorCode"    		column="ERRORCODE"    		/>
		<property name="errorMessage"    	column="ERRORMESSAGE"       />
		<property name="status"    			column="STATUS"             />
		<property name="lastValidatedDate"  column="LASTVALIDATEDDATE"  />
		<property name="lastValidatedBy"    column="LASTVALIDATEDBY"    />
		<property name="isLEICodeValidated" column="ISLEICODEVALIDATED" />
		

	</class>
</hibernate-mapping>
