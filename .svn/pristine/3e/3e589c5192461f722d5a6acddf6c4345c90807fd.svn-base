    </class>    
    
    	 <class entity-name="hrmsData" 
		name="com.integrosys.cms.app.relationshipmgr.bus.OBHRMSData" table="CMS_HRMS_DATA" lazy="false">
            <id name="id">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
