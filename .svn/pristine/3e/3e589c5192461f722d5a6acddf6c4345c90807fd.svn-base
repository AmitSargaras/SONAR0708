			<property name="localCADName" column="CAD_NAME"/>
			<property name="localCADEmailID" column="CAD_EMAIL_ID"/>
			<property name="localCADmobileNo" column="CAD_MOBILE_NO"/>
			<property name="localCADSupervisorName" column="CAD_SUPERVISOR_NAME"/>
			<property name="localCADSupervisorEmail" column="CAD_SUPERVISOR_EMAIL"/>
			<property name="localCADSupervisorMobileNo" column="CAD_SUPERVISOR_MOBILE_NO"/>
			<property name="relationshipMgrID" column="RM_MGR_ID"/>
            <property name="createdBy" column="CREATE_BY"/>
            <property name="creationDate" column="CREATION_DATE" />
            <property name="lastUpdateBy" column="LAST_UPDATE_BY"/>
            <property name="lastUpdateDate" column="LAST_UPDATE_DATE"/>            
			<property name="status" column="STATUS"/>
			<property name="deprecated"	column="DEPRECATED"/>
    </class>       	
    
        <class entity-name="stagingLocalCAD" 
		name="com.integrosys.cms.app.relationshipmgr.bus.OBLocalCAD" table="CMS_STG_LOCAL_CAD" lazy="false">
            <id name="id">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">CMS_STG_LOCAL_CAD_SEQ</param>
			   </generator>
            </id>
            <property name="versionTime" column="VERSION_TIME" type="long"/>
    		<property name="localCADEmployeeCode" column="CAD_EMPLOYEE_CODE"/>					
			<property name="localCADName" column="CAD_NAME"/>
			<property name="localCADEmailID" column="CAD_EMAIL_ID"/>
			<property name="localCADmobileNo" column="CAD_MOBILE_NO"/>
			<property name="localCADSupervisorName" column="CAD_SUPERVISOR_NAME"/>
			<property name="localCADSupervisorEmail" column="CAD_SUPERVISOR_EMAIL"/>
			<property name="localCADSupervisorMobileNo" column="CAD_SUPERVISOR_MOBILE_NO"/>
			<property name="relationshipMgrID" column="RM_MGR_ID"/>
            <property name="createdBy" column="CREATE_BY"/>
            <property name="creationDate" column="CREATION_DATE" />
            <property name="lastUpdateBy" column="LAST_UPDATE_BY"/>
            <property name="lastUpdateDate" column="LAST_UPDATE_DATE"/>            
			<property name="status" column="STATUS"/>
