######################
# MQ Settings
######################

mq.queue.manager=QM_CLM
mq.host.ip=localhost
mq.port=1414
mq.listener.concurrent.consumers=1

# obsolete config for MQ classes prior to Spring adoption
mq.server.isremote=true
mq.ccsid=819

# channel config is only required for client conn, must be authenticated by mq user (MCA)
mq.in.channel.name=CLM.SVRCONN
mq.out.channel.name=

# queues for Trade host (XML)
mq.trade.in.queue=trade.out
mq.trade.out.queue=trade.in
