<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
	
	<description>
		To be used for transaction controller and transaction operation.
		Transaction operation will have the property to set bus manager, which get from 'AppContext_Services.xml'
	</description>

	<!-- CreditApproval -->
	
	<bean id="CreditApprovalTrx.CheckerApproveUpdateOperation" class="com.integrosys.cms.app.creditApproval.trx.CheckerApproveUpdateOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.CheckerApproveCreateCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.CheckerApproveCreateCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>

	<bean id="CreditApprovalTrx.CheckerRejectOperation" class="com.integrosys.cms.app.creditApproval.trx.CheckerRejectOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerCloseDraftOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerCloseDraftOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerCloseRejectedOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerCloseRejectedOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerSubmitOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerSubmitOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerSubmitRejectedOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerSubmitRejectedOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerUpdateOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerUpdateOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerUpdateRejectedOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerUpdateRejectedOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.ReadCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.ReadCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	<bean id="CreditApprovalTrx.MakerEditRejectedUpdateCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerEditRejectedUpdateCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerEditRejectedDeleteCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerEditRejectedDeleteCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerUpdateSubmitOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerUpdateSubmitOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerDeleteCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerDeleteCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerEditRejectedCreateCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerEditRejectedCreateCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerUpdateDraftCreateCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerUpdateDraftCreateCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerSaveCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerSaveCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerSaveUpdateCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerSaveUpdateCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	<bean id="CreditApprovalTrx.MakerCloseDraftCreditApprovalOperation" class="com.integrosys.cms.app.creditApproval.trx.MakerCloseDraftCreditApprovalOperation">
		<property name="creditApprovalBusManager" ref="creditApprovalBusManager" />
		<property name="creditApprovalBusManagerStaging" ref="stagingCreditApprovalBusManager" />
	</bean>
	
	

	<bean id="CreditApprovalReadController" class="com.integrosys.cms.app.creditApproval.trx.CreditApprovalReadController">
		<property name="nameTrxOperationMap">
			<map>
				<entry key="ReadCreditApprovalOperation" value-ref="CreditApprovalTrx.ReadCreditApprovalOperation" />
			</map>
		</property>
	</bean>
	
	<bean id="CreditApprovalTrxContoller" class="com.integrosys.cms.app.creditApproval.trx.CreditApprovalTrxController">
		<property name="nameTrxOperationMap">
			<map>
				<entry key="CheckerApproveUpdateOperation" value-ref="CreditApprovalTrx.CheckerApproveUpdateOperation" />
				<entry key="CheckerApproveCreateCreditApprovalOperation" value-ref="CreditApprovalTrx.CheckerApproveCreateCreditApprovalOperation" />
				<entry key="CheckerRejectOperation" value-ref="CreditApprovalTrx.CheckerRejectOperation" />
				<entry key="MakerCloseDraftOperation" value-ref="CreditApprovalTrx.MakerCloseDraftOperation" />
				<entry key="MakerCloseRejectedOperation" value-ref="CreditApprovalTrx.MakerCloseRejectedOperation" />
				<entry key="MakerSubmitOperation" value-ref="CreditApprovalTrx.MakerSubmitOperation" />
				<entry key="MakerSubmitRejectedOperation" value-ref="CreditApprovalTrx.MakerSubmitRejectedOperation" />
				<entry key="MakerUpdateOperation" value-ref="CreditApprovalTrx.MakerUpdateOperation" />
				<entry key="MakerUpdateRejectedOperation" value-ref="CreditApprovalTrx.MakerUpdateRejectedOperation" />
				<entry key="MakerEditRejectedUpdateCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerEditRejectedUpdateCreditApprovalOperation" />
				<entry key="MakerEditRejectedDeleteCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerEditRejectedDeleteCreditApprovalOperation" />
				<entry key="MakerUpdateSubmitOperation" value-ref="CreditApprovalTrx.MakerUpdateSubmitOperation" />
				<entry key="MakerDeleteCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerDeleteCreditApprovalOperation" />
				<entry key="MakerEditRejectedCreateCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerEditRejectedCreateCreditApprovalOperation" />
				<entry key="MakerUpdateDraftCreateCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerUpdateDraftCreateCreditApprovalOperation" />
				<entry key="MakerSaveCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerSaveCreditApprovalOperation" />
				<entry key="MakerSaveUpdateCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerSaveUpdateCreditApprovalOperation" />
				<entry key="MakerCloseDraftCreditApprovalOperation" value-ref="CreditApprovalTrx.MakerCloseDraftCreditApprovalOperation" />	
			</map>
		</property>
		<property name="processTrxManager" ref="processTrxManager" />
	</bean>
	
	
	
	
	<bean id="CreditApprovalTrx.MakerCreditApprovalInsertFileOperation"
		class="com.integrosys.cms.app.creditApproval.trx.MakerCreditApprovalInsertFileOperation">
		 <property name="creditApprovalFileMapperIdBusManager" ref="creditApprovalFileMapperIdBusManager"/>
        <property name="stagingCreditApprovalFileMapperIdBusManager" ref="stagingCreditApprovalFileMapperIdBusManager"/>
	</bean>
	<bean id="CreditApprovalTrx.CheckerApproveInsertMasterOperation"
		class="com.integrosys.cms.app.creditApproval.trx.CheckerApproveInsertMasterOperation">
		 <property name="creditApprovalFileMapperIdBusManager" ref="creditApprovalFileMapperIdBusManager"/>
        <property name="stagingCreditApprovalFileMapperIdBusManager" ref="stagingCreditApprovalFileMapperIdBusManager"/>
	</bean>
	<bean id="CreditApprovalTrx.CheckerRejectInsertMasterOperation"
		class="com.integrosys.cms.app.creditApproval.trx.CheckerRejectInsertMasterOperation">
		 <property name="creditApprovalFileMapperIdBusManager" ref="creditApprovalFileMapperIdBusManager"/>
        <property name="stagingCreditApprovalFileMapperIdBusManager" ref="stagingCreditApprovalFileMapperIdBusManager"/>
	</bean>
	<bean id="CreditApprovalTrx.FileInsertReadOperation"
		class="com.integrosys.cms.app.creditApproval.trx.FileInsertReadOperation">
		<property name="creditApprovalFileMapperIdBusManager" ref="creditApprovalFileMapperIdBusManager"/>
        <property name="stagingCreditApprovalFileMapperIdBusManager" ref="stagingCreditApprovalFileMapperIdBusManager"/>
	</bean>
	<bean id="CreditApprovalTrx.MakerCloseRejectedInsertMasterOperation"
		class="com.integrosys.cms.app.creditApproval.trx.MakerCloseRejectedInsertMasterOperation">
		 <property name="creditApprovalFileMapperIdBusManager" ref="creditApprovalFileMapperIdBusManager"/>
        <property name="stagingCreditApprovalFileMapperIdBusManager" ref="stagingCreditApprovalFileMapperIdBusManager"/>
	</bean>
	<bean id="CreditApprovalTrx.CheckerCreateCreditApprovalOperation"
		class="com.integrosys.cms.app.creditApproval.trx.CheckerCreateCreditApprovalOperation">
		<property name="creditApprovalFileMapperIdBusManager" ref="creditApprovalFileMapperIdBusManager"/>
        <property name="stagingCreditApprovalFileMapperIdBusManager" ref="stagingCreditApprovalFileMapperIdBusManager"/>
	</bean>
	
	
	 <bean id="creditApprovalInsertFileTrxController" class="com.integrosys.cms.app.creditApproval.trx.InsertFileTrxController">
        <property name="nameTrxOperationMap">
            <map>        	 
                  <entry key="MakerCreditApprovalInsertFileOperation" value-ref="CreditApprovalTrx.MakerCreditApprovalInsertFileOperation" />
                  <entry key="CheckerApproveInsertMasterOperation" value-ref="CreditApprovalTrx.CheckerApproveInsertMasterOperation" />
                  <entry key="CheckerRejectInsertMasterOperation" value-ref="CreditApprovalTrx.CheckerRejectInsertMasterOperation" />
                  <entry key="MakerCloseRejectedInsertMasterOperation" value-ref="CreditApprovalTrx.MakerCloseRejectedInsertMasterOperation"/>
                   <entry key="CheckerCreateCreditApprovalOperation" value-ref="CreditApprovalTrx.CheckerCreateCreditApprovalOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
    
    <bean id="readCreditApprovalInsertFileTrxController" class="com.integrosys.cms.app.creditApproval.trx.ReadInsertFileTrxController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="FileInsertReadOperation" value-ref="CreditApprovalTrx.FileInsertReadOperation"/>                   
            </map>
        </property>
    </bean>
	
	
	

	<bean id="creditApprovalTrxControllerFactory" class="com.integrosys.cms.app.creditApproval.trx.CreditApprovalTrxControllerFactory">
		<property name="creditApprovalReadController" ref="CreditApprovalReadController" />
		<property name="creditApprovalTrxContoller" ref="CreditApprovalTrxContoller" />
		 <property name="creditApprovalInsertFileTrxController" ref="creditApprovalInsertFileTrxController"/>
        <property name="readCreditApprovalInsertFileTrxController" ref="readCreditApprovalInsertFileTrxController"/>
	</bean>
	

</beans>