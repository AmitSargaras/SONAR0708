<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<description>Castor Common Mapping for response message</description>

	<class name="com.integrosys.cms.host.eai.EAIMessage">
		<map-to xml="Msg"/>
		<field name="MsgHeader" type="com.integrosys.cms.host.eai.EAIHeader">
			<bind-xml name="MsgHeader" />
		</field>
		<field name="MsgBody" type="com.integrosys.cms.host.eai.EAIBody">
			<bind-xml name="MsgBody" />
		</field>
	</class>

	<class name="com.integrosys.cms.host.eai.EAIHeader">
		<map-to xml="MsgHeader"/>
		<field name="MessageId" type="string">
			<bind-xml name="MessageId" />
		</field>
		<field name="MessageType" type="string">
			<bind-xml name="MessageType" />
		</field>
		<field name="MessageRefNum" type="string">
			<bind-xml name="MessageRefNum" />
		</field>
		<field name="PublishType" type="string">
			<bind-xml name="PublishType" />
		</field>
		<field name="PublishDate" type="string">
			<bind-xml name="PublishDate" />
		</field>
		<field name="Source" type="string">
			<bind-xml name="Source" />
		</field>
	</class>

	<class name="com.integrosys.cms.host.eai.StandardCode">
		<field name="StandardCodeNumber" type="string">
			<bind-xml name="StandardCodeNumber" />
		</field>
		<field name="StandardCodeValue" type="string">
			<bind-xml name="StandardCodeValue" />
		</field>
		<!--
		<field name="StandardCodeDescription" type="string">
			<bind-xml name="StandardCodeDescription" />
		</field>
		-->
	</class>

    <class name="com.integrosys.cms.host.eai.OriginatingBookingLocation">
        <map-to xml="OriginatingBookingLocation"/>
        <field name="OriginatingLocationId" type="int">
			<bind-xml name="OriginatingLocationId" />
		</field>
        <field name="OriginatingLocationCountry" type="string">
			<bind-xml name="OriginatingLocationCountry" />
		</field>
        <field name="OriginatingLocationOrganisation" type="string">
			<bind-xml name="OriginatingLocationOrganisation" />
		</field>
		<!-- 
        <field name="OriginatingLocationOrganisationDesc" type="string">
			<bind-xml name="OriginatingLocationOrganisationDesc" />
		</field>
		-->
    </class>

</mapping>