            <map>
                <entry key="ReadRiskTypeOperation" value-ref="RiskTypeTrx.ReadRiskTypeOperation"/>
                 <entry key="ReadRiskTypeIDOperation" value-ref="RiskTypeTrx.ReadRiskTypeIDOperation"/>  
            </map>
        </property>
    </bean> 
    <bean id="riskTypeTrxController" class="com.integrosys.cms.app.riskType.trx.RiskTypeTrxController">
         <property name="nameTrxOperationMap">
        <map>
            <entry key="MakerCreateRiskTypeOperation" value-ref="RiskTypeTrx.MakerCreateRiskTypeOperation"/>
            <entry key="CheckerApproveCreateRiskTypeOperation" value-ref="RiskTypeTrx.CheckerApproveCreateRiskTypeOperation"/>
           <entry key="CheckerApproveDeleteRiskTypeOperation" value-ref="RiskTypeTrx.CheckerApproveDeleteRiskTypeOperation"/>
           
            <!--added by santosh-UBS CR  -->
            <entry key="MakerUpdateRiskTypeOperation" value-ref="RiskTypeTrx.MakerUpdateRiskTypeOperation"/>
            <entry key="CheckerApproveUpdateRiskTypeOperation" value-ref="RiskTypeTrx.CheckerApproveUpdateRiskTypeOperation"/>
            <entry key="CheckerRejectRiskTypeOperation" value-ref="RiskTypeTrx.CheckerRejectRiskTypeOperation"/>
            <entry key="MakerEditRejectedCreateRiskTypeOperation" value-ref="RiskTypeTrx.MakerEditRejectedCreateRiskTypeOperation"/>
            <entry key="MakerEditRejectedUpdateRiskTypeOperation" value-ref="RiskTypeTrx.MakerEditRejectedUpdateRiskTypeOperation"/>
            <entry key="MakerCloseRejectedCreateRiskTypeOperation" value-ref="RiskTypeTrx.MakerCloseRejectedCreateRiskTypeOperation"/>
            <entry key="MakerCloseRejectedUpdateRiskTypeOperation" value-ref="RiskTypeTrx.MakerCloseRejectedUpdateRiskTypeOperation"/>
            <entry key="MakerSaveUpdateRiskTypeOperation" value-ref="RiskTypeTrx.MakerSaveUpdateRiskTypeOperation"/>
            <entry key="MakerSaveRiskTypeOperation" value-ref="RiskTypeTrx.MakerSaveRiskTypeOperation"/>
            <entry key="MakerUpdateDraftCreateRiskTypeOperation" value-ref="RiskTypeTrx.MakerUpdateDraftCreateRiskTypeOperation"/>
   			<entry key="MakerCloseDraftRiskTypeOperation" value-ref="RiskTypeTrx.MakerCloseDraftRiskTypeOperation"/>
   			<entry key="MakerDeleteRiskTypeOperation" value-ref="RiskTypeTrx.MakerDeleteRiskTypeOperation"/>
            <entry key="MakerCloseRejectedDeleteRiskTypeOperation" value-ref="RiskTypeTrx.MakerCloseRejectedDeleteRiskTypeOperation"/>
            <entry key="MakerEditRejectedDeleteRiskTypeOperation" value-ref="RiskTypeTrx.MakerEditRejectedDeleteRiskTypeOperation"/>
            
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

