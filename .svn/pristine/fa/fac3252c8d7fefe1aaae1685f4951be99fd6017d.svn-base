<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
<form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainNewTatForm" type="com.integrosys.cms.ui.newtatmaster.MaintainNewTatForm">
        </form-bean>
    </form-beans>
    
  <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
   <action path="/tatmaster" type="com.integrosys.cms.ui.newtatmaster.MaintainNewTatMasterAction" name="MaintainNewTatForm" validate="false" scope="request">
   
    <forward name="maker_list_tat_master" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/list_tat_event.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
    </forward>
            
   <forward name="maker_view_tat_event" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/maker_view_tat_event.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
  </forward>
  
  <forward name="maker_edit_tat_event" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/maker_edit_tat_event.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
  </forward>
  
   <forward name="work_in_process" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
     </forward>
     
     <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/ack_submit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
      </forward>
      
      
        <forward name="checker_process_edit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/checker_process_edit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
      </forward>
      
      <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
            </forward>
            
      <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
      </forward>
      
      
      <forward name="maker_prepare_resubmit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/maker_resubmit_tat_master.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
            </forward>
            
      <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/maker_close_tat_master.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
            </forward>
            
        <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
            </forward>
            
        <forward name="common_resubmit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_resubmit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
            </forward>
            
        <forward name="chk_view_to_track_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/newTatMaster/view_to_track_tat_master.jsp"/>
                <set-property property="title" value="menu.manual_feeds.tat_master.name"/>
            </forward>    
   
   
  </action>
  </action-mappings>
</struts-config>
