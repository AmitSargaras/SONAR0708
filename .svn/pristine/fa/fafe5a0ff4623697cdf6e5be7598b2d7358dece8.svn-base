<reports>
	<report>
		<query>
			SELECT sp.LSP_SHORT_NAME,
			  cc_constit.entry_name AS constitution,
			  sp.PAN,
			  lc.CUSTOMER_RAM_ID,
			  lc.CUSTOMER_APR_CODE,
			  lc.CUSTOMER_EXT_RATING,
			  lc.IS_NBFS,
			  cc_nbfc_a.entry_name AS NBFS_A,
			  cc_nbfc_b.entry_name AS NBFS_B,
			  lc.IS_PRIORITY_SECTOR,
			  cc_msme.entry_name AS MSMEClassificationlc,
			  lc.IS_PERMENENT_SSI_CERT,
			  lc.IS_WEAKER_SECTION,
			  cc_type.entry_name AS Type,
			  lc.IS_KISAN_CREDIT_CARD,
			  cff_kcc.FACILITY_NAME   AS KCC_FACILITY_NAME,
			  cff_kcc.FACILITY_AMOUNT AS KCC_FACILITY_AMOUNT,
			  lc.IS_MINORITY_COMMUNITY,
			  lc.IS_CAPITAL_MARKET_EXPOS,
			  lc.IS_REAL_ESTATE_EXPOS,
			  lc.IS_COMMODITIES_EXPOSER,
			  lc.IS_SENSITIVE,
			  cc_commodity.entry_name AS CommodityName,
			  lc.GROSS_INVS_IN_PM,
			  'INR' AS PM_currency,
			  lc.GROSS_INVS_IN_EQUIP,
			  'INR' AS EQUIP_currency,
			  pg.GROUP_EXP_LIMIT,
			  lc.PSU,
			  lc.PSU_PERCENTAGE,
			  lc.IS_BACKED_BY_GOVT,
			  cff_bgovt.FACILITY_NAME   AS GOVT_FACILITY_NAME,
			  cff_bgovt.FACILITY_AMOUNT AS GOVT_FACILITY_AMOUNT,
			  'INR'                     AS faci_currency,
			  lc.GOVT_UNDER_TAKING,
			  <!-- cc_bank_method.entry_name AS BankingMethod, -->
			  ( SELECT get_banking_method(SP.LSP_LE_ID) AS BANKING_METHOD
     			FROM   CMS_BANKING_METHOD_CUST WHERE ROWNUM = 1)  As BankingMethod,
			  lc.IS_INFRASTRUCTURE_FINANACE,
			  cff_InfrFin.FACILITY_NAME   AS INFRA_FACILITY_NAME,
			  cff_InfrFin.FACILITY_AMOUNT AS INFRA_FACILITY_AMOUNT,
			  'INR'                       AS faci1_currency,
			  cc_infra_fin.entry_name     AS InfrastructureFinanceType,
			  lc.IS_SEMS_GUIDE_APPLICABLE,
			  lc.IS_FAIL_IFC_EXCLU_LIST,
			  lc.IS_TUFS,
			  lc.IS_RBI_DEFAULTER,
			  lc.IS_LITIGATION,
			  IS_INTEREST_OF_BANK,
			  lc.IS_ADVERSE_REMARK,
			  lc.AUDIT_TYPE,
			  lc.AVG_ANNUAL_TURNOVER,
			  sp.rbi_ind_code
			  ||'-'
			  ||cc_rbi.entry_name    AS rbi,
			  cc_industry.ENTRY_NAME AS industryName,
			  lc.INDUSTRY_EXPOSER,
			  lc.IS_DIREC_OTHER_BANK,
			  lc.IS_PARTNER_OTHER_BANK,
			  lc.IS_SUBSTANTIAL_OTHER_BANK,
			  lc.IS_HDFC_DIREC_RLTV,
			  lc.IS_OBK_DIREC_RLTV,
			  lc.IS_PARTNER_DIREC_RLTV,
			  lc.IS_SUBSTANTIAL_RLTV_HDFC_OTHER,
			  (select entry_name from common_code_category_entry where entry_code=lc.ENTITY_TYPE and category_code='ENTITY_TYPE') as entity_type,
				(CASE WHEN lc.IS_SPV_FUNDING is NULL THEN 'No' ELSE lc.IS_SPV_FUNDING END) AS IS_SPV_FUNDING,
				(CASE WHEN lc.INDIRECT_COUNTRY_RISK_EXPOSURE is NULL THEN 'No' ELSE lc.INDIRECT_COUNTRY_RISK_EXPOSURE END) AS INDIRECT_COUNTRY_RISK_EXPOSURE,
				(select country_name from cms_country where id=lc.COUNTRYNAME) as country,
				lc.SALESPERCENTAGE,
				(CASE WHEN lc.ISCGTMSE is NULL THEN 'No' ELSE lc.ISCGTMSE END) AS ISCGTMSE,
				(CASE WHEN lc.ISIPRE is NULL THEN 'No' ELSE lc.ISIPRE END) AS ISIPRE,
				(CASE WHEN lc.FINANCE_FOR_ACCQUISITION is NULL THEN 'No' ELSE lc.FINANCE_FOR_ACCQUISITION END) AS FINANCE_FOR_ACCQUISITION,
				(select new_facility_name from cms_facility_new_master where new_facility_code=lc.FACILITY_APPROVED) approved_facility,
				lc.FACILITY_AMOUNT,
				(select new_collateral_description from cms_collateral_new_master where new_collateral_code=lc.SECURITY_NAME) security_name,
				(select entry_name from common_code_category_entry where entry_code = lc.SECURITY_TYPE and category_code='31') security_type,
				lc.SECURITY_VALUE,
				(CASE WHEN lc.COMPANY_TYPE is NULL THEN 'No' ELSE lc.COMPANY_TYPE END) AS COMPANY_TYPE,
				lc.NAME_OF_HOLDING_COMPANY,
				(select entry_name from common_code_category_entry where entry_code= lc.TYPE and category_code='TYPE_OF_COMPANY') type_comp,
				(CASE WHEN lc.IF_BREACH_WITH_PRESCRIPTION is NULL THEN 'No' ELSE lc.IF_BREACH_WITH_PRESCRIPTION END) AS IF_BREACH_WITH_PRESCRIPTION,
				lc.COMMENTS,
				(select entry_name from common_code_category_entry where entry_code=lc.CATEGORY_OF_FARMER and category_code='CATEGORY_OF_FARMER') category_of_farmer,
				lc.LANDHOLDING,
				(select country_name from cms_country where id=lc.COUNTRY_OF_GAURANTOR) COUNTRY_OF_GAURANTOR,
				(CASE WHEN lc.IS_AFFORDABLE_HOUSING_FINANCE is NULL THEN 'No' ELSE lc.IS_AFFORDABLE_HOUSING_FINANCE END) AS IS_AFFORDABLE_HOUSING_FINANCE,
				(CASE WHEN lc.IS_INRESTRICTED_LIST is NULL THEN 'No' ELSE lc.IS_INRESTRICTED_LIST END) AS IS_INRESTRICTED_LIST,
				(select entry_name from common_code_category_entry where category_code='RESTRICTED_FINANCING' and entry_code=lc.RESTRICTED_FINANCING) RESTRICTED_FINANCING,
				(CASE WHEN lc.RESTRICTED_INDUSTRIES is NULL THEN 'No' ELSE lc.RESTRICTED_INDUSTRIES END) AS RESTRICTED_INDUSTRIES,
				(select entry_name from common_code_category_entry where category_code='CREDIT_LIST_RESTRICTED_INDUSTRIES' and entry_code=lc.RESTRICTEDLISTINDUSTRIES) as restricted_industries_value,
				(CASE WHEN lc.IS_QUALIFYING_NOTES_PRESENT is NULL THEN 'No' ELSE lc.IS_QUALIFYING_NOTES_PRESENT END) AS IS_QUALIFYING_NOTES_PRESENT,
				lc.STATE_IMPLICATIONS,
				(CASE WHEN lc.IS_BORROWER_IN_REJECT_DATABASE is NULL THEN 'No' ELSE lc.IS_BORROWER_IN_REJECT_DATABASE END) AS IS_BORROWER_IN_REJECT_DATABASE,
				lc.REJECT_HISTORY_REASON,
				(CASE WHEN lc.CAPITAL_FOR_COMMODITY is NULL THEN 'Non Broker' ELSE lc.CAPITAL_FOR_COMMODITY END) AS CAPITAL_FOR_COMMODITY,
				(CASE WHEN lc.OBJECTFINANCE is NULL THEN 'No' ELSE lc.OBJECTFINANCE END) AS OBJECTFINANCE,
				(CASE WHEN lc.IS_COMMODITY_FINANCE_CUST is NULL THEN 'No' ELSE lc.IS_COMMODITY_FINANCE_CUST END) AS IS_COMMODITY_FINANCE_CUST,
				(CASE WHEN lc.RESTRUCTURED_BOR_FAC is NULL THEN 'No' ELSE lc.RESTRUCTURED_BOR_FAC END) AS RESTRUCTURED_BOR_FAC,
				(select new_facility_name from cms_facility_new_master where new_facility_code=lc.FACILITY) facility,
				lc.LIMIT_AMOUNT,
				(CASE WHEN lc.CONDUCT_OTHER_BANK is NULL THEN 'Satisfactory' ELSE lc.CONDUCT_OTHER_BANK END) AS CONDUCT_OTHER_BANK,
				lc.CRILIC_STATUS,
				lc.COMMENT_CRILIC,
				(CASE WHEN lc.SUBSIDY_FLAG is NULL THEN 'No' ELSE lc.SUBSIDY_FLAG END) AS SUBSIDY_FLAG,
				lc.HOLDING_COMPANY,
				(CASE WHEN lc.CAUTION_LIST is NULL THEN 'No' ELSE lc.CAUTION_LIST END) AS CAUTION_LIST,
				lc.DATE_CAUTION_LIST,
				lc.COMPANY,
				lc.DIRECTORS,
				lc.GROUP_COMPANIES,
				(CASE WHEN lc.DEFAULTERS_LIST is NULL THEN 'No' ELSE lc.DEFAULTERS_LIST END) AS DEFAULTERS_LIST,
				lc.RBI_DATE_CAUTION_LIST,
				lc.RBI_COMPANY,
				lc.RBI_DIRECTORS,
				lc.RBI_GROUP_COMPANIES,
				(CASE WHEN lc.COMMERICIAL_REAL_EASTATE is NULL THEN 'No' ELSE lc.COMMERICIAL_REAL_EASTATE END) AS COMMERICIAL_REAL_EASTATE,
				(select entry_name from common_code_category_entry where category_code='COMMERCIAL_REAL_ESTATE' and entry_code=lc.COMMERICIAL_REAL_EASTATE_VALUE) as COMMERICIAL_REAL_EASTATE_VALUE,
				(CASE WHEN lc.COMM_REAL_EASTATE_HOUSING is NULL THEN 'No' ELSE lc.COMM_REAL_EASTATE_HOUSING END) AS COMM_REAL_EASTATE_HOUSING,
				(CASE WHEN lc.RESIDENTIAL_REAL_ESTATE is NULL THEN 'No' ELSE lc.RESIDENTIAL_REAL_ESTATE END) AS RESIDENTIAL_REAL_ESTATE,
				(CASE WHEN lc.INDIRECT_REAL_ESTATE is NULL THEN 'No' ELSE lc.INDIRECT_REAL_ESTATE END) AS INDIRECT_REAL_ESTATE,
				DECODE(lc.BROKER_TYPE_SHARE,'Y','Share Borrower ') || DECODE(lc.BROKER_TYPE_COMMODITY,'Y','Commodity Borrower')  as BROKER_TYPE,
			  cc_segment.entry_name AS segment,
			  (SELECT region_name FROM cms_region WHERE ID = addr.lra_region
			  ) AS region,
			  (SELECT rm.rm_mgr_name
			  FROM cms_relationship_mgr rm
			  WHERE rm.id = sp.relation_mgr
			  ) AS RM,
			  (SELECT pg.party_group_name
			  FROM cms_party_group pg
			  WHERE pg.id= sp.party_grp_nm
			  )                     AS PartyGroup,
			  lc.is_project_finance AS cff_project_finance,
			  cam.rbi_asset_classification,
			  sp.LEI_CODE,
  			  to_char(sp.LEI_EXPIRY_DATE,'DD-Mon-YYYY') as LEI_EXPIRY_DATE,
  			  lc.CUSTOMER_FY_CLOUSER
			FROM SCI_LE_CRI lc,
			  (SELECT entry_name,
			    entry_code
			  FROM common_code_category_entry
			  WHERE category_code = 'HDFC_RBI_CODE'
			  ) cc_rbi,
			  (SELECT entry_name,
			    entry_code
			  FROM common_code_category_entry
			  WHERE category_code = 'HDFC_INDUSTRY'
			  ) cc_industry,
			  CMS_PARTY_GROUP pg,
			  SCI_LE_SUB_PROFILE sp,
			  common_code_category_entry cc_type,
			  common_code_category_entry cc_msme,
			  common_code_category_entry cc_constit,
			  common_code_category_entry cc_nbfc_a,
			  common_code_category_entry cc_nbfc_b,
			  common_code_category_entry cc_bank_method,
			  common_code_category_entry cc_commodity,
			  common_code_category_entry cc_infra_fin,
			  (SELECT * FROM SCI_LE_CRI_FAC WHERE facility_for = 'Kisan Credit Card'
			  ) cff_kcc,
			  (SELECT * FROM SCI_LE_CRI_FAC WHERE facility_for = 'Backed by Govt.'
			  ) cff_bgovt,
			  (SELECT * FROM SCI_LE_CRI_FAC WHERE facility_for = 'Infrastructure Finance'
			  ) cff_InfrFin,
			  (SELECT * FROM SCI_LE_CRI_FAC WHERE facility_for = 'Project Finance'
			  ) cff_project_finance,
			  (SELECT entry_name,
			    entry_code
			  FROM common_code_category_entry
			  WHERE category_code = 'HDFC_SEGMENT'
			  ) cc_segment,
			  SCI_LE_REG_ADDR addr,
			  SCI_LSP_LMT_PROFILE cam
			</query>
		<whereClause>
			<mandatoryClause>
				sp.CMS_LE_MAIN_PROFILE_ID =                       lc.CMS_LE_MAIN_PROFILE_ID(+)
				AND sp.PARTY_GRP_NM                               = pg.ID(+)
				AND lc.WEAKER_SECTION                             = cc_type.entry_code(+)
				AND lc.MSME_CLASSIFICATION                        = cc_msme.entry_code(+)
				AND sp.ENTITY                                     = cc_constit.entry_code(+)
				AND lc.NBFS_A                                     = cc_nbfc_a.entry_code(+)
				AND lc.NBFS_B                                     = cc_nbfc_b.entry_code(+)
				AND sp.BANKING_METHOD                             = cc_bank_method.entry_code(+)
				AND cc_rbi.entry_code(+)                          =sp.rbi_ind_code
				AND cc_industry.entry_code(+)                     =sp.ind_nm
				AND lc.COMMODITIES_NAME                           = cc_commodity.entry_code(+)
				AND lc.INFRASTRUCTURE_FINANACE_TYPE               = cc_infra_fin.entry_code(+)
				AND (cc_type.category_code                        = 'WEAKER_SEC'
				OR cc_type.category_code                         IS NULL)
				AND (cc_msme.category_code                        = 'MSME_CLASSIC'
				OR cc_msme.category_code                         IS NULL)
				AND (cc_constit.category_code                     = 'Entity'
				OR cc_constit.category_code                      IS NULL)
				AND (cc_nbfc_a.category_code                      = 'NBFC_A'
				OR cc_nbfc_a.category_code                       IS NULL)
				AND (cc_nbfc_b.category_code                      = 'NBFC_B'
				OR cc_nbfc_b.category_code                       IS NULL)
				AND (cc_bank_method.category_code                 = 'BANKING_METHOD'
				OR cc_bank_method.category_code                  IS NULL)
				AND (cc_commodity.category_code                   = 'COMMODITY_NAMES'
				OR cc_commodity.category_code                    IS NULL)
				AND (cc_infra_fin.category_code                   = 'INFRA_FINANACE_TYPE'
				OR cc_infra_fin.category_code                    IS NULL)
				AND cff_kcc.cms_le_main_profile_id(+)             =sp.CMS_LE_MAIN_PROFILE_ID
				AND cff_bgovt.cms_le_main_profile_id(+)           =sp.CMS_LE_MAIN_PROFILE_ID
				AND cff_InfrFin.cms_le_main_profile_id(+)         =sp.CMS_LE_MAIN_PROFILE_ID
				AND cff_project_finance.cms_le_main_profile_id(+) =sp.CMS_LE_MAIN_PROFILE_ID
				AND sp.cms_le_sub_profile_id                      = cam.cms_customer_id(+)
				AND cc_segment.entry_code(+)                      = sp.lsp_sgmnt_code_value
				AND sp.CMS_LE_MAIN_PROFILE_ID                     = addr.CMS_LE_MAIN_PROFILE_ID(+)
				and (addr.lra_type_value                           = 'CORPORATE'
				OR addr.lra_type_value                    IS NULL)
				AND sp.status != 'INACTIVE'
		 
            </mandatoryClause>			
			<param>
				<name>segment</name>
				<condition> AND sp.lsp_sgmnt_code_value = </condition>
			</param>
			<param>
				<name>party</name>
				<condition> AND sp.cms_le_sub_profile_id = </condition>
			</param>					
		</whereClause>
