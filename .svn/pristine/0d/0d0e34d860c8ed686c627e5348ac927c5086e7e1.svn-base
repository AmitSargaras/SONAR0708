<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">


  <bean name="/MaintainAssetLife" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.systemparameters.assetlife.MaintainAssetLifeAction">
    <property name="nameCommandMap">
      <map>
        <entry key="AssetLifeMakerReadCmd" value-ref="MaintainAssetLifeAction.AssetLifeMakerReadCmd"/>
        <entry key="AssetLifeMakerReadRejectedCmd" value-ref="MaintainAssetLifeAction.AssetLifeMakerReadRejectedCmd"/>
        <entry key="AssetLifeCheckerRejectEditCmd" value-ref="MaintainAssetLifeAction.AssetLifeCheckerRejectEditCmd"/>
        <entry key="AssetLifeMakerEditCmd" value-ref="MaintainAssetLifeAction.AssetLifeMakerEditCmd"/>
        <entry key="AssetLifeMakerCancelEditCmd" value-ref="MaintainAssetLifeAction.AssetLifeMakerCancelEditCmd"/>
        <entry key="AssetLifeCheckerReadCmd" value-ref="MaintainAssetLifeAction.AssetLifeCheckerReadCmd"/>
        <entry key="AssetLifeCheckerApproveEditCmd" value-ref="MaintainAssetLifeAction.AssetLifeCheckerApproveEditCmd"/>
        <entry key="ReloadAssetValuationProfilesCmd" value-ref="CollateralAction.ReloadAssetValuationProfileSingletonCommand"/>
      </map>
    </property>
  </bean>
  
</beans>