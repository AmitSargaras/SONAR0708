<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <description>Collateral Mapping - CO007 - Others</description>
    
    <class name="com.integrosys.cms.host.eai.security.bus.others.OthersSecurity">
        <map-to xml="OtherDetail"/>
        <field name="LOSSecurityId"                              type="string">
			<bind-xml name="LOSSecurityId" />
		</field>
        <field name="AssetDescription"                           type="string" >
        	<bind-xml name ="Description"/>
        </field>
        <field name="PhysicalInspection"                         type="string">
			<bind-xml name="PhysicalInspection" />
		</field>
        <field name="PhysicalInspectionFrequencyUnit"            type="int">
			<bind-xml name="PhysicalInspectionFrequencyUnit" />
		</field>
        <field name="PhysicalInspectionFrequencyUOM"             type="string">
			<bind-xml name="PhysicalInspectionFrequencyUOM" />
		</field>
        <field name="LastPhysicalInspectionDate"                 type="string">
			<bind-xml name="LastPhysicalInspectionDate" />
		</field>
        <field name="NextPhysicalInspectionDate"                 type="string">
			<bind-xml name="NextPhysicalInspectionDate" />
		</field>
        <field name="EnvironmentallyRiskyStatus"                 type="string">
			<bind-xml name="EnvironmentallyRiskyStatus" />
		</field>
        <field name="EnvironmentallyRiskyConfirmedDate"          type="string">
			<bind-xml name="EnvironmentallyRiskyConfirmedDate" />
		</field>
        <field name="EnvironmentallyRiskyRemarks"                type="string" >
        	<bind-xml name="EnvironmentallyRiskyRemark" />
        </field>	
        <field name="NumberOfUnits"                				 type="double">
			<bind-xml name="NumberOfUnits" />
		</field>
        <field name="ChangeIndicator"                            type="string">
			<bind-xml name="ChangeIndicator" />
		</field>
        <field name="UpdateStatusIndicator"                      type="string">
			<bind-xml name="UpdateStatusIndicator" />
		</field>
    </class>
    
</mapping>