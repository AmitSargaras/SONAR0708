<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.integrosys.cms.host.eai.security.bus.others.OthersSecurity" table="CMS_OTHERS" polymorphism="explicit">
    <id name="CMSSecurityId" column="CMS_COLLATERAL_ID" type="long" />

    <property name="assetDescription" column="description_others" />
    <property name="PhysicalInspection" column="IS_PHY_INSPECT" />
    <property name="PhysicalInspectionFrequencyUnit" column="PHY_INSPECT_FREQ" />
    <property name="PhysicalInspectionFrequencyUOM" column="PHY_INSPECT_FREQ_UNIT" />
    <property name="JDOLastPhysicalInspectionDate" column="LAST_PHY_INSPECT_DATE" type="timestamp" />
    <property name="JDONextPhysicalInspectionDate" column="NEXT_PHY_INSPECT_DATE" type="timestamp" />
    <property name="EnvironmentallyRiskyStatus" column="ENV_RISKY_STATUS" />
    <property name="JDOEnvironmentallyRiskyConfirmedDate" column="ENV_RISKY_DATE" type="timestamp" />
    <property name="EnvironmentallyRiskyRemarks" column="ENV_RISKY_REMARKS" />
    <property name="numberOfUnits" column="NUMBER_OF_UNITS"  />
  </class>

  <class name="com.integrosys.cms.host.eai.security.bus.others.StageOthersSecurity" table="CMS_STAGE_OTHERS" polymorphism="explicit">
    <id name="CMSSecurityId" column="CMS_COLLATERAL_ID" type="long" />

    <property name="assetDescription" column="description_others" />
    <property name="PhysicalInspection" column="IS_PHY_INSPECT" />
    <property name="PhysicalInspectionFrequencyUnit" column="PHY_INSPECT_FREQ" />
    <property name="PhysicalInspectionFrequencyUOM" column="PHY_INSPECT_FREQ_UNIT" />
    <property name="JDOLastPhysicalInspectionDate" column="LAST_PHY_INSPECT_DATE" type="timestamp" />
    <property name="JDONextPhysicalInspectionDate" column="NEXT_PHY_INSPECT_DATE" type="timestamp" />
    <property name="EnvironmentallyRiskyStatus" column="ENV_RISKY_STATUS" />
    <property name="JDOEnvironmentallyRiskyConfirmedDate" column="ENV_RISKY_DATE" type="timestamp" />
    <property name="EnvironmentallyRiskyRemarks" column="ENV_RISKY_REMARKS" />
    <property name="numberOfUnits" column="NUMBER_OF_UNITS" />
  </class>
</hibernate-mapping>