<?xml version="1.0" encoding="UTF-8"?>
<reports>
	<report>
		<query>
			SELECT 'GENCOV'           AS "PARTY_ID",
				   'GENERAL COVENANT' AS "PARTY_NAME",
				   'CAM123'           AS "CAM_NUMBER",
				   'General Covenant' AS "TYPE_OF_GEN_COVENANT",
				   'Condition 1'      AS "COVENANT_COND",
				   'Quarterly'        AS "FREQUENCY",
				   '12/09/2021'       AS "DUE_DATE",
				   'Comments'         AS "COMMENT",
				   'Yes'              AS "IS_RECEIVED_FRM_CAM"
		      FROM DUAL D
			</query>
		<whereClause>
			<mandatoryClause>				
			</mandatoryClause>
			<param>
				<name>party</name>
				<condition></condition>
			</param>			
		</whereClause>
		<reportParamters>
			<reportColumns>
			    <reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>Party ID</header><!-- 1 -->
				</reportColumn>
				<reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>Party Name</header><!-- 2 -->
				</reportColumn>
				<reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>CAM Number</header><!-- 3 -->
				</reportColumn>
				<reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>Type of General Covenants</header><!-- 4 -->
				</reportColumn>
				<reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>Covenant Condition</header><!-- 5 -->
				</reportColumn>
				<reportColumn>
					<width>50</width>
					<format>textFormat</format>
					<header>Frequency</header><!-- 6 -->
				</reportColumn>
				<reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>Due Date</header><!-- 7 -->
				</reportColumn>
				<reportColumn>
					<width>50</width>
					<format>textFormat</format>
					<header>Comment</header><!-- 8 -->
				</reportColumn>				
				<reportColumn>
					<width>50</width>
