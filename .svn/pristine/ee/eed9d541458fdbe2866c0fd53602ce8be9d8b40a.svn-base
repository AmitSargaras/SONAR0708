<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

    <description>
        To be used for transaction controller and transaction operation.
        Transaction operation will have the property to set bus manager, which get from 'AppContext_Services.xml'
    </description>

    <bean id="RbiCategoryTrx.ReadRbiCategoryOperation" class="com.integrosys.cms.app.rbicategory.trx.ReadRbiCategoryOperation">
        <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
    </bean>
    
    <bean id="RbiCategoryTrx.ReadRbiCategoryIDOperation" class="com.integrosys.cms.app.rbicategory.trx.ReadRbiCategoryIDOperation">
        <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
    </bean>
	<bean id="RbiCategoryTrx.MakerUpdateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerUpdateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	
	
	<bean id="RbiCategoryTrx.MakerCreateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerCreateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerDeleteRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerDeleteRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerEditRejectedUpdateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerEditRejectedUpdateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerEditRejectedCreateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerEditRejectedCreateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerEditRejectedDeleteRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerEditRejectedDeleteRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerCloseRejectedUpdateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerCloseRejectedUpdateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerCloseRejectedDeleteRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerCloseRejectedDeleteRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.MakerCloseRejectedCreateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.MakerCloseRejectedCreateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.CheckerApproveUpdateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerApproveUpdateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
		<bean id="RbiCategoryTrx.CheckerApproveDeleteRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerApproveDeleteRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.CheckerApproveCreateRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerApproveCreateRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.CheckerRejectRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerRejectRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.CheckerCreateRejectRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerCreateRejectRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.CheckerEditRejectRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerEditRejectRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>
	<bean id="RbiCategoryTrx.CheckerDeleteRejectRbiCategoryOperation"
		class="com.integrosys.cms.app.rbicategory.trx.CheckerDeleteRejectRbiCategoryOperation">
		 <property name="rbiCategoryBusManager" ref="rbiCategoryBusManager"/>
        <property name="stagingRbiCategoryBusManager" ref="stagingRbiCategoryBusManager"/>
	</bean>

    <bean id="rbiCategoryReadController" class="com.integrosys.cms.app.rbicategory.trx.RbiCategoryReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadRbiCategoryOperation" value-ref="RbiCategoryTrx.ReadRbiCategoryOperation"/>
                 <entry key="ReadRbiCategoryIDOperation" value-ref="RbiCategoryTrx.ReadRbiCategoryIDOperation"/>
                
                
            </map>
        </property>
    </bean>

    <bean id="rbiCategoryTrxController" class="com.integrosys.cms.app.rbicategory.trx.RbiCategoryTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerEditRejectedUpdateRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerEditRejectedUpdateRbiCategoryOperation"/>
            	   <entry key="MakerEditRejectedCreateRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerEditRejectedCreateRbiCategoryOperation"/>
            	  <entry key="MakerEditRejectedDeleteRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerEditRejectedDeleteRbiCategoryOperation"/>
                  <entry key="MakerUpdateRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerUpdateRbiCategoryOperation"/>
                  <entry key="MakerCreateRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerCreateRbiCategoryOperation"/>
                   <entry key="MakerDeleteRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerDeleteRbiCategoryOperation"/>
                  <entry key="MakerCloseRejectedUpdateRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerCloseRejectedUpdateRbiCategoryOperation"/>
                  <entry key="MakerCloseRejectedCreateRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerCloseRejectedCreateRbiCategoryOperation"/>
                  <entry key="MakerCloseRejectedDeleteRbiCategoryOperation" value-ref="RbiCategoryTrx.MakerCloseRejectedDeleteRbiCategoryOperation"/>
                  <entry key="CheckerApproveUpdateRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerApproveUpdateRbiCategoryOperation"/>
                  <entry key="CheckerApproveDeleteRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerApproveDeleteRbiCategoryOperation"/>
                  <entry key="CheckerApproveCreateRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerApproveCreateRbiCategoryOperation"/>
                  <entry key="CheckerRejectRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerRejectRbiCategoryOperation"/>
                  <entry key="CheckerCreateRejectRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerCreateRejectRbiCategoryOperation"/>
                  <entry key="CheckerEditRejectRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerEditRejectRbiCategoryOperation"/>
                  <entry key="CheckerDeleteRejectRbiCategoryOperation" value-ref="RbiCategoryTrx.CheckerDeleteRejectRbiCategoryOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

    <bean id="rbiCategoryTrxControllerFactory" class="com.integrosys.cms.app.rbicategory.trx.RbiCategoryTrxControllerFactory">
        <property name="rbiCategoryReadController" ref="rbiCategoryReadController"/>
        <property name="rbiCategoryTrxController" ref="rbiCategoryTrxController"/>
    </bean>
</beans>