<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
   <display-name>InterestRate</display-name>
   <enterprise-beans>
      <session>
         <ejb-name>SBInterestRateProxy</ejb-name>
         <home>com.integrosys.cms.app.interestrate.proxy.SBInterestRateProxyHome</home>
         <remote>com.integrosys.cms.app.interestrate.proxy.SBInterestRateProxy</remote>
         <ejb-class>com.integrosys.cms.app.interestrate.proxy.SBInterestRateProxyBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
         <resource-ref>
            <res-ref-name>jdbc/CMSPool</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
         </resource-ref>
      </session>
      <session>
         <ejb-name>SBInterestRateBusManager</ejb-name>
         <home>com.integrosys.cms.app.interestrate.bus.SBInterestRateBusManagerHome</home>
         <remote>com.integrosys.cms.app.interestrate.bus.SBInterestRateBusManager</remote>
         <ejb-class>com.integrosys.cms.app.interestrate.bus.SBInterestRateBusManagerBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
         <resource-ref>
            <res-ref-name>jdbc/CMSPool</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
         </resource-ref>
      </session>
      <session>
         <ejb-name>SBInterestRateBusManagerStaging</ejb-name>
         <home>com.integrosys.cms.app.interestrate.bus.SBInterestRateBusManagerHome</home>
         <remote>com.integrosys.cms.app.interestrate.bus.SBInterestRateBusManager</remote>
         <ejb-class>com.integrosys.cms.app.interestrate.bus.SBInterestRateBusManagerStagingBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
         <resource-ref>
            <res-ref-name>jdbc/CMSPool</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
         </resource-ref>
      </session>
      
      <entity>
         <ejb-name>EBInterestRate</ejb-name>
         <home>com.integrosys.cms.app.interestrate.bus.EBInterestRateHome</home>
         <remote>com.integrosys.cms.app.interestrate.bus.EBInterestRate</remote>
         <local-home>com.integrosys.cms.app.interestrate.bus.EBInterestRateLocalHome</local-home>
         <local>com.integrosys.cms.app.interestrate.bus.EBInterestRateLocal</local>
         <ejb-class>com.integrosys.cms.app.interestrate.bus.EBInterestRateBean</ejb-class>
         <persistence-type>Container</persistence-type>
         <prim-key-class>java.lang.Long</prim-key-class>
         <reentrant>False</reentrant>
         <cmp-version>2.x</cmp-version>
         <abstract-schema-name>EBInterestRate</abstract-schema-name>
         <cmp-field>
            <field-name>intRateID</field-name>
         </cmp-field>                  
         <cmp-field>
            <field-name>intRateType</field-name>
         </cmp-field>
         <cmp-field>
            <field-name>intRateDate</field-name>
         </cmp-field>
	  <cmp-field>
	     <field-name>intRatePercent</field-name>
	  </cmp-field>
         <cmp-field>
            <field-name>groupID</field-name>
         </cmp-field>
         <cmp-field>
            <field-name>versionTime</field-name>
         </cmp-field>
         <primkey-field>intRateID</primkey-field>
         <resource-ref>
            <res-ref-name>jdbc/CMSPool</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
         </resource-ref>
         <query>
            <query-method>
               <method-name>findAll</method-name>
               <method-params/>
            </query-method>
            <ejb-ql>SELECT OBJECT(o) FROM EBInterestRate o</ejb-ql>
         </query>
         <query>
            <query-method>
               <method-name>findByGroupID</method-name>
               <method-params>
                  <method-param>long</method-param>
               </method-params>
            </query-method>
            <ejb-ql>SELECT OBJECT(o) FROM EBInterestRate o WHERE o.groupID = ?1 ORDERBY o.intRateID</ejb-ql>
         </query>
      </entity>
      <entity>
         <ejb-name>EBInterestRateStaging</ejb-name>
         <home>com.integrosys.cms.app.interestrate.bus.EBInterestRateHome</home>
         <remote>com.integrosys.cms.app.interestrate.bus.EBInterestRate</remote>
         <local-home>com.integrosys.cms.app.interestrate.bus.EBInterestRateLocalHome</local-home>
         <local>com.integrosys.cms.app.interestrate.bus.EBInterestRateLocal</local>
         <ejb-class>com.integrosys.cms.app.interestrate.bus.EBInterestRateStagingBean</ejb-class>
         <persistence-type>Container</persistence-type>
         <prim-key-class>java.lang.Long</prim-key-class>
         <reentrant>False</reentrant>
         <cmp-version>2.x</cmp-version>
         <abstract-schema-name>EBInterestRateStaging</abstract-schema-name>
         <cmp-field>
            <field-name>intRateID</field-name>
         </cmp-field>         
         <cmp-field>
            <field-name>intRateType</field-name>
         </cmp-field>
         <cmp-field>
            <field-name>intRateDate</field-name>
         </cmp-field>
	  <cmp-field>
	     <field-name>intRatePercent</field-name>
	  </cmp-field>
         <cmp-field>
            <field-name>groupID</field-name>
         </cmp-field>        
         <cmp-field>
            <field-name>versionTime</field-name>
         </cmp-field>         
         <primkey-field>intRateID</primkey-field>
         <resource-ref>
            <res-ref-name>jdbc/CMSPool</res-ref-name>
            <res-type>javax.sql.DataSource</res-type>
            <res-auth>Container</res-auth>
            <res-sharing-scope>Shareable</res-sharing-scope>
         </resource-ref>
         <query>
            <query-method>
               <method-name>findAll</method-name>
               <method-params/>
            </query-method>
            <ejb-ql>SELECT OBJECT(o) FROM EBInterestRateStaging o</ejb-ql>
         </query>
         <query>
            <query-method>
               <method-name>findByGroupID</method-name>
               <method-params>
                  <method-param>long</method-param>
               </method-params>
            </query-method>
            <ejb-ql>SELECT OBJECT(o) FROM EBInterestRateStaging o WHERE o.groupID = ?1 ORDERBY o.intRateID</ejb-ql>
         </query>
      </entity>



   </enterprise-beans>
   

   <assembly-descriptor>
      <container-transaction>       
         
         <method>
            <ejb-name>SBInterestRateProxy</ejb-name>
            <method-name>*</method-name>
         </method>
         <method>
            <ejb-name>SBInterestRateBusManager</ejb-name>
            <method-name>*</method-name>
         </method>
         <method>
            <ejb-name>SBInterestRateBusManagerStaging</ejb-name>
            <method-name>*</method-name>
         </method>
         <method>
            <ejb-name>EBInterestRate</ejb-name>
            <method-name>*</method-name>
         </method>


         <trans-attribute>Required</trans-attribute>
      </container-transaction>
   </assembly-descriptor>
</ejb-jar>
