<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.feed.bus.unittrust">

  <class entity-name="stageUnitTrustFeedGroup" name="OBUnitTrustFeedGroup" table="CMS_STAGE_FEED_GROUP">
	<id name="unitTrustFeedGroupID" type="long" column="FEED_GROUP_ID" >
		<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
			<param name="sequence">CMS_FEED_GROUP_SEQ</param>
		</generator>
	</id>
	<version name="versionTime" column="CMS_VERSION_TIME" type="long" />

    <property name="type" column="GROUP_TYPE" />
    <property name="subType" column="GROUP_SUBTYPE" />
    <property name="stockType" column="GROUP_STOCK_TYPE" />
    
	<set name="feedEntriesSet" cascade="save-update, delete-orphan" fetch="select" lazy="false">
		<key column="FEED_GROUP_ID" not-null="true" />
		<one-to-many entity-name="stageUnitTrustFeedEntry" />
	</set>
  </class>

  <class entity-name="stageUnitTrustFeedEntry" name="OBUnitTrustFeedEntry" table="CMS_STAGE_PRICE_FEED" lazy="false">
	<id name="unitTrustFeedEntryID" type="long" column="FEED_ID" >
		<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
			<param name="sequence">CMS_PRICE_FEED_SEQ</param>
		</generator>
	</id>
	<version name="versionTime" column="CMS_VERSION_TIME" type="long" />

    <property name="countryCode" column="COUNTRY" />
    <property name="currencyCode" column="CURRENCY" />
    <property name="exchange" column="EXCHANGE" />
    <property name="fundManagerName" column="FUND_MANAGER_NAME" />
    <property name="isinCode" column="ISIN_CODE" />
    <property name="lastUpdatedDate" column="LAST_UPDATED_DATE" type="timestamp" />
    <property name="name" column="NAME" />
    <property name="offer" column="OFFER" type="double" />
    <property name="ric" column="RIC" />
    <property name="rating" column="RATING" />
    <property name="type" column="TYPE" />
    <property name="unitPrice" column="UNIT_PRICE" type="double" />
    <property name="fundManagerCode" column="FUND_MANAGER_CODE" />
    <property name="productCode" column="PRODUCT_CODE" />
    <property name="unitTrustFeedEntryRef" column="FEED_REF" type="long" />
    <property name="fundCode" column="FUND_CODE" />
    <property name="fundSize" column="FUND_SIZE" type="long"/>
    <property name="fundSizeUpdateDate" column="FUND_SIZE_UPDATE_DATE" type="timestamp"/>
    <property name="dateLaunched" column="DATE_LAUNCHED" type="timestamp"/>
  </class>
  
  <class entity-name="actualUnitTrustFeedGroup" name="OBUnitTrustFeedGroup" table="CMS_FEED_GROUP">
	<id name="unitTrustFeedGroupID" type="long" column="FEED_GROUP_ID" >
		<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
			<param name="sequence">CMS_FEED_GROUP_SEQ</param>
		</generator>
	</id>
	<version name="versionTime" column="CMS_VERSION_TIME" type="long" />

    <property name="type" column="GROUP_TYPE" />
    <property name="subType" column="GROUP_SUBTYPE" />
    <property name="stockType" column="GROUP_STOCK_TYPE" />
    
	<set name="feedEntriesSet" cascade="save-update, delete-orphan" fetch="select" lazy="false">
		<key column="FEED_GROUP_ID" not-null="true" />
		<one-to-many entity-name="actualUnitTrustFeedEntry" />
	</set>
  </class>

  <class entity-name="actualUnitTrustFeedEntry" name="OBUnitTrustFeedEntry" table="CMS_PRICE_FEED" lazy="false">
	<id name="unitTrustFeedEntryID" type="long" column="FEED_ID" >
		<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
			<param name="sequence">CMS_PRICE_FEED_SEQ</param>
		</generator>
	</id>	
	<version name="versionTime" column="CMS_VERSION_TIME" type="long" />
  
    <property name="countryCode" column="COUNTRY" />
    <property name="currencyCode" column="CURRENCY" />
    <property name="exchange" column="EXCHANGE" />
    <property name="fundManagerName" column="FUND_MANAGER_NAME" />
    <property name="isinCode" column="ISIN_CODE" />
    <property name="lastUpdatedDate" column="LAST_UPDATED_DATE" type="timestamp" />
    <property name="name" column="NAME" />
    <property name="offer" column="OFFER" type="double" />
    <property name="ric" column="RIC" />
    <property name="rating" column="RATING" />
    <property name="type" column="TYPE" />
    <property name="unitPrice" column="UNIT_PRICE" type="double" />
    <property name="fundManagerCode" column="FUND_MANAGER_CODE" />
    <property name="productCode" column="PRODUCT_CODE" />
    <property name="unitTrustFeedEntryRef" column="FEED_REF" type="long" />
    <property name="fundCode" column="FUND_CODE" />
    <property name="fundSize" column="FUND_SIZE" type="long"/>
    <property name="fundSizeUpdateDate" column="FUND_SIZE_UPDATE_DATE" type="timestamp"/>
    <property name="dateLaunched" column="DATE_LAUNCHED" type="timestamp"/>
  </class>

</hibernate-mapping>