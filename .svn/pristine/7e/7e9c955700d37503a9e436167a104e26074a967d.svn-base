	</bean>
	
    <bean id="relationshipMgrReadController" class="com.integrosys.cms.app.relationshipmgr.trx.RelationshipMgrReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadRelationShipMgrOperation" value-ref="RelationshipManagerTrx.ReadRelationshipManagerOperation"/>
                 <entry key="ReadRelationShipMgrIDOperation" value-ref="RelationshipManagerTrx.ReadRelationshipManagerIDOperation"/>
            </map>
        </property>
    </bean>

    <bean id="relationshipMgrTrxController" class="com.integrosys.cms.app.relationshipmgr.trx.RelationshipMgrTrxController">
        <property name="nameTrxOperationMap">
            <map>
            	  <entry key="MakerCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerCreateRelationShipMgrOperation"/>
            	  
            	  <entry key="MakerSaveCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerSaveCreateRelationshipMgrOperation"/>
            	  <entry key="MakerSubmitSaveCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerSubmitSaveCreateRelationshipMgrOperation"/>
            	  <entry key="MakerCloseDraftCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerCloseDraftCreateRelationshipMgrOperation"/>
            	  <entry key="MakerSubmitSaveUpdateRelationshipMgrOperation" value-ref="RelationshipManagerTrx.MakerSubmitSaveUpdateRelationshipMgrOperation"/>
            	  <entry key="MakerUpdateSaveRelationshipMgrOperation" value-ref="RelationshipManagerTrx.MakerUpdateSaveRelationshipMgrOperation"/>
            	  	
				  <entry key="MakerUpdateRelationshipMgrOperation" value-ref="RelationshipManagerTrx.MakerUpdateRelationshipManagerOperation"/>
				  <entry key="MakerDeleteRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerDeleteRelationShipMgrOperation"/>
            	  <entry key="MakerEditRejectedCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerEditRejectedCreateRelationShipMgrOperation"/>
            	  <entry key="MakerEditRejectedUpdateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerEditRejectedUpdateRelationShipMgrOperation"/>
            	  <entry key="MakerEditRejectedDeleteRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerEditRejectedDeleteRelationShipMgrOperation"/>
            	  <entry key="MakerCloseRejectedCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerCloseRejectedCreateRelationShipMgrOperation"/>
                  <entry key="MakerCloseRejectedUpdateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerCloseRejectedUpdateRelationShipMgrOperation"/>
                  <entry key="MakerCloseRejectedDeleteRelationshipMgrOperation" value-ref="RelationShipMgrTrx.MakerCloseRejectedDeleteRelationShipMgrOperation"/>

                  <entry key="CheckerRejectRelationshipMgrOperation" value-ref="RelationShipMgrTrx.CheckerRejectRelationShipMgrOperation"/>
                  <entry key="CheckerApproveUpdateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.CheckerApproveUpdateRelationShipMgrOperation"/>
                  <entry key="CheckerApproveCreateRelationshipMgrOperation" value-ref="RelationShipMgrTrx.CheckerApproveCreateRelationShipMgrOperation"/>
                  <entry key="CheckerApproveDeleteRelationshipMgrOperation" value-ref="RelationShipMgrTrx.CheckerApproveDeleteRelationShipMgrOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
    
    
