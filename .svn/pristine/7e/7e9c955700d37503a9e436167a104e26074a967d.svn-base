		class="com.integrosys.cms.app.relationshipmgr.trx.MakerRelationshipMgrInsertFileOperation">
		 <property name="relationshipMgrFileMapperIdBusManager" ref="relationshipMgrFileMapperIdBusManager"/>
        <property name="stagingRelationshipMgrFileMapperIdBusManager" ref="stagingRelationshipMgrFileMapperIdBusManager"/>
	</bean>
	<bean id="RelationshipMgrTrx.CheckerApproveInsertMasterOperation"
