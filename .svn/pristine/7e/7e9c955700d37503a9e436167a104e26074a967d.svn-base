	</bean>
    
    <bean id="relationshipMgrInsertFileTrxController" class="com.integrosys.cms.app.relationshipmgr.trx.InsertFileTrxController">
        <property name="nameTrxOperationMap">
            <map>        	 
                  <entry key="MakerRelationshipMgrInsertFileOperation" value-ref="RelationshipMgrTrx.MakerRelationshipMgrInsertFileOperation" />
                  <entry key="CheckerApproveInsertMasterOperation" value-ref="RelationshipMgrTrx.CheckerApproveInsertMasterOperation" />
                  <entry key="CheckerRejectInsertMasterOperation" value-ref="RelationshipMgrTrx.CheckerRejectInsertMasterOperation" />
                  <entry key="MakerCloseRejectedInsertMasterOperation" value-ref="RelationshipMgrTrx.MakerCloseRejectedInsertMasterOperation"/>
                   <entry key="CheckerCreateRelationshipMgrOperation" value-ref="RelationshipMgrTrx.CheckerCreateRelationshipMgrOperation"/>
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>
    
    <bean id="readRelationshipMgrInsertFileTrxController" class="com.integrosys.cms.app.relationshipmgr.trx.ReadInsertFileTrxController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="FileInsertReadOperation" value-ref="RelationshipMgrTrx.FileInsertReadOperation"/>                   
            </map>
        </property>
    </bean>

    <bean id="relationshipMgrTrxControllerFactory" class="com.integrosys.cms.app.relationshipmgr.trx.RelationshipMgrTrxControllerFactory">
        <property name="relationshipMgrReadController" ref="relationshipMgrReadController"/>
        <property name="relationshipMgrTrxController" ref="relationshipMgrTrxController"/>
        <property name="relationshipMgrInsertFileTrxController" ref="relationshipMgrInsertFileTrxController"/>
        <property name="readRelationshipMgrInsertFileTrxController" ref="readRelationshipMgrInsertFileTrxController"/>
    </bean>
</beans>