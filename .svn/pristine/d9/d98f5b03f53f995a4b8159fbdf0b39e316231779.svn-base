<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
	<description>
		All the Hibernate and Jdbc DAOs go here.
	</description>

	<bean id="limitJdbc" class="com.integrosys.cms.host.eai.limit.bus.LimitJdbcImpl">
		<property name="dataSource" ref="ofaPool" />
	</bean>
	
	<bean id="restCamInterfaceLogDAO" class="com.aurionpro.clims.rest.interfaceLog.RestCamInterfaceLogDAOImpl">
		<property name="sessionFactory" ref="ofaSessionFactory" />
	</bean>
	
</beans>