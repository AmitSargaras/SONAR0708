    <bean id="productMasterReadController" class="com.integrosys.cms.app.productMaster.trx.ProductMasterReadController">
        <property name="nameTrxOperationMap">
            <map>
                <entry key="ReadProductMasterOperation" value-ref="ProductMasterTrx.ReadProductMasterOperation"/>
                 <entry key="ReadProductMasterIDOperation" value-ref="ProductMasterTrx.ReadProductMasterIDOperation"/>  
            </map>
        </property>
    </bean> 
    <bean id="productMasterTrxController" class="com.integrosys.cms.app.productMaster.trx.ProductMasterTrxController">
         <property name="nameTrxOperationMap">
        <map>
            <entry key="MakerCreateProductMasterOperation" value-ref="ProductMasterTrx.MakerCreateProductMasterOperation"/>
            <entry key="CheckerApproveCreateProductMasterOperation" value-ref="ProductMasterTrx.CheckerApproveCreateProductMasterOperation"/>
           
            <!--added by santosh-UBS CR  -->
            <entry key="MakerUpdateProductMasterOperation" value-ref="ProductMasterTrx.MakerUpdateProductMasterOperation"/>
            <entry key="CheckerApproveUpdateProductMasterOperation" value-ref="ProductMasterTrx.CheckerApproveUpdateProductMasterOperation"/>
            <entry key="CheckerRejectProductMasterOperation" value-ref="ProductMasterTrx.CheckerRejectProductMasterOperation"/>
            <entry key="MakerEditRejectedCreateProductMasterOperation" value-ref="ProductMasterTrx.MakerEditRejectedCreateProductMasterOperation"/>
            <entry key="MakerEditRejectedUpdateProductMasterOperation" value-ref="ProductMasterTrx.MakerEditRejectedUpdateProductMasterOperation"/>
            <entry key="MakerCloseRejectedCreateProductMasterOperation" value-ref="ProductMasterTrx.MakerCloseRejectedCreateProductMasterOperation"/>
            <entry key="MakerCloseRejectedUpdateProductMasterOperation" value-ref="ProductMasterTrx.MakerCloseRejectedUpdateProductMasterOperation"/>
            <entry key="MakerSaveUpdateProductMasterOperation" value-ref="ProductMasterTrx.MakerSaveUpdateProductMasterOperation"/>
            <entry key="MakerSaveProductMasterOperation" value-ref="ProductMasterTrx.MakerSaveProductMasterOperation"/>
            <entry key="MakerUpdateDraftCreateProductMasterOperation" value-ref="ProductMasterTrx.MakerUpdateDraftCreateProductMasterOperation"/>
   			<entry key="MakerCloseDraftProductMasterOperation" value-ref="ProductMasterTrx.MakerCloseDraftProductMasterOperation"/>
   			
            </map>
        </property>
        <property name="processTrxManager" ref="processTrxManager"/>
    </bean>

