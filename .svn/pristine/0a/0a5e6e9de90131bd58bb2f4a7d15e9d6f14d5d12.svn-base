<binding name="sibs_cif_search_req">
    <include path="sibs_header_req.xml" />
    <mapping name="Definition" class="com.integrosys.simulator.host.SIBS.CIFSearch.OBSIBSCustomerSearchInquiryRes" extends="com.integrosys.simulator.host.SIBS.OBMessageHeader"> 
        <structure name="HeaderDefinition">
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
			<collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.CIFSearch.OBSIBSCustomerSearchInquiryRes">
			        <value name="FixColumn" field="idNumber"/>
			        <value name="FixColumn" field="idType"/>
			        <value name="FixColumn" field="idIssueCountry"/>
			        <value name="FixColumn" field="mainIdFlag" format="stringFmt"/>
			        <value name="FixColumn" field="customerNumber" format="longFmt"/>
					<value name="FixColumn" field="formattedShortName" format="stringFmt"/>
					<value name="FixColumn" field="searchMethod"/>
				</structure>
		     </collection>
	     </structure>
    </mapping> 	
</binding>