					<header>MAKER ID</header>
				</reportColumn>
				<reportColumn>
					<width>500</width>
					<format>textFormat</format>
					<header>CHECKER ID</header>
				</reportColumn>
				<reportColumn>
					<width>500</width>
					<format>textFormat</format>
					<header>MAKER DATETIME</header>
				</reportColumn>
				<reportColumn>
					<width>500</width>
					<format>textFormat</format>
					<header>CHECKER DATETIME</header>
				</reportColumn>
				<reportColumn>
					<width>500</width>
