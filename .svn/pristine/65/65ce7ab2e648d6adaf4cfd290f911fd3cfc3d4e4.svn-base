					<header>RESPONSE DATE/TIME</header>
				</reportColumn>
				<reportColumn>
					<width>25</width>
					<format>textFormat</format>
					<header>STATUS</header>
				</reportColumn>
				<reportColumn>
					<width>500</width>
					<format>textFormat</format>
					<header>ERROR CODE</header>
				</reportColumn>
				<reportColumn>
					<width>500</width>
