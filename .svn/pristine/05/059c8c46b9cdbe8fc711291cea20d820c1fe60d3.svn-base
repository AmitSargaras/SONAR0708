.shadetabs {
	PADDING-RIGHT: 0px; MARGIN-TOP: 1px; PADDING-LEFT: 20px; MARGIN-BOTTOM: 0px; PADDING-BOTTOM: 3px; FONT: bold 12px Verdana; MARGIN-LEFT: 0px; PADDING-TOP: 30px; LIST-STYLE-TYPE: none; TEXT-ALIGN: left
}
.shadetabs LI {
	DISPLAY: inline; MARGIN: 0px
}
.shadetabs LI A {
	MARGIN-LEFT: 0px; MARGIN-RIGHT: 3px; BORDER-RIGHT: #778 1px solid; PADDING-RIGHT: 7px; BORDER-TOP: #778 1px solid; PADDING-LEFT: 7px; Z-INDEX: 1; BACKGROUND: url(shade.gif) white repeat-x left top; PADDING-BOTTOM: 3px; BORDER-LEFT: #778 1px solid; COLOR: #2d2b2b; PADDING-TOP: 3px; BORDER-BOTTOM: #778 1px solid; TEXT-DECORATION: none;
	/* POSITION: relative;  */
}
.shadetabs LI A:visited {
	COLOR: #2d2b2b
}
.shadetabs LI A:hover {
	COLOR: #2d2b2b; TEXT-DECORATION: underline; BACKGROUND: url(shadeactive.gif) white repeat-x left top;
}
.shadetabs LI A.selected {
	TOP: -2px;
	/* POSITION: relative;  */
}
.shadetabs LI A.selected {
	BACKGROUND-IMAGE: url(shadeactive.gif); BORDER-BOTTOM-COLOR: white
}
.shadetabs LI A.selected:hover {
	TEXT-DECORATION: none
}
.tabcontent {
	DISPLAY: none;
}

