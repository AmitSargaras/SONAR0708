<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
                         "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <display-name>Manualinput</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>SBMILmtProxy</ejb-name>
            <home>com.integrosys.cms.app.manualinput.limit.proxy.SBMILmtProxyHome</home>
            <remote>com.integrosys.cms.app.manualinput.limit.proxy.SBMILmtProxy</remote>
            <ejb-class>com.integrosys.cms.app.manualinput.limit.proxy.SBMILmtProxyBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <resource-ref>
               <res-ref-name>jdbc/CMSPool</res-ref-name>
               <res-type>javax.sql.DataSource</res-type>
               <res-auth>Container</res-auth>
               <res-sharing-scope>Shareable</res-sharing-scope>
            </resource-ref>
        </session>
	  <session>
            <ejb-name>SBMISecProxy</ejb-name>
            <home>com.integrosys.cms.app.manualinput.security.proxy.SBMISecProxyHome</home>
            <remote>com.integrosys.cms.app.manualinput.security.proxy.SBMISecProxy</remote>
            <ejb-class>com.integrosys.cms.app.manualinput.security.proxy.SBMISecProxyBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <resource-ref>
               <res-ref-name>jdbc/CMSPool</res-ref-name>
               <res-type>javax.sql.DataSource</res-type>
               <res-auth>Container</res-auth>
               <res-sharing-scope>Shareable</res-sharing-scope>
            </resource-ref>
        </session>
	  <session>
            <ejb-name>SBMIAAProxy</ejb-name>
            <home>com.integrosys.cms.app.manualinput.aa.proxy.SBMIAAProxyHome</home>
            <remote>com.integrosys.cms.app.manualinput.aa.proxy.SBMIAAProxy</remote>
            <ejb-class>com.integrosys.cms.app.manualinput.aa.proxy.SBMIAAProxyBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <resource-ref>
               <res-ref-name>jdbc/CMSPool</res-ref-name>
               <res-type>javax.sql.DataSource</res-type>
               <res-auth>Container</res-auth>
               <res-sharing-scope>Shareable</res-sharing-scope>
            </resource-ref>
        </session>	
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>SBMILmtProxy</ejb-name>
                <method-name>*</method-name>
            </method>
		<method>
                <ejb-name>SBMISecProxy</ejb-name>
                <method-name>*</method-name>
            </method>
		<method>
                <ejb-name>SBMIAAProxy</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>	
