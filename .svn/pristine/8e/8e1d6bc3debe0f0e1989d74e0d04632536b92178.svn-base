CREATE OR REPLACE PROCEDURE sp_mig_cam_ui_facility_tenor_VALIDATE
AS
  TENOR_UNIT VARCHAR2(30);
  CURSOR CR_DATA
  IS
    SELECT * FROM DAY_ZERO_MIG_CAM_UI_FACILITY WHERE UPDATE_STATUS = 'N';
  RC_DATA CR_DATA%ROWTYPE;
BEGIN
  OPEN CR_DATA;
  LOOP
    FETCH CR_DATA INTO RC_DATA;
    EXIT
  WHEN CR_DATA%NOTFOUND;
    TENOR_UNIT   := RC_DATA.TENOR_UNIT;
    IF TENOR_UNIT = 'YEAR' OR TENOR_UNIT='MONTH' OR TENOR_UNIT='DAYS' THEN
      UPDATE DAY_ZERO_MIG_CAM_UI_FACILITY
      SET IS_VALIDATED        = 'Y'
      WHERE CMS_LSP_APPR_LMTS_ID = RC_DATA.CMS_LSP_APPR_LMTS_ID;
	 
    END IF;
  END LOOP;
  CLOSE CR_DATA;
  COMMIT;
END sp_mig_cam_ui_facility_REM_VALIDATE;
