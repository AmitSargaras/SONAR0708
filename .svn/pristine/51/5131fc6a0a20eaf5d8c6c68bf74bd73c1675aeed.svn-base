<binding>
	<format label="stringFmt" type="java.lang.String" serializer="com.integrosys.simulator.host.transformer.utils.StringConverter.serializeString"/>
	<format label="longFmt" type="java.lang.Long" serializer="com.integrosys.simulator.host.transformer.utils.IntegerConverter.serializeLong"/>
	<format label="dateFmt" type="java.util.Date" serializer="com.integrosys.simulator.host.transformer.utils.DateConverter.serializeDateDDMMYYYY"/>
	<format label="decimalFmt" type="java.math.BigDecimal" serializer="com.integrosys.simulator.host.transformer.utils.BigDecimalConverter.serializeBigDecimalToStr"/>
	<format label="timestampFmt" type="java.sql.Timestamp" serializer="com.integrosys.simulator.host.transformer.utils.DateConverter.serializeTimestampYYMM"/>
	<mapping class="com.integrosys.simulator.host.SEMA.OBSEMAMessageHeader" abstract="true"> 
	    <!--  message header start -->
	    <value name="HeaderColumn" field="socketMessageLength"/>
        <value name="HeaderColumn" field="transactionCode"/>
		<value name="HeaderColumn" field="msgDate" />
		<value name="HeaderColumn" field="referenceNumber" /> 
		<value name="HeaderColumn" field="messageId"/> 
		<value name="HeaderColumn" field="responseCode"/> 
		<value name="HeaderColumn" field="responseMessage" />		
		<value name="HeaderColumn" field="rlosUserId"/> 
		<value name="HeaderColumn" field="numberOfTxnMessage"/> 
		<value name="HeaderColumn" field="moreRecordIndicator"/> 
		<value name="HeaderColumn" field="lastRecordKey" /> 		
    </mapping>
</binding>