	</description>
	
	<bean id="customEditorConfigurer" class="org.springframework.beans.factory.config.CustomEditorConfigurer">
	    <property name="customEditors">
	        <map>
	            <entry key="int[]" value="org.springframework.batch.support.IntArrayPropertyEditor"/>
				<entry key="org.springframework.batch.item.file.transform.Range[]" value="org.springframework.batch.item.file.transform.RangeArrayPropertyEditor"/>
	            <entry key="java.util.Date" value="com.integrosys.cms.batch.common.BatchCustomDateEditor"/>
				<entry key="com.integrosys.cms.batch.common.mapping.ColumnMetaInfo[]" value="com.integrosys.cms.batch.common.mapping.ColumnMetaInfoArrayPropertyEditor"/>      
	        </map>
	    </property>
	</bean>

