<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="actualOtherCovenantDetailsValues" 
		name="com.integrosys.cms.app.limit.bus.OBOtherCovenant" table="SCI_LSP_OTHER_COVENANT_VALUES" lazy="false">
            <id name="otherCovenantId"  type="long" column="CMS_LE_OTHER_COVENANT_DETAILS_ID">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">"SCI_LSP_OTHER_COVENANT_VALUES_SEQ"  </param>
			   </generator>
            </id>
            <!-- <id name="otherCovenantId" type="long" column="CMS_LE_OTHER_COVENANT_ID" /> -->
    		<property name="CustRef" column="CMS_LE_ID"/>
    		<property name="stagingRefid" column="CMS_LE_STAGE_OC_ID"/>
			<property name="FacilityNameValue" column="COVENANT_FACILITY_NAME_VALUE"/>
			<property name="status" column="STATUS"/>
			<property name="MonitoringResponsibiltyValue" column="COVENANT_MONITORING_RESP_VALUE"/>
    </class>    
    
    
    <class entity-name="stagingotherCovenantDetailsValues" 
		name="com.integrosys.cms.app.limit.bus.OBOtherCovenant" table="STAGE_SCI_LSP_OTHER_COVENANT_VALUES" lazy="false">
          <id name="otherCovenantId"  type="long" column="CMS_LE_OTHER_COVENANT_DETAILS_ID">
                <generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">"STAGE_SCI_LSP_OTHER_COVENANT_VALUES_SEQ"  </param>
			   </generator>
            </id>
            <!-- <property name="customerId" column="CUSTOMER_ID" /> -->
            <property name="CustRef" column="CMS_LE_ID"/>
    		<property name="stagingRefid" column="CMS_LE_STAGE_OC_ID"/>
			<property name="FacilityNameValue" column="COVENANT_FACILITY_NAME_VALUE"/>
			<property name="status" column="STATUS"/>
			<property name="MonitoringResponsibiltyValue" column="COVENANT_MONITORING_RESP_VALUE"/>
		</class>
</hibernate-mapping>