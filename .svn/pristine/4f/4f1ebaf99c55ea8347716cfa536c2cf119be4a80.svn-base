		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="MakerCloseRejectedCreatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerCloseRejectedCreatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="MakerSaveUpdatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerSaveUpdatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerSavePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerSavePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="ReadPartyGroupIDOperation"
		class="com.integrosys.cms.app.partygroup.trx.ReadPartyGroupIDOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerUpdatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerUpdatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerEditRejectedDeletePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerEditRejectedDeletePartyGroupOperation">
