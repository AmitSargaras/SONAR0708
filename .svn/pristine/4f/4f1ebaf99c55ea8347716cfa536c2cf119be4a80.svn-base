		class="com.integrosys.cms.app.partygroup.trx.MakerEditRejectedCreatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerCloseRejectedDisablePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerCloseRejectedDisablePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerEditRejectedEnablePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerEditRejectedEnablePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="CheckerRejectActivatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.CheckerRejectActivatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="CheckerApproveEnablePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.CheckerApproveEnablePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="MakerDeletePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerDeletePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>


	<bean id="MakerCreatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerCreatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="partyGroupReadController"
		class="com.integrosys.cms.app.partygroup.trx.PartyGroupReadController">
		<property name="nameTrxOperationMap">
			<map>
				<entry key="ReadPartyGroupOperation" value-ref="ReadPartyGroupOperation" />
				<entry key="ReadPartyGroupIDOperation" value-ref="ReadPartyGroupIDOperation" />


			</map>
		</property>
	</bean>

	<bean id="partyGroupTrxController"
		class="com.integrosys.cms.app.partygroup.trx.PartyGroupTrxController">
		<property name="nameTrxOperationMap">
			<map>
				<entry key="MakerEditRejectedUpdatePartyGroupOperation"
					value-ref="MakerEditRejectedUpdatePartyGroupOperation" />
				<entry key="MakerUpdatePartyGroupOperation" value-ref="MakerUpdatePartyGroupOperation" />
				<entry key="MakerSaveUpdatePartyGroupOperation" value-ref="MakerSaveUpdatePartyGroupOperation" />
				<entry key="MakerCloseDraftPartyGroupOperation" value-ref="MakerCloseDraftPartyGroupOperation" />
				<entry key="MakerCloseRejectedUpdatePartyGroupOperation"
					value-ref="MakerCloseRejectedUpdatePartyGroupOperation" />
				<entry key="CheckerApproveUpdatePartyGroupOperation"
					value-ref="CheckerApproveUpdatePartyGroupOperation" />
				<entry key="CheckerApproveCreatePartyGroupOperation"
					value-ref="CheckerApproveCreatePartyGroupOperation" />
				<entry key="CheckerRejectPartyGroupOperation" value-ref="CheckerRejectPartyGroupOperation" />
				<entry key="MakerCreatePartyGroupOperation" value-ref="MakerCreatePartyGroupOperation" />
				<entry key="MakerEditRejectedCreatePartyGroupOperation"
					value-ref="MakerEditRejectedCreatePartyGroupOperation" />
				<entry key="MakerDeletePartyGroupOperation" value-ref="MakerDeletePartyGroupOperation" />
				<entry key="CheckerApproveUpdatePartyGroupOperation"
					value-ref="CheckerApproveUpdatePartyGroupOperation" />
				<entry key="CheckerRejectPartyGroupOperation" value-ref="CheckerRejectPartyGroupOperation" />
				<entry key="CheckerApproveDeletePartyGroupOperation"
					value-ref="CheckerApproveDeletePartyGroupOperation" />
				<entry key="MakerActivatePartyGroupOperation" value-ref="MakerActivatePartyGroupOperation" />
				<entry key="CheckerApproveEnablePartyGroupOperation"
					value-ref="CheckerApproveEnablePartyGroupOperation" />
				<entry key="MakerSavePartyGroupOperation" value-ref="MakerSavePartyGroupOperation" />
				<entry key="MakerEditRejectedDeletePartyGroupOperation"
					value-ref="MakerEditRejectedDeletePartyGroupOperation" />
				<entry key="CheckerRejectActivatePartyGroupOperation"
					value-ref="CheckerRejectActivatePartyGroupOperation" />
				<entry key="MakerCloseRejectedDisablePartyGroupOperation"
					value-ref="MakerCloseRejectedDisablePartyGroupOperation" />
				<entry key="MakerEditRejectedEnablePartyGroupOperation"
					value-ref="MakerEditRejectedEnablePartyGroupOperation" />
				<entry key="MakerCloseRejectedCreatePartyGroupOperation"
					value-ref="MakerCloseRejectedCreatePartyGroupOperation" />
				<entry key="MakerCreateSavedPartyGroupOperation" value-ref="MakerCreateSavedPartyGroupOperation" />

			</map>
		</property>
		<property name="processTrxManager" ref="processTrxManager" />
	</bean>

	<bean id="partyGroupTrxControllerFactory"
		class="com.integrosys.cms.app.partygroup.trx.PartyGroupTrxControllerFactory">
		<property name="partyGroupReadController" ref="partyGroupReadController" />
		<property name="partyGroupTrxController" ref="partyGroupTrxController" />
	</bean>
</beans>