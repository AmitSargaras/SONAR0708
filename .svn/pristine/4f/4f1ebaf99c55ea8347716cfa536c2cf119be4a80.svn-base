		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>

	<bean id="MakerEditRejectedUpdatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerEditRejectedUpdatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerCloseRejectedUpdatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerCloseRejectedUpdatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="MakerCloseDraftPartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.MakerCloseDraftPartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="CheckerApproveUpdatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.CheckerApproveUpdatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="CheckerApproveDeletePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.CheckerApproveDeletePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="CheckerApproveCreatePartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.CheckerApproveCreatePartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
	<bean id="CheckerRejectPartyGroupOperation"
		class="com.integrosys.cms.app.partygroup.trx.CheckerRejectPartyGroupOperation">
		<property name="partyGroupBusManager" ref="partyGroupBusManager" />
		<property name="stagingPartyGroupBusManager" ref="stagingPartyGroupBusManager" />
	</bean>
