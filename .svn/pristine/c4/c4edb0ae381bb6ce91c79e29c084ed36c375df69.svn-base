<binding name="sibs_app_search_req">
    <include path="sibs_header_req.xml" />
    <mapping name="Definition" class="com.integrosys.simulator.host.SIBS.APPSearch.OBSIBSAppSearchRes" extends="com.integrosys.simulator.host.SIBS.OBMessageHeader"> 
        <structure name="HeaderDefinition">
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
	        <collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.APPSearch.OBSIBSAppSearchRes">
	        		<value name="FixColumn" field="idNumber"/>
	        		<value name="FixColumn" field="idTypeCode"/>
	        		<value name="FixColumn" field="idIssueCountry"/>
		        	<value name="FixColumn" field="customerNumber"/>
		        	<value name="FixColumn" field="applicationNumber"/>
		        	<value name="FixColumn" field="applicationBranchNumber"/>
			        <value name="FixColumn" field="applicationDate" format="dateFmt"/>
			        <value name="FixColumn" field="shortName"/>
			        <value name="FixColumn" field="formattedShortName"/>
			     </structure>
		     </collection>
	     </structure>
    </mapping> 	
</binding>