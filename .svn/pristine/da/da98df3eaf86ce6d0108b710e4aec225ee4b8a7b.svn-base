<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.integrosys.cms.app.manualinput.party">
	<class  entity-name="actualPANValidationLog" name="PANValidationLog" table="PAN_VALIDATION_LOG" lazy="false">
		<id name="id" column="ID" type="long">
			<generator class="com.integrosys.base.techinfra.orm.DateAppendedSequenceGenerator">
				<param name="sequence">PAN_VALIDATION_LOG_SEQ</param>
			</generator>
		</id>
		
        <property name="panNo"    	column="PANNO"  />		
        <property name="partyID"    	column="PARTYID"  />		
        <property name="cmsMainProfileID"    	column="CMSMAINPROFILEID"  />		
        <property name="requestMessage" column="REQUESTMESSAGE"/>
		<property name="responseMessage" column="RESPONSEMESSAGE"/>
		<property name="requestDateTime"    	column="REQUESTDATETIME"  />
		<property name="responseDateTime"    	column="RESPONSEDATETIME"   />
		<property name="responseCode"    	column="RESPONSECODE"    		 />
		<property name="errorMessage"    	column="ERRORMESSAGE"     />
		<property name="status"    	column="STATUS"    />
		<property name="lastValidatedDate"    	column="LASTVALIDATEDDATE"    />
		<property name="lastValidatedBy"    	column="LASTVALIDATEDBY"    />
		<property name="isPANNoValidated"    	column="ISPANNOVALIDATED"    />
		<property name="requestNo"    	column="REQUESTNO"    />
		<property name="partyNameAsPerPan"    	column="PARTYNAMEASPERPAN"    />
		<property name="dateOfIncorporation"    	column="DATEOFINCORPORATION"    />

	</class>
</hibernate-mapping>
