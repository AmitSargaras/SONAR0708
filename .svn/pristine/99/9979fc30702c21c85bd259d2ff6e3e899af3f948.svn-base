../set_mig_env.sh $*

if [ ! "$IS_PROCEED" = "true" ]; then
	return
fi

"$DB2_HOME"/bin/db2 LOAD FROM "$FILE_PATH" OF DEL MODIFIED BY COLDEL, DUMPFILE="$DUMP_PATH" IMPLIEDDECIMAL TIMESTAMPFORMAT=\"YYYYMMDD\" METHOD P \(1, 2, 3, 4, 5, 6, 7, 9, 10, 12, 14, 16, 18, 19\)  MESSAGES "$LOG_PATH" INSERT INTO "$DB_SCHEMA".DC_MS \(CIF_NUMBER, BORROWER_NAME, ACCOUNT_NUMBER, BUSINESS_UNIT, BRANCH_CODE, CMS_COLLATERAL_ID, COLLATERAL_CODE, COLLATERAL_NAME, MATURITY_DATE, ISSUER_NAME, BOND_ISSUE_DATE, STOCK_CODE, STOCK_NAME, ISIN_CODE\) FOR EXCEPTION "$DB_SCHEMA".DC_MS_ERR NONRECOVERABLE CHECK PENDING CASCADE IMMEDIATE

"$DB2_HOME"/bin/db2 SET INTEGRITY FOR "$DB_SCHEMA".DC_MS IMMEDIATE CHECKED FORCE GENERATED FULL ACCESS FOR EXCEPTION IN "$DB_SCHEMA".DC_MS USE "$DB_SCHEMA".DC_MS_ERR
