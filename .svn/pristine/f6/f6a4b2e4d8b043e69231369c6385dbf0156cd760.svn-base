<reports>
	<report>
		<query>
				SELECT DISTINCT PARTY_ID , PARTY_NAME , SOURCE_SEC_ID,COLLATERAL_CODE_NAME,FACILITY_ID,FACILITY_NAME,
 FAC_SANCTION_AMT,FACILITY_RELEASED_AMOUNT,DP_TO_BE_CALCULATED,DRAWING_POWER_AS_PER_STOCK_STATEMENT,
 STOCK_DOC_MONTH,STOCK_DOC_YEAR,DUE_DATE,DP_SHARE,MAKER_ID,CHECKER_ID,MAKER_DATE,CHECKER_DATE 
 
FROM
  DP_REGUL_AUDIT_TRAIL_MV
              
			</query>
		<whereClause>
			<mandatoryClause>
				  1=1
				</mandatoryClause>
				<param>
                <name>fromDate</name>
                <condition> AND TRUNC(TRANSACTION_DATE) BETWEEN </condition>
            </param>
            <param>
                <name>toDate</name>
                <condition> AND </condition>
            </param>
            <param>
				<name>party</name>
				<condition> and PARTY_ID = </condition>
			</param>
		</whereClause>
