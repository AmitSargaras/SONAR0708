<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<description>Convenant Response Mapping - CV002R</description>

	<include href="common-response.cm.xml" /> 

	<class extends="com.integrosys.cms.host.eai.EAIBody" name=" com.integrosys.cms.host.eai.covenant.CovenantResponseMsgBody" >
		<map-to xml="MsgBody"/>
		<field name="ConvenantItem" type="com.integrosys.cms.host.eai.covenant.bus.CovenantItem" collection="vector">
			<bind-xml name="CovenantItem" />
		</field>
	</class>

	<class name="com.integrosys.cms.host.eai.covenant.bus.CovenantItem"  collection="vector">
		<map-to xml="CovenantItem"/>
		<field name="CMSCovenantItemID" type="long">
			<bind-xml name="CMSCovenantItemID" />
		</field>
		<field name="CovenantCondition" type="string">
			<bind-xml name="CovenantCondition" />
		</field>
		<field name="DocEndDate" type="string">
			<bind-xml name="DocEndDate" />
		</field>
		<field name="Remarks" type="string">
			<bind-xml name="Remarks" />
		</field>
	</class>

</mapping>