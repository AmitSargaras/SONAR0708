<?xml version="1.0"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SBCMSTrxManager</ejb-name>
        <reference-descriptor>
			<resource-description>
				<res-ref-name>jdbc/CMSPool</res-ref-name>
				<jndi-name>CMSPool</jndi-name>
			</resource-description>
		</reference-descriptor>
        <jndi-name>SBCMSTrxManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>SBCompTrxManager</ejb-name>
        <reference-descriptor>
			<resource-description>
				<res-ref-name>jdbc/CMSPool</res-ref-name>
				<jndi-name>CMSPool</jndi-name>
			</resource-description>
		</reference-descriptor>
        <jndi-name>SBCompTrxManagerHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBCMSTrxValue</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
			<resource-description>
				<res-ref-name>jdbc/CMSPool</res-ref-name>
				<jndi-name>CMSPool</jndi-name>
			</resource-description>
		</reference-descriptor>
        
        <jndi-name>EBCMSTrxValueHome</jndi-name>
    </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
        <ejb-name>EBTrxStateMatrix</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                  <type-version>6.0</type-version>
                  <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
      <reference-descriptor>
			<resource-description>
				<res-ref-name>jdbc/CMSPool</res-ref-name>
				<jndi-name>CMSPool</jndi-name>
			</resource-description>
		</reference-descriptor>
			
        <jndi-name>EBTrxStateMatrixHome</jndi-name>
    </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
        <ejb-name>EBTrxHistory</ejb-name>
        <entity-descriptor>
            <persistence>
                <persistence-use>
                    <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
                    <type-version>6.0</type-version>
                    <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
                </persistence-use>
            </persistence>
        </entity-descriptor>
        <reference-descriptor>
			<resource-description>
				<res-ref-name>jdbc/CMSPool</res-ref-name>
				<jndi-name>CMSPool</jndi-name>
			</resource-description>
		</reference-descriptor>
        <jndi-name>EBTrxHistoryHome</jndi-name>
    </weblogic-enterprise-bean>
</weblogic-ejb-jar>