<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

  <bean name="/ToDo" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.todo.TodoAction">
    <property name="nameCommandMap">
      <map>
        <entry key="RefreshPendingCasesCommand" value-ref="TodoAction.RefreshPendingCasesCommand" />
        <entry key="ReassignPendingCasesCommand" value-ref="TodoAction.ReassignPendingCasesCommand" />
        <entry key="PrepareToTrackCommand" value-ref="TodoAction.PrepareToTrackCommand" />
        <entry key="PreparePendingCasesCommand" value-ref="TodoAction.PreparePendingCasesCommand" />
        <entry key="PrepareTodoCommand" value-ref="TodoAction.PrepareTodoCommand" />
        <entry key="PrepareNewLimitProfileCommand" value-ref="TodoAction.PrepareNewLimitProfileCommand" />
        <entry key="PrepareCustomerTodoCommand" value-ref="TodoAction.PrepareCustomerTodoCommand" />
        <entry key="PreparePendingPerfectionCreditFolderCommand" value-ref="TodoAction.PreparePendingPerfectionCreditFolderCommand" />
        <entry key="PrepareTodoDiscrepancyCommand" value-ref="TodoAction.PrepareTodoDiscrepancyCommand" />
        <entry key="PrepareToTrackDiscrepancyCommand" value-ref="TodoAction.PrepareToTrackDiscrepancyCommand" />
      </map>
    </property>
  </bean>

</beans>