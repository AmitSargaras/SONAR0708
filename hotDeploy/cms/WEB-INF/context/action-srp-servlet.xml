<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">


  <bean name="/SRPCountry" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.srp.country.SRPCountryAction">
    <property name="nameCommandMap">
      <map>
        <entry key="SRPCountryMakerEditCmd" value-ref="SRPCountryAction.SRPCountryMakerEditCmd"/>
        <entry key="SRPCountryCheckerApproveEditCmd" value-ref="SRPCountryAction.SRPCountryCheckerApproveEditCmd"/>
        <entry key="SRPCountryMakerReadCmd" value-ref="SRPCountryAction.SRPCountryMakerReadCmd"/>
        <entry key="SRPCountryMakerReadRejectedCmd" value-ref="SRPCountryAction.SRPCountryMakerReadRejectedCmd"/>
        <entry key="SRPCountryMakerCancelEditCmd" value-ref="SRPCountryAction.SRPCountryMakerCancelEditCmd"/>
        <entry key="SRPCountryCheckerReadCmd" value-ref="SRPCountryAction.SRPCountryCheckerReadCmd"/>
        <entry key="SRPCountryMakerPrepareCommand" value-ref="SRPCountryAction.SRPCountryMakerPrepareCommand"/>
        <entry key="SRPCountryCheckerRejectEditCmd" value-ref="SRPCountryAction.SRPCountryCheckerRejectEditCmd"/>
      </map>
    </property>
  </bean>
  
  <bean name="/SRPGlobal" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.srp.global.SRPGlobalAction">
    <property name="nameCommandMap">
      <map>
        <entry key="SRPGlobalCheckerRejectEditCmd" value-ref="SRPGlobalAction.SRPGlobalCheckerRejectEditCmd"/>
        <entry key="SRPGlobalCheckerReadCmd" value-ref="SRPGlobalAction.SRPGlobalCheckerReadCmd"/>
        <entry key="SRPGlobalMakerCancelEditCmd" value-ref="SRPGlobalAction.SRPGlobalMakerCancelEditCmd"/>
        <entry key="SRPGlobalMakerEditCmd" value-ref="SRPGlobalAction.SRPGlobalMakerEditCmd"/>
        <entry key="SRPGlobalCheckerApproveEditCmd" value-ref="SRPGlobalAction.SRPGlobalCheckerApproveEditCmd"/>
        <entry key="SRPGlobalMakerReadRejectedCmd" value-ref="SRPGlobalAction.SRPGlobalMakerReadRejectedCmd"/>
        <entry key="SRPGlobalMakerPrepareCommand" value-ref="SRPGlobalAction.SRPGlobalMakerPrepareCommand"/>
        <entry key="SRPGlobalMakerReadCmd" value-ref="SRPGlobalAction.SRPGlobalMakerReadCmd"/>
      </map>
    </property>
  </bean>
  
</beans>