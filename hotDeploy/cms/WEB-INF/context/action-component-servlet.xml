<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
			
			
			<bean name="/component" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.component.MaintainComponentAction">
			<property name="nameCommandMap">
			<map>
				<entry key="ListComponentCmd" value-ref="MaintainComponentAction.ListComponentCmd"/>
				<entry key="MakerPrepareCreateComponentCmd" value-ref="MaintainComponentAction.MakerPrepareCreateComponentCmd"/>
				<entry key="MakerCreateComponentCmd" value-ref="MaintainComponentAction.MakerCreateComponentCmd"/>
				<entry key="MakerReadComponentCmd" value-ref="MaintainComponentAction.MakerReadComponentCmd"/>
				<entry key="CheckerReadComponentCmd" value-ref="MaintainComponentAction.CheckerReadComponentCmd"/>
				<entry key="CheckerApproveEditComponentCmd" value-ref="MaintainComponentAction.CheckerApproveEditComponentCmd"/>
				<entry key="CheckerRejectEditComponentCmd" value-ref="MaintainComponentAction.CheckerRejectEditComponentCmd"/>
				<entry key="MakerDeleteComponentCmd" value-ref="MaintainComponentAction.MakerDeleteComponentCmd"/>
				<entry key="MakerEditComponentCmd" value-ref="MaintainComponentAction.MakerEditComponentCmd"/>
				<entry key="MakerCloseComponentCmd" value-ref="MaintainComponentAction.MakerCloseComponentCmd"/>
				<entry key="SearchComponentCmd" value-ref="MaintainComponentAction.SearchComponentCmd"/>
			</map>
			</property>
			</bean>

</beans>
