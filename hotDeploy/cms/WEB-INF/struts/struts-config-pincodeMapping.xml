<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="PincodeMappingForm" type="com.integrosys.cms.ui.pincodemapping.PincodeMappingForm">
        </form-bean>
    </form-beans>
    
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/StatePincodeMapping" type="com.integrosys.cms.ui.pincodemapping.PincodeMappingAction" name="PincodeMappingForm" validate="false" scope="request">
          <forward name="maker_list_state_pincode_mapping" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/pincodemapping/maker_list_state_pincode_mapping.jsp"/>
                <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                 <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
            </forward>
            <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/maker_read_save_state_pincode_mapping.jsp"/>   
            </forward>
            <forward name="maker_prepare_create_state_pincode_mapping" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/pincodemapping/maker_create_state_pincode_mapping.jsp"/>
                <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
            </forward>
            
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
            </forward>
            <forward name="checker_list_state_pincode_mapping" path="/template/common_template.jsp">
			<set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
			<set-property property="rightFrame" value="/pincodemapping/checker_list_state_pincode_mapping.jsp"/>
		</forward>
		 <forward name="checker_state_pincode_mapping_page" path="/template/common_template.jsp">
            <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/checker_state_pincode_mapping.jsp"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
             <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
             <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
            </forward>
            <forward name="common_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_save.jsp"/>
                <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
            </forward>
             <forward name="maker_prepare_resubmit" path="/template/common_template.jsp">
                    <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/maker_resubmit_edit_state_pincode_mapping.jsp"/>
            </forward>
            <forward name="maker_prepare_close" path="/template/common_template.jsp">
                  <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
               <set-property property="rightFrame" value="/pincodemapping/maker_close_state_pincode_mapping.jsp"/>
            </forward>
            <forward name="maker_view_page" path="/template/common_template.jsp">
            <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/maker_view_delete_state_pincode_mapping.jsp"/>
            </forward>
             <forward name="maker_edit_state_pincode_mapping_page" path="/template/common_template.jsp">
                 <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/maker_edit_state_pincode_mapping.jsp"/>
            </forward>
             <forward name="work_in_process" path="/template/common_template.jsp">
                 <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/common/wip.html"/>
              </forward>
              <forward name="state_pincode_mapping_prepare_delete_page" path="/template/common_template.jsp">
                  <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/maker_prepare_delete_state_pincode_mapping.jsp"/>
            </forward>
             <forward name="maker_create_draft_state_pincode_mapping" path="/template/common_template.jsp">
                  <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/maker_create_state_pincode_mapping.jsp"/>
            </forward>
            <forward name="checker_process_create" path="/template/common_template.jsp">
                  <set-property property="title" value="menu.manual_feeds.state_pincode_mapping.name"/>
                <set-property property="rightFrame" value="/pincodemapping/checker_state_pincode_mapping.jsp"/>
            </forward>
            
            
        </action>
        </action-mappings>
</struts-config>
