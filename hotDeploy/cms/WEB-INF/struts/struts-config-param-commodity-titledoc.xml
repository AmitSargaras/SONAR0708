<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="TitleDocumentListForm" type="com.integrosys.cms.ui.commodityglobal.titledocument.list.TitleDocumentListForm">
        </form-bean>

        <form-bean name="TitleDocumentItemForm" type="com.integrosys.cms.ui.commodityglobal.titledocument.item.TitleDocumentItemForm">
        </form-bean>
    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/TitleDocumentList"
            type="com.integrosys.cms.ui.commodityglobal.titledocument.list.TitleDocumentListAction"
            name="TitleDocumentListForm" validate="false" scope="request">
            <forward name="prepare" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/commodityglobal/titledocument/list/update_titledocument_list.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="process" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/commodityglobal/titledocument/list/chk_view_titledocument_list.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/commodityglobal/titledocument/list/view_titledocument_list.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="read" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/commodityglobal/titledocument/list/view_titledocument_list.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="to_track" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/commodityglobal/titledocument/list/view_titledocument_list.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="submit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="update" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_save.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="approve" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="reject" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="wip" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="prepare_add" path="/TitleDocumentItem.do?event=prepare_add"></forward>
            <forward name="edit_item" path="/TitleDocumentItem.do?event=prepare"></forward>
        </action>

        <action path="/TitleDocumentItem"
            type="com.integrosys.cms.ui.commodityglobal.titledocument.item.TitleDocumentItemAction"
            name="TitleDocumentItemForm" validate="false" scope="request">
            <forward name="prepare" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/commodityglobal/titledocument/item/update_titledocument_item.jsp"/>
                <set-property property="title" value="title.commodityglobal.titledocument"/>
            </forward>
            <forward name="return" path="/TitleDocumentList.do?event=return"></forward>
        </action>
    
    </action-mappings>
    
    <!--<message-resources parameter="ApplicationResources-param-commodity-titledoc"/>-->
</struts-config>