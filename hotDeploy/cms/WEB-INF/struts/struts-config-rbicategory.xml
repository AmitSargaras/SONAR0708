<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="RbiCategoryForm" type="com.integrosys.cms.ui.rbicategory.RbiCategoryForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/rbi_category" type="com.integrosys.cms.ui.rbicategory.RbiCategoryAction" name="RbiCategoryForm" validate="false" scope="request">
            <forward name="get_list_rbi_category_page" path="/rbi_category.do?event=redirect_list_rbi_category">
            </forward>
            <forward name="maker_list_rbi_category_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_list_rbi_category.jsp"/>
                <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="search_rbi_category_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/search_rbi_category.jsp"/>
                <set-property property="title" value="title.master.rbicategory"/>
            </forward>            
            <forward name="maker_add_edit_rbi_category_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_add_edit_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_view_delete_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="prepare_create_rbi_category" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_add_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_read_delete_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_read_save_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="maker_prepare_resubmit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_resubmit_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
             <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_resubmit_delete_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
             <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_close_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
             <forward name="maker_view_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/maker_view_todo_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="checker_rbi_category_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/rbicategory/checker_rbi_category.jsp"/>
                 <set-property property="title" value="title.master.rbicategory"/>
            </forward>
            <forward name="rejected_read_page" path="/rbicategory/forward.jsp"/>
        </action>
		
    </action-mappings>
    
    <message-resources key="message-rbicategory" parameter="ApplicationResources-rbicategory"/>
</struts-config>