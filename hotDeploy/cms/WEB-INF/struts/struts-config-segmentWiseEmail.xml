<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="SegmentWiseEmailForm" type="com.integrosys.cms.ui.segmentWiseEmail.SegmentWiseEmailForm">
        </form-bean>
    </form-beans>
    
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/segmentWiseEmail" type="com.integrosys.cms.ui.segmentWiseEmail.SegmentWiseEmailAction" name="SegmentWiseEmailForm" validate="false" scope="request">
  
           <forward name="list_segment_wise_email_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/segmentWiseEmail/maker_list_segment_wise_email.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
            <forward name="maker_prepare_create_segment_wise_email" path="/template/common_template.jsp">
				<set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
				<set-property property="rightFrame" value="/segmentWiseEmail/maker_create_segment_wise_email.jsp"/>
			</forward>
			
			 <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
               <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
             <forward name="checker_segment_wise_email_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/segmentWiseEmail/checker_segment_wise_email.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
             <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
             <forward name="maker_view_segment_wise_email_page" path="/template/common_template.jsp">
               <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
			   <set-property property="rightFrame" value="/segmentWiseEmail/maker_view_segment_wise_email.jsp"/>
            </forward>
            
            <forward name="maker_prepare_resubmit_create" path="/template/common_template.jsp">
				<set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
				<set-property property="rightFrame" value="/segmentWiseEmail/maker_resubmit_create_segment_wise_email.jsp"/>
			</forward>
			
			 <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/segmentWiseEmail/maker_close_segment_wise_email.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
            
            <forward name="maker_edit_segment_wise_email_read_page" path="/template/common_template.jsp">
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
			   <set-property property="rightFrame" value="/segmentWiseEmail/maker_edit_segment_wise_email_read.jsp"/>
            </forward>
            
            <forward name="maker_delete_segment_wise_email_page" path="/template/common_template.jsp">
               <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
			   <set-property property="rightFrame" value="/segmentWiseEmail/maker_delete_segment_wise_email.jsp"/>
            </forward>
            
             <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
             <forward name="maker_prepare_resubmit_edit_segment_wise_email_page" path="/template/common_template.jsp">
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
			   <set-property property="rightFrame" value="/segmentWiseEmail/maker_resubmit_edit_segment_wise_email.jsp"/>
            </forward>
            
             <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/segmentWiseEmail/maker_resubmit_common.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
            
            <forward name="checker_list_segment_wise_email_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/segmentWiseEmail/maker_list_segment_wise_email.jsp"/>
                <set-property property="title" value="menu.manual_feeds.segment_wise_email.name"/>
            </forward>
        </action>
        </action-mappings>
</struts-config>
