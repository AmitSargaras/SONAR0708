            </forward>
            <forward name="maker_list_systemBankBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBankBranch/maker_list_systemBankBranch.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBankBranch"/>
            </forward>
            <forward name="search_systemBankBranch_page" path="/template/common_template.jsp">
