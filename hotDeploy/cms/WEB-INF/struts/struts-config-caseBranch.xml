<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainCaseBranchForm" type="com.integrosys.cms.ui.caseBranch.MaintainCaseBranchForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/caseBranch" type="com.integrosys.cms.ui.caseBranch.MaintainCaseBranchAction" name="MaintainCaseBranchForm" validate="false" scope="request">
            <forward name="get_list_caseBranch_page" path="/caseBranch.do?event=redirect_list_caseBranch">
            </forward>
            <forward name="maker_list_caseBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_list_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="checker_list_caseBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/checker_list_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="search_caseBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/search_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>            
            <forward name="maker_add_edit_caseBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_add_edit_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_view_delete_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="prepare_create_caseBranch" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_add_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_read_delete_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_read_save_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="maker_prepare_resubmit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_resubmit_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
             <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_resubmit_delete_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
             <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_close_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
             <forward name="maker_view_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_view_todo_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            
