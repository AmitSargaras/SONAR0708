            </forward>
             <forward name="checker_systemBankBranch_insert_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/checker_caseBranch_insert.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
             <forward name="checker_caseBranch_insert_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/checker_caseBranch_insert.jsp"/>
                <set-property property="title"  value="menu.caseCput.name"/>
            </forward>
             <forward name="maker_prepare_insert_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_insert_close_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="maker_fileupload_caseBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/maker_fileupload_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
        </action>
		
    </action-mappings>
    
    <message-resources key="message-caseBranchteam" parameter="ApplicationResources-caseBranchteam"/>
</struts-config>