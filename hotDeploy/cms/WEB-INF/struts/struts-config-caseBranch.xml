            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="checker_caseBranch_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/caseBranch/checker_caseBranch.jsp"/>
                <set-property property="title" value="menu.caseCput.name"/>
            </forward>
            <forward name="rejected_read_page" path="/caseBranch/forward.jsp"/>
