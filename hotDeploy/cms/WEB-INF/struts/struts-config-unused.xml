                <set-property property="leftFrame" value="/template/cpc_maker_leftframe.jsp"/>
                <set-property property="title" value="title.generate.req.waiver"/>
            </forward>
			<forward name="after_generate_req_waiver_WF" path="/template/common_template.jsp">
