        <action path="/Workspace" type="com.integrosys.cms.ui.workspace.WorkspaceAction"
            name="WorkspaceForm" validate="false" scope="request">
            <forward name="after_prepare" path="/template/cpc_cust_checker_template.jsp">
                <set-property property="title" value="cpc.cust.checker.title"/>
                <set-property property="rightFrame" value="/workspace/workspace_list_do.jsp"/>
            </forward>
        </action>
        
