<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="ReleaselinedetailsUploadForm" type="com.integrosys.cms.ui.releaselinedetailsupload.ReleaselinedetailsUploadForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/Releaselinedetailsupload" type="com.integrosys.cms.ui.releaselinedetailsupload.ReleaselinedetailsUploadAction" name="ReleaselinedetailsUploadForm" validate="false" scope="request">
            <forward name="excelupload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/upload_releaselinedetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward>
            <forward name="releaselinedetailsupload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/list_releaselinedetails_upload.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward>
            
             <forward name="downloadReport" path="/file/popup.jsp" />
             <forward name="checker_approve_create_releaselinedetails" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/chk_approve_releaselinedetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
            <forward name="checker_reject_create_releaselinedetails" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/checker_reject_create_releaselinedetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
            <forward name="maker_prepare_releaselinedetails_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/maker_prepare_releaselinedetails_close.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
            <forward name="checker_process_create_releaselinedetails" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/checker_process_create_releaselinedetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
            <forward name="error_reject_releaselinedetails" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/checker_process_create_releaselinedetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
            <forward name="maker_close_releaselinedetails_file" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/maker_close_releaselinedetails_file.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
            <forward name="error_excelupload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/upload_releaselinedetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.releaselinedetailsupload"/>
            </forward> 
             <forward name="rejected_delete_read" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/releaselinedetailsupload/process_releaselinedetails_file.jsp"/>
                <set-property property="title" value="menu.maual_feeds.file_upload"/>
            </forward>
            
            
        </action>
		
    </action-mappings>
    
    <message-resources key="message-uploadteam" parameter="ApplicationResources-upload"/>
</struts-config>