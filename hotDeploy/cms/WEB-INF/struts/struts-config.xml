<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>

    <!-- ========== Form Mapping Definitions ==============================  -->

    <form-beans type="org.apache.struts.action.ActionFormBean">

        <form-bean name="LoginForm" type="com.integrosys.cms.ui.login.LoginRoleForm">
        </form-bean>
        
        <form-bean name="ChangeRoleForm" type="com.integrosys.cms.ui.role.ChangeRoleForm">
		</form-bean>

        <form-bean name="ForceChangePasswordForm" type="com.integrosys.component.login.ui.ChangePasswordForm">
        </form-bean>

        <form-bean name="PasswordChangeForm" type="com.integrosys.cms.ui.profile.PasswordChangeForm">
        </form-bean>

        <form-bean name="WorkspaceForm" type="com.integrosys.cms.ui.workspace.WorkspaceForm">
        </form-bean>  
                
        <form-bean name="WelcomeForm" type="com.integrosys.cms.ui.welcome.WelcomeForm">
        </form-bean>
	
	
    </form-beans>
    
	<global-exceptions>
		<exception
			key="error.synchronization"
			handler="com.integrosys.base.uiinfra.common.CommonExceptionHandler"
			type="java.lang.Exception" />
	</global-exceptions>

    <!-- ========== Action Mapping Definitions ==============================   -->

    <global-forwards type="com.integrosys.base.uiinfra.common.IntegroActionForward">
        <forward name="error" path="/template/common_template.jsp" redirect="false">
            <set-property property="title" value="title.error"/>
            <set-property property="rightFrame" value="/system/system_error.jsp"/>
        </forward>
        <forward name="ui_error" path="/template/common_template.jsp" redirect="false">
            <set-property property="title" value="title.error"/>
            <set-property property="rightFrame" value="/system/ui_system_error.jsp"/>
        </forward>
        <forward name="command_error" path="/template/common_template.jsp" redirect="false">
            <set-property property="title" value="title.error"/>
            <set-property property="rightFrame" value="/system/command_system_error.jsp"/>
        </forward>
        <forward name="access_error" path="/template/common_template.jsp" redirect="false">
            <set-property property="title" value="title.error"/>
            <set-property property="rightFrame" value="/system/access_error.jsp"/>
        </forward>
        <forward name="badflow_error" path="/template/common_template.jsp" redirect="false">
            <set-property property="title" value="title.error"/>
            <set-property property="rightFrame" value="/system/badflow_error.jsp"/>
        </forward>
        <!--<forward name="invalid_session_error" path="/system/invalid_session_error.jsp" redirect="false">-->
		<forward name="invalid_session_error" path="/login.jsp" redirect="false">
        </forward>
        <forward name="multi_login_error" path="/system/invalid_session.jsp" redirect="false">
        </forward>
        <forward name="concurrent_error" path="/template/common_template.jsp" redirect="false">
            <set-property property="title" value="title.error"/>
            <set-property property="rightFrame" value="/system/concurrentupdate.jsp"/>
        </forward>
        <forward name="change_pwd" path="/ForceChangePassword.do" redirect="true"/>
    </global-forwards>

    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/ToDo" type="com.integrosys.cms.ui.todo.TodoAction"
            name="WorkspaceForm" validate="false" scope="request">

            <forward name="customertodo" path="/template/common_view_template.jsp">
                <set-property property="title" value="title.todo.customer"/>
                <set-property property="rightFrame" value="/todo/customertodo.jsp"/>
            </forward>

            <forward name="normaltodo" path="/template/common_template.jsp">
                <set-property property="title" value="title.todo.page"/>
                <set-property property="rightFrame" value="/todo/todo_list.jsp"/>
            </forward>
            
             <forward name="prepare_discrepancy" path="/template/common_template.jsp">
                <set-property property="title" value="title.todo.discrepency.page"/>
                <set-property property="rightFrame" value="/todo/todo_list_discrepancy.jsp"/>
            </forward>
            
             <forward name="totrack_discrepancy" path="/template/common_template.jsp">
                <set-property property="title" value="title.todo.discrepency.page"/>
                <set-property property="rightFrame" value="/todo/todo_list_discrepancy.jsp"/>
            </forward>
            
            <forward name="pending_perfection_credit_folder" path="/template/common_template.jsp">
                <set-property property="title" value="title.todo.page"/>
                <set-property property="rightFrame" value="/todo/todo_pending_perfection_credit_folder.jsp"/>
            </forward>

            <forward name="pending_cases" path="/template/common_template.jsp">
            	<set-property property="title" value="title.todo.page"/>
            	<set-property property="rightFrame" value="/todo/todo_pending_cases.jsp"/>
            </forward>

            <forward name="reassign_pending_cases" path="/template/common_template.jsp">
            	<set-property property="title" value="title.todo.page"/>
            	<set-property property="rightFrame" value="/todo/ack_reassign.jsp"/>
            </forward>
            
            <forward name="refresh_pending_cases" path="/todo/refresh_filter_list.jsp" />   

            <forward name="reenter" path="/login.jsp"/>
        </action>

        <action path="/SSLLogin" type="com.integrosys.cms.ui.login.SSLLoginAction" name="LoginForm" scope="request">
            <forward name="pre_verify" path="/ssl_verify.jsp" redirect="false"/>
            <!--<forward name="welcome" path="/cms/Welcome.do?event=prepare" redirect="true"/>-->
            <!--<forward name="welcome" path="/Welcome.do"/>-->
            <forward name="reenter" path="/login.jsp"/>
            <forward name="change_pwd" path="/ForceChangePassword.do"/>
        </action>

        <action path="/Login" type="com.integrosys.cms.ui.login.LoginAction" name="LoginForm" scope="request">
            <!--<forward name="welcome" path="/Welcome.do"/>-->
            <forward name="welcome" path="Welcome.do?event=prepare" redirect="true"/>
            <forward name="reenter" path="/login.jsp"/>
            <forward name="change_pwd" path="/ForceChangePassword.do"/>
        </action>

        <action path="/ChangeRole" type="com.integrosys.cms.ui.role.ChangeRoleAction" name="ChangeRoleForm" scope="request">
            <!--<forward name="welcome" path="/Welcome.do"/>-->
            <forward name="welcome" path="Welcome.do?event=prepare" redirect="true"/>
            <forward name="reenter" path="/login.jsp"/>
            <forward name="change_pwd" path="/ForceChangePassword.do"/>
		</action>

        <action path="/ForceChangePassword" type="com.integrosys.cms.ui.login.ForceChangePasswordAction" name="ForceChangePasswordForm" scope="request">
            <forward name="reenter" path="/security/change_pwd.jsp"/>
            <forward name="proceed" path="/security/change_pwd_finished.jsp"/>
        </action>

        <action path="/PasswordChange" type="com.integrosys.cms.ui.profile.PasswordChangeAction" name="PasswordChangeForm" scope="request">
            <forward name="after_prepare" path="/security/change_login_password.jsp"/>
            <forward name="show_confirm" path="/security/change_login_password_confirm.jsp"/>
        </action>

        <action path="/Logout" type="com.integrosys.component.login.ui.LogoutAction" name="LoginForm" scope="request">
            <forward name="LOSUSER" path="/login.jsp"/>
        </action>

        <action path="/Welcome" type="com.integrosys.cms.ui.welcome.WelcomeAction" name="WelcomeForm" scope="request">
            <forward name="prepare" path="/template/common_template.jsp">
                <set-property property="title" value="title.welcome.page"/>
                <set-property property="rightFrame" value="/welcome/common_welcome.jsp"/>
            </forward>
            <forward name="loadcount" path="/welcome/load_count.jsp"/>
            <!--
            <forward name="reenter" path="/login.jsp"/>
            -->
        </action>            	

    </action-mappings>
    
	<controller>
		<set-property property="processorClass" value="org.springframework.web.struts.DelegatingRequestProcessor"/>
	</controller>
    
    <message-resources key="message-main" parameter="ApplicationResources"/>
    <message-resources key="message-menu" parameter="ApplicationResources-menu"/>

