<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="CollateralRocForm" type="com.integrosys.cms.ui.collateralrocandinsurance.CollateralRocForm">
        </form-bean>
    </form-beans>
    
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/collateralrocandinsurance" type="com.integrosys.cms.ui.collateralrocandinsurance.CollateralRocAction" name="CollateralRocForm" validate="false" scope="request">
  
            <forward name="list_collateral_roc_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_list_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="maker_prepare_create_collateral_roc" path="/template/common_template.jsp">
			<set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
			<set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_create_collateral_roc.jsp"/>
			</forward>
            
            <forward name="refresh_collateral_code" path="/collateralRocAndInsuranceMapping/collateralCodeList.jsp"/>
            
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="checker_collateral_roc_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/checker_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="common_approve_page" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/common/ack_approve.jsp"/>
				<set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
	  	    </forward>
            
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
               <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="maker_add_edit_collateral_roc_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_add_edit_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
             <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_view_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_read_delete_collateral_roc.jsp"/>
              <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_read_save_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
             <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_resubmit_delete_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
             <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/maker_close_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
             <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
            <forward name="checker_list_collateral_roc_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/collateralRocAndInsuranceMapping/checker_list_collateral_roc.jsp"/>
                <set-property property="title" value="menu.manual_feeds.collateral_roc_and_insurance_mapping.name"/>
            </forward>
            
        </action>
        </action-mappings>
</struts-config>
