<binding name="sibs_collateral_charge_res">
    <include path="sibs_header_res.xml" />
    <mapping name="ColumnFields" class="com.integrosys.simulator.host.SIBS.collateralcharge.OBSIBSCollateralCharge" extends="com.integrosys.simulator.host.SIBS.OBMessageHeader"> 
	        <structure map-as="com.integrosys.simulator.host.SIBS.OBMessageHeader"/> 
				<collection field="responseFixList">
	        	<structure name="ColumnField" type="com.integrosys.simulator.host.SIBS.collateralcharge.OBSIBSCollateralCharge">
					<value name="applicationNo" field="applicationNo" />
					<value name="facilityCode" field="facilityCode" />
					<value name="accountSeq" field="accountSeq" />
					<value name="collateralID" field="collateralID" />
					<value name="chargeType" field="chargeType" />
					<value name="chargeAmt" field="chargeAmt" />
					<value name="chargePresentationNo" field="chargePresentationNo" />
					<value name="chargePresentationDate" field="chargePresentationDate" format="dateFmt"/>
					<value name="chargeRegisteredDate" field="chargeRegisteredDate" format="dateFmt"/>
					<value name="folio" field="folio" />
					<value name="jilid" field="jilid" />
					<value name="partyChargeInd" field="partyChargeInd" />
			     </structure>
		     </collection>
    </mapping> 	
</binding>						