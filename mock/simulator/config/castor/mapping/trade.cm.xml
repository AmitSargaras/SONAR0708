<?xml version="1.0"?>

<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<description>TradeSpring Mapping</description>
	<include href="common-stpheader.cm.xml" />
	                                          
  <class extends="com.integrosys.simulator.host.stp.STPMessage" name="com.integrosys.simulator.host.stp.trade.TradeMessage">
  	<map-to xml="TestMessage"/>
	</class> 
  
  <class name="com.integrosys.simulator.host.stp.trade.TradeBody" extends="com.integrosys.simulator.host.stp.STPBody">
		<field name="id" type="string"> 
      <bind-xml name="ID" node="element"/>
    </field>
		<field name="cdtFileId" type="string">
		  <bind-xml name="CDT_FILE_ID" node="element"/>
    </field>
		<field name="accountNo" type="string">
		  <bind-xml name="ACCT_NO" node="element"/>
    </field>
    <field name="limitNo" type="string">
		  <bind-xml name="LIMIT_NO" node="element"/>
    </field>
		<field name="firstLineFacilityId" type="string">
		  <bind-xml name="FIRST_LINE_FAC_ID" node="element"/>
    </field>
		<field name="mainLineFacilityId" type="string">
		  <bind-xml name="MAIN_LINE_FAC_ID" node="element"/>
    </field>
		<field name="mtFacilityCode" type="string">
		  <bind-xml name="MT_FAC_CD" node="element"/>
    </field>
		<field name="mtBrCode" type="string">
		  <bind-xml name="MT_BR_CD" node="element"/>
    </field>
		<field name="mtCurrencyCode" type="string">
		  <bind-xml name="MT_CUR_CD" node="element"/>
    </field>
		<field name="approvedLimit" type="string">
		  <bind-xml name="LMT_APPR" node="element"/>
    </field>
		<field name="mtTenureTimeCode" type="string">
		  <bind-xml name="TENURE_MT_TIME_CD" node="element"/>
    </field>
		<field name="approvedTenure" type="string">
		  <bind-xml name="TENURE_APPR" node="element"/>
    </field>
		<field name="reviewDt" type="string" handler="com.integrosys.simulator.host.stp.core.CustomDateFieldHandler">
		  <bind-xml name="DT_REVIEW" node="element"/>
    </field>
		<field name="tempFlag" type="string">
		  <bind-xml name="IS_TEMP" node="element"/>
    </field>
		<field name="createBy" type="string">
		  <bind-xml name="CREATED_BY" node="element"/>
    </field>
		<field name="updateBy" type="string">
		  <bind-xml name="UPDATED_BY" node="element"/>
    </field>
		<field name="createdDt" type="string" handler="com.integrosys.simulator.host.stp.core.CustomDateFieldHandler">
		  <bind-xml name="DT_CREATED" node="element"/>
    </field>
		<field name="updatedDt" type="string" handler="com.integrosys.simulator.host.stp.core.CustomDateFieldHandler">
		  <bind-xml name="DT_UPDATED" node="element"/>
    </field>
		<field name="version" type="long">
		  <bind-xml name="VERSION" node="element"/>
    </field>
		<field name="intRate" type="string">
		  <bind-xml name="INT_RATE" node="element"/>
    </field>
		<field name="mtIntRateCode" type="string">
		  <bind-xml name="MT_INT_RATE_CD" node="element"/>
    </field>
		<field name="expiryDt" type="string" handler="com.integrosys.simulator.host.stp.core.CustomDateFieldHandler">
		  <bind-xml name="DT_EXPIRY" node="element"/>
    </field>
		<field name="specialRemark" type="string" handler="com.integrosys.simulator.host.stp.core.ClobFieldHandler">
		  <bind-xml name="SPECIAL_REMARK" node="element"/>
    </field>
		<field name="mtMaintStsCode" type="string">
		  <bind-xml name="MT_MAINT_STS_CD" node="element"/>
    </field>
		<field name="penaltyIntRate" type="string">
		  <bind-xml name="PEN_INT_RATE" node="element"/>
    </field>
		<field name="mtPenaltyIntRateCode" type="string">
		  <bind-xml name="MT_PEN_INT_RATE_CD" node="element"/>
    </field>
		<field name="odRate" type="string">
		  <bind-xml name="OD_RATE" node="element"/>
    </field>                                     
		<field name="mtOdRateCode" type="string">
		  <bind-xml name="MT_OD_RATE_CD" node="element"/>
    </field>
		<field name="autoPurgeFlag" type="string">
		  <bind-xml name="IS_AUTO_PURGE" node="element"/>
    </field>
		<field name="mtFacilityId" type="string">
		  <bind-xml name="MT_FAC_ID" node="element"/>
    </field>
		<field name="lsmCode" type="string">
		  <bind-xml name="LSM_CD" node="element"/>
    </field>
		<field name="rbsPurposeCode" type="string">
		  <bind-xml name="PPS_OF_LOAN" node="element"/>
    </field>
		<field name="limitDesc" type="string">
		  <bind-xml name="LIMIT_DESC" node="element"/>
    </field>
		<field name="revolvingFlag" type="string">
		  <bind-xml name="IS_REVOLVING" node="element"/>
    </field>
		<field name="cgcFlag" type="string">
		  <bind-xml name="IS_CGC" node="element"/>
    </field>
		<field name="sectorFlag" type="string">
		  <bind-xml name="IS_SECTOR" node="element"/>
    </field>
		<field name="cgcSchema" type="string">
		  <bind-xml name="CGC_SCHEMA" node="element"/>
    </field>
		<field name="cgcRefNo" type="string">
		  <bind-xml name="CGC_REF_NO" node="element"/>
    </field>
		<field name="commissionRate" type="string">
		  <bind-xml name="COM_RATE" node="element"/>     
    </field>
		<field name="delFlag" type="string">
		  <bind-xml name="IS_DEL" node="element"/>
    </field>
		<field name="approvedDt" type="string" handler="com.integrosys.simulator.host.stp.core.CustomDateFieldHandler"> 
		  <bind-xml name="APPROVED_DATE" node="element"/>
    </field>
		<field name="mtrFlag" type="string">
		  <bind-xml name="IS_MTR" node="element"/>
    </field>
		<field name="mbgFlag" type="string">
		  <bind-xml name="IS_MBG" node="element"/>
    </field>
	</class>
  
</mapping>