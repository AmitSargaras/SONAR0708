<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>udf</shortname>
	<tag>
		<name>udfDisplay</name>
		<tagclass>com.integrosys.cms.ui.udf.tag.UdfTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>Tag to support page indexing</info>
		<attribute>
			<name>module</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>action</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>refId</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>refId2</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>oldUdf</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>udfDisplay2</name>
		<tagclass>com.integrosys.cms.ui.udf.tag.UdfTag2</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>Tag to support page indexing</info>
		<attribute>
			<name>module</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>action</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>refId</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>refId2</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>oldUdf</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
