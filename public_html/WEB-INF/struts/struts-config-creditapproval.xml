<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
	<form-beans type="org.apache.struts.action.ActionFormBean">
		

		<!-- Credit Approval list ActionForm. -->
		<form-bean name="creditApprovalForm"
			type="com.integrosys.cms.ui.creditApproval.CreditApprovalForm" />

	</form-beans>

	<action-mappings
		type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
		

		<!--Credit Approval item Action. -->
		<action path="/credit_approval"
			type="com.integrosys.cms.ui.creditApproval.CreditApprovalAction"
			name="creditApprovalForm" validate="false" scope="request">

			<forward name="list" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/list.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="prepare"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/prepare.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_prepare_submit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_view_credit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/view.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="checker_confirm_approve_create"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_approve.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_view_approve_reject_credit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/checker_credit_approval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_view_approve_update_credit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/checker_credit_approval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="view" path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/checker_credit_approval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="event_reject_create"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_reject.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="event_reject_update"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_reject.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="view_reject"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/checker_credit_approval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="view_reject_close"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/checker_credit_approval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_confirm_close"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_view_creditApproval_read"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/maker_view_creditApproval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="checker_view_creditApproval_read"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/maker_view_creditApproval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="checker_view_creditApproval_read"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/maker_view_creditApproval.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="prepare_maker_submit_edit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/prepare_edit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_confirm_resubmit_edit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="view_edit_reject_resubmit"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/prepare_edit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_confirm_resubmit_edit_rejected"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="prepare_maker_submit_remove"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/creditApproval/view_edit_delete.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="confirm_maker_submit_remove"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="wip"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/wip.html" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="common_submit_page"
				path="/template/common_template.jsp">
				<set-property property="rightFrame"
					value="/common/ack_submit.jsp" />
				<set-property property="title"
					value="title.manual.update.creditapproval" />
			</forward>
			<forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/creditApproval/maker_close_credit_approval.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
            
            
            <forward name="prepare_upload_creditApproval_page" path="/template/common_template.jsp">
				<set-property property="rightFrame" value="/creditApproval/maker_upload_creditApproval.jsp"/>
			 <set-property property="title" value="title.manual.update.creditapproval" />
 			</forward>
			<forward name="maker_view_insert_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/creditApproval/maker_view_insert_todo_creditApproval.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
             <forward name="checker_creditApproval_insert_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/creditApproval/checker_creditApproval_insert.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
             <forward name="maker_prepare_insert_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/creditApproval/maker_insert_close_creditApproval.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
            <forward name="maker_fileupload_creditApproval_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/creditApproval/maker_fileupload_creditApproval.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>
             <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.manual.update.creditapproval" />
            </forward>

		</action>
	</action-mappings>

	<message-resources key="message-creditapproval"
		parameter="ApplicationResources-creditapproval" />
</struts-config>