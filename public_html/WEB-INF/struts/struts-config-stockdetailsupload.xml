<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="StockDetailsUploadForm" type="com.integrosys.cms.ui.stockdetailsupload.StockDetailsUploadForm">
        </form-bean>
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/Stockdetailsupload" type="com.integrosys.cms.ui.stockdetailsupload.StockDetailsUploadAction" name="StockDetailsUploadForm" validate="false" scope="request">
            <forward name="prepare_excelupload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/upload_stockdetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="error_excelupload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/upload_stockdetails.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="excelupload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/list_stockdetails_upload.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="download_report" path="/file/popup.jsp" />
             
            <forward name="checker_prepare_process" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/checker_prepare_process.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="checker_reject" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/checker_reject.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="checker_reject_error" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/checker_prepare_process.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/maker_prepare_close.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="maker_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/maker_close.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
            
            <forward name="checker_approve" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/stockdetailsupload/list_stockdetails_upload.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.stockdetailsupload"/>
            </forward>
             
        </action>
    </action-mappings>
    
    <message-resources key="message-uploadteam" parameter="ApplicationResources-upload"/>
</struts-config>