<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="LimitBookingForm" type="com.integrosys.cms.ui.limitbooking.LimitBookingForm">
        </form-bean>        
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/LimitBooking" type="com.integrosys.cms.ui.limitbooking.LimitBookingAction" name="LimitBookingForm" scope="request">
          <forward name="prepare_add_booking" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_limitbooking.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="prepare_edit_booking" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_limitbooking.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="retrieve_bgel" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_limitbooking.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="prepare_search_group" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/search_group.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="search_group" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/search_group_result.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="add_group" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_limitbooking.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="prepare_add_pol" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_pol.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="prepare_edit_pol" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_pol.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           
           <forward name="add" path="/LimitBooking.do?event=prepare_add_booking"/>
           <forward name="edit" path="/LimitBooking.do?event=prepare_edit_booking"/>
            
           <forward name="view_result_add" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/view_limitbooking_result.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="view_result_edit" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/view_limitbooking_result.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="add_booking" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/confirm_limitbooking.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
          
           <forward name="view_cancel_result" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/maker_add_edit_limitbooking.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="list" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/limitbooking/limitbooking_list.jsp"/>
               <set-property property="title" value="title.limit.booking"/>
           </forward>
           <forward name="prepare_search_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitbooking/search_limitbooking.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="search_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitbooking/search_result_limitbooking.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="search_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitbooking/search_result_limitbooking.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="prepare_delete_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitbooking/maker_delete_successful_limitbooking.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="prepare_successful_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitbooking/maker_delete_successful_limitbooking.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="edit_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitbooking/confirm_limitbooking.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="delete_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
             <forward name="successful_booking" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.limit.booking"/>
            </forward>
            <forward name="limit_booking_popup" path="/limitbooking/view_limitbooking_popup.jsp"/>
        </action>		

    </action-mappings>
    
    <message-resources key="message-limitbooking" parameter="ApplicationResources-limitbooking"/>
</struts-config>