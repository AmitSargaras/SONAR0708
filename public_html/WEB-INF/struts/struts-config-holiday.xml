<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainHolidayForm" type="com.integrosys.cms.ui.holiday.MaintainHolidayForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/holiday" type="com.integrosys.cms.ui.holiday.MaintainHolidayAction" name="MaintainHolidayForm" validate="false" scope="request">
            <forward name="get_list_holiday_page" path="/holiday.do?event=redirect_list_holiday">
            </forward>
            <forward name="maker_list_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_list_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="checker_list_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/checker_list_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="search_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/search_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>            
            <forward name="maker_add_edit_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_add_edit_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_view_delete_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="prepare_create_holiday" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_add_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_read_delete_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_read_save_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="maker_prepare_resubmit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_resubmit_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
             <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_resubmit_delete_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
             <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_close_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
             <forward name="maker_view_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_view_todo_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="checker_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/checker_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="rejected_read_page" path="/holiday/forward.jsp"/>
            <forward name="prepare_upload_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_upload_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="maker_view_insert_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_view_insert_todo_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
             <forward name="checker_systemBankBranch_insert_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/checker_holiday_insert.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
             <forward name="checker_holiday_insert_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/checker_holiday_insert.jsp"/>
                <set-property property="title"  value="title.sys.admin.maint.holiday"/>
            </forward>
             <forward name="maker_prepare_insert_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_insert_close_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
            <forward name="maker_fileupload_holiday_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/holiday/maker_fileupload_holiday.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.holiday"/>
            </forward>
        </action>
		
    </action-mappings>
    
    <message-resources key="message-holidayteam" parameter="ApplicationResources-holidayteam"/>
</struts-config>