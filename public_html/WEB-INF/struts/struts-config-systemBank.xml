<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainSystemBankForm" type="com.integrosys.cms.ui.systemBank.MaintainSystemBankForm">
        </form-bean>

       
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/systemBank" type="com.integrosys.cms.ui.systemBank.MaintainSystemBankAction" name="MaintainSystemBankForm" validate="false" scope="request">
            <forward name="get_list_systemBank_page" path="/systemBank.do?event=redirect_list_systemBank">
            </forward>
            <forward name="maker_list_systemBank_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_list_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="search_systemBank_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/search_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>            
            <forward name="maker_add_edit_systemBank_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_add_edit_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_view_delete_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="checker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_view_delete_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="maker_view_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_view_todo_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
             <forward name="maker_prepare_resubmit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_resubmit_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_close_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="maker_confirm_close" path="/template/common_template.jsp">
            <set-property property="rightFrame" value="/common/ack_close.jsp"/>
            <set-property property="title" value="title.sys.admin.maint.systemBank"/>
        </forward>
        <forward name="maker_view_save_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/maker_read_save_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="checker_reject_edit" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="checker_systemBank_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemBank/checker_systemBank.jsp"/>
                <set-property property="title" value="title.sys.admin.maint.systemBank"/>
            </forward>
            <forward name="rejected_read_page" path="/systemBank/forward.jsp"/>
            
            <forward name="refresh_region_id" path="/systemBank/refresh_maker_create_region.jsp"/>
            
            <forward name="refresh_state_id" path="/systemBank/refresh_maker_create_state.jsp"/>
            
               <forward name="refresh_city_id" path="/systemBank/refresh_maker_create_city.jsp"/>
        </action>
		
    </action-mappings>
    
    <message-resources key="message-systemBankteam" parameter="ApplicationResources-systemBankteam"/>
</struts-config>