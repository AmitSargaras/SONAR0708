<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="CommodityMainForm" type="com.integrosys.cms.ui.collateral.commodity.CommodityMainForm">
        </form-bean>

        <form-bean name="ApprovedCommForm" type="com.integrosys.cms.ui.collateral.commodity.approvedcommodity.ApprovedCommForm">
        </form-bean>

        <form-bean name="ContractForm" type="com.integrosys.cms.ui.collateral.commodity.contract.ContractForm">
        </form-bean>

        <form-bean name="HedgedConForm" type="com.integrosys.cms.ui.collateral.commodity.hedgedcontract.HedgedConForm">
        </form-bean>

        <form-bean name="CommSecForm" type="com.integrosys.cms.ui.collateral.commodity.security.SecurityForm">
        </form-bean>

        <form-bean name="LoanAgencyForm" type="com.integrosys.cms.ui.collateral.commodity.loanagency.LoanAgencyForm">
        </form-bean>

        <form-bean name="LoanScheduleForm" type="com.integrosys.cms.ui.collateral.commodity.loanagency.loanschedule.LoanScheduleForm">
        </form-bean>

        <form-bean name="SubLimitListForm" type="com.integrosys.cms.ui.collateral.commodity.sublimit.list.SubLimitListForm">
        </form-bean>

        <form-bean name="SubLimitItemForm" type="com.integrosys.cms.ui.collateral.commodity.sublimit.item.SubLimitItemForm">
        </form-bean>    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
	    <action path="/CommodityMain"
	        type="com.integrosys.cms.ui.collateral.commodity.CommodityMainAction"
	        name="CommodityMainForm" validate="false" scope="request">
	        <forward name="read" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_read.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
			<forward name="prepare_create" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_update.jsp"/>
	            <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
			    <set-property property="title" value="title.security"/>
			</forward>
	        <forward name="prepare_update" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_update.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_process.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="prepare_close" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_view.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="track" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_view.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="summary" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/Commodity_exposure_summary.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="submit" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="update" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_save.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="approve" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="reject" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="close" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/ack_close.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="wip" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/common/wip.html"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="update_approved_commodity" path="/ApprovedComm.do?event=prepare_update_sub"></forward>
	        <forward name="update_contract" path="/Contract.do?event=prepare_update_sub"></forward>
	        <forward name="update_hedged_contract" path="/HedgedContract.do?event=prepare_update_sub"></forward>
	        <forward name="update_security" path="/CommoditySec.do?event=prepare_update_sub"></forward>
	        <forward name="update_loan_agency" path="/LoanAgency.do?event=prepare_update_sub"></forward>
	        <forward name="add_approved_commodity" path="/ApprovedComm.do?event=prepare"></forward>
	        <forward name="add_contract" path="/Contract.do?event=prepare"></forward>
	        <forward name="add_hedged_contract" path="/HedgedContract.do?event=prepare"></forward>
	        <forward name="add_loan_agency" path="/LoanAgency.do?event=prepare"></forward>
	        <forward name="view_approved_commodity" path="/ApprovedComm.do?event=read"></forward>
	        <forward name="view_contract" path="/Contract.do?event=read"></forward>
	        <forward name="view_hedged_contract" path="/HedgedContract.do?event=read"></forward>
	        <forward name="view_security" path="/CommoditySec.do?event=read"></forward>
	        <forward name="view_loan_agency" path="/LoanAgency.do?event=read"></forward>
	        <forward name="view_sub_limit" path="/CommoditySubLimit.do?event=view"></forward>
	    </action>
    
	    <action path="/ApprovedComm"
	        type="com.integrosys.cms.ui.collateral.commodity.approvedcommodity.ApprovedCommAction"
	        name="ApprovedCommForm" validate="false" scope="request">
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/approvedcommodity/update_approvedcomm.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="view_approvedCommodity" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/approvedcommodity/view_approvedcomm.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process_approvedCommodity" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/approvedcommodity/chk_view_approvedcomm.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="close_return" path="/CommodityMain.do?event=close_return"></forward>
	        <forward name="track_return" path="/CommodityMain.do?event=track_return"></forward>
	        <forward name="process_return" path="/CommodityMain.do?event=process_return"></forward>
	        <forward name="update_return" path="/CommodityMain.do?event=update_return"></forward>
	        <forward name="read_return" path="/CommodityMain.do?event=read_return"></forward>
	    </action>
	    
	    <action path="/Contract"
	        type="com.integrosys.cms.ui.collateral.commodity.contract.ContractAction"
	        name="ContractForm" validate="false" scope="request">
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/contract/update_contract.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="view_contract" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/contract/view_contract.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process_contract" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/contract/chk_view_contract.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="close_return" path="/CommodityMain.do?event=close_return"></forward>
	        <forward name="track_return" path="/CommodityMain.do?event=track_return"></forward>
	        <forward name="process_return" path="/CommodityMain.do?event=process_return"></forward>
	        <forward name="update_return" path="/CommodityMain.do?event=update_return"></forward>
	        <forward name="read_return" path="/CommodityMain.do?event=read_return"></forward>
	    </action>
	    
	    <action path="/HedgedContract"
	        type="com.integrosys.cms.ui.collateral.commodity.hedgedcontract.HedgedConAction"
	        name="HedgedConForm" validate="false" scope="request">
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/hedgedcontract/update_hedgedcon.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="view_hedgedContract" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/hedgedcontract/view_hedgedcon.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process_hedgedContract" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/hedgedcontract/chk_view_hedgedcon.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="close_return" path="/CommodityMain.do?event=close_return"></forward>
	        <forward name="track_return" path="/CommodityMain.do?event=track_return"></forward>
	        <forward name="process_return" path="/CommodityMain.do?event=process_return"></forward>
	        <forward name="update_return" path="/CommodityMain.do?event=update_return"></forward>
	        <forward name="read_return" path="/CommodityMain.do?event=read_return"></forward>
	    </action>
	    
	    <action path="/CommoditySec"
	        type="com.integrosys.cms.ui.collateral.commodity.security.SecurityAction"
	        name="CommSecForm" validate="false" scope="request">
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/update_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="prepare_apportion" path="/CommoditySecurityApportionment.do?event=prepare" />
	        <forward name="prepare_update_sub_apportion" path="/CommoditySecurityApportionment.do?event=prepare_update_sub" />
	        <forward name="itemDelete" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/update_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="view_security" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/view_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process_security" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/chk_view_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="close_return" path="/CommodityMain.do?event=close_return"></forward>
	        <forward name="track_return" path="/CommodityMain.do?event=track_return"></forward>
	        <forward name="process_return" path="/CommodityMain.do?event=process_return"></forward>
	        <forward name="update_return" path="/CommodityMain.do?event=update_return"></forward>
	        <forward name="read_return" path="/CommodityMain.do?event=read_return"></forward>
	        <forward name="close_return_apportion" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/view_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="read_return_apportion" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/view_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="track_return_apportion" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/view_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process_return_apportion" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/chk_view_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="update_return_apportion" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/security/update_security.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	    </action>
	    
	    <action path="/LoanAgency"
	        type="com.integrosys.cms.ui.collateral.commodity.loanagency.LoanAgencyAction"
	        name="LoanAgencyForm" validate="false" scope="request">
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/update_loan_agency.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="view_loan_agency" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/view_loan_agency.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process_loan_agency" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/chk_view_loan_agency.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="view_sec_doc" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/view_security_doc.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="limitProtection" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/ack_loan_agency_diff_bca_limit.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="edit_non_equal_payment" path="/LoanSchedule.do?event=prepare_update"></forward>
	        <forward name="view_non_equal_payment" path="/LoanSchedule.do?event=read"></forward>
	        <forward name="view_equal_payment" path="/LoanSchedule.do?event=read"></forward>
	        <forward name="close_return" path="/CommodityMain.do?event=close_return"></forward>
	        <forward name="track_return" path="/CommodityMain.do?event=track_return"></forward>
	        <forward name="process_return" path="/CommodityMain.do?event=process_return"></forward>
	        <forward name="update_return" path="/CommodityMain.do?event=update_return"></forward>
	        <forward name="read_return" path="/CommodityMain.do?event=read_return"></forward>
	    </action>
	    
	    <action path="/LoanSchedule"
	        type="com.integrosys.cms.ui.collateral.commodity.loanagency.loanschedule.LoanScheduleAction"
	        name="LoanScheduleForm" validate="false" scope="request">
	        <forward name="prepare" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/loanschedule/update_loan_schedule.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="read" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/loanschedule/view_loan_schedule.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/loanagency/loanschedule/chk_view_loan_schedule.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="return" path="/LoanAgency.do?event=return"></forward>
	        <forward name="print_equal_payment" path="/collateral/commodity/loanagency/loanschedule/view_loan_schedule.jsp"></forward>
	    </action>
	    
	    <action path="/CommoditySubLimit"
	        type="com.integrosys.cms.ui.collateral.commodity.sublimit.list.SubLimitListAction"
	        name="SubLimitListForm" validate="false" scope="request">
	        <forward name="update_sl_list" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/limitdetails/update_sublimit_list.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="prepare_update_item" path="/CmdtSubLimitItem.do?event=prepare_update_item"></forward>  
	        <forward name="update_return" path="/CommodityMain.do?event=update_return"></forward>
	        <forward name="sl_view" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/limitdetails/view_sublimit_list.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	         <forward name="ck_process_view_sl" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/limitdetails/ck_process_view_sl_list.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="mk_close_cmdt" path="/CommodityMain.do?event=close_return"></forward>        
	        <forward name="mk_view_cmdt" path="/CommodityMain.do?event=read_return"></forward>        
	        <forward name="ck_process_cmdt" path="/CommodityMain.do?event=process_return"></forward>
	        <forward name="track_cmdt" path="/CommodityMain.do?event=track_return"></forward>        
	    </action>    
	    
	    <action path="/CmdtSubLimitItem"
	        type="com.integrosys.cms.ui.collateral.commodity.sublimit.item.SubLimitItemAction"
	        name="SubLimitItemForm" validate="false" scope="request">
	        <forward name="prepare_update_item" path="/template/common_template.jsp">
	            <set-property property="rightFrame" value="/collateral/commodity/limitdetails/update_sublimit_item.jsp"/>
	            <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="return_update_item" path="/CommoditySubLimit.do?event=return_update_item"></forward>      
	    </action>
	    
	    <action path="/CommoditySecurityApportionment" 
	     type="com.integrosys.cms.ui.collateral.commodity.secapportion.SecApportionmentAction" 
	     name="SecurityApportionmentDtlForm" validate="false" scope="request">
	        <forward name="read" path="/template/common_template.jsp">
	           <set-property property="rightFrame" value="/collateral/commodity/security/view_apportionment.jsp"/>
	           <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="prepare" path="/template/common_template.jsp">
	           <set-property property="rightFrame" value="/collateral/commodity/security/update_apportionment.jsp"/>
	           <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="prepare_update_sub" path="/template/common_template.jsp">
	           <set-property property="rightFrame" value="/collateral/commodity/security/update_apportionment.jsp"/>
	           <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="refresh" path="/template/common_template.jsp">
	           <set-property property="rightFrame" value="/collateral/commodity/security/update_apportionment.jsp"/>
	           <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="process" path="/template/common_template.jsp">
	           <set-property property="rightFrame" value="/collateral/commodity/security/view_apportionment.jsp"/>
	           <set-property property="title" value="title.security"/>
	        </forward>
	        <forward name="return_update" path="/CommoditySec.do?event=update_return_apportion"/>
	        <forward name="return_read" path="/CommoditySec.do?event=read_return_apportion"/>
	        <forward name="return_process" path="/CommoditySec.do?event=process_return_apportion"/>
	        <forward name="return_prepare_close" path="/CommoditySec.do?event=close_return_apportion"/>
	        <forward name="return_track" path="/CommoditySec.do?event=track_return_apportion"/>
	    </action>	    
    </action-mappings>
    
