<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="SecurityColTaskForm" type="com.integrosys.cms.ui.seccoltask.SecurityColTaskForm">
        </form-bean>
    
        <form-bean name="CCColTaskForm" type="com.integrosys.cms.ui.cccoltask.CCColTaskForm">
        </form-bean>
        
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/SecurityColTask" type="com.integrosys.cms.ui.seccoltask.SecurityColTaskAction"
            name="SecurityColTaskForm" validate="false" scope="request">
            <forward name="info" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/no_task.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/listtask.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="wip_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="wip" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_read_task" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/create_task.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_read_task_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/create_task.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_close_item" path="/template/common_frame_template.jsp">
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>            
                <set-property property="rightFrame" value="/seccoltask/create_task_view.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>            
            <forward name="after_submit" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_submit_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_edit_staging_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/create_task.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_close_item_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/create_task_view.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_close_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_process_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/seccoltask/chk_create_task.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_approve_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
            <forward name="after_reject_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.sec.col.task"/>
            </forward>
        </action>

        <action path="/CCColTask" type="com.integrosys.cms.ui.cccoltask.CCColTaskAction"
            name="CCColTaskForm" validate="false" scope="request">
            <forward name="info" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/no_task.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_list" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/listtask.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_close_item" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/create_task_view.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>                
                <set-property property="title" value="title.cc.col.task"/>
            </forward>            
            <forward name="wip_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="wip" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_read_task" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/create_task.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_read_task_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/create_task.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_submit" path="/template/common_frame_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="leftFrame" value="/template/common_left_frame.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_submit_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_edit_staging_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/create_task.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_close_item_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/create_task_view.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_close_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_process_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/cccoltask/chk_create_task.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_approve_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
            <forward name="after_reject_WF" path="/template/common_view_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.cc.col.task"/>
            </forward>
        </action>    
    </action-mappings>
    
    <!--<message-resources parameter="ApplicationResources-collaborationtask"/>-->
</struts-config>