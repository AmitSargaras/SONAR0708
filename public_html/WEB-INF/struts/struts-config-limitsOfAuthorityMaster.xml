                <set-property property="title" value="menu.manual_feeds.limits_of_authority_master.name"/>
            </forward>
            <forward name="maker_delete_read" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitsOfAuthorityMaster/maker_delete_read.jsp"/>
