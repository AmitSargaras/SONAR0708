            <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/limitsOfAuthorityMaster/maker_close_limits_of_authority.jsp"/>
                <set-property property="title" value="menu.manual_feeds.limits_of_authority_master.name"/>
