<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
		<form-bean name="ImageUploadForm" type="com.integrosys.cms.ui.image.ImageUploadForm">
        </form-bean> 
        <form-bean name="imageUploadAddForm" type="com.integrosys.cms.ui.image.ImageUploadAddForm">
        </form-bean> 
    </form-beans>
<action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
            <action path="/ImageUpload" type="com.integrosys.cms.ui.image.ImageUploadAction" name="ImageUploadForm" validate="false" scope="request">
 				<forward name="image_upload" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/image/image_upload.jsp"/>
                 <set-property property="title" value="title.customer.search"/>
            </forward>
            <forward name="after_list_image" path="/template/common_template.jsp">
                <set-property property="title" value="title.customer.list"/>
                <set-property property="rightFrame" value="/image/ImageList.jsp"/>
            </forward>
            
            <forward name="list_upload_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/image/image_upload_details_page.jsp"/>
                 <set-property property="title" value="title.image.upload"/>
            </forward>
            <forward name="downloadImage" path="/image/popup.jsp" />
            <forward name="print_image" path="/image/print_image_page.jsp"/>
            
            <forward name="list" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/image/CustomerList.jsp"/>
                <set-property property="title" value="title.customer.search"/>
            </forward>
            
            <forward name="submit" path="/template/common_template.jsp">
               <set-property property="rightFrame" value="/image/image_upload.jsp"/>
               <set-property property="title" value="title.customer.search"/>
            </forward>
            
            <forward name="cancleFilter" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/image/image_upload.jsp"/>
               <set-property property="title" value="title.customer.search"/>
            </forward>
            
            <!--<forward name="list_upload_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/image/image_upload_page.jsp"/>
                 <set-property property="title" value="title.image.upload"/>
            </forward>
            --></action>
            
        <action path="/ImageUploadAdd" type="com.integrosys.cms.ui.image.ImageUploadAction" name="imageUploadAddForm" validate="false" scope="request">
				<!-- Anil Start -->
				<forward name="process_upload_request" path="/ProcessUploadRequest.do"/>
				<forward name="print_image" path="/image/print_image_page.jsp"/>
				<forward name="save_image_details" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/image/upload_images.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>
				<forward name="remove_images" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/image/image_upload.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>
				<forward name="list_upload_page" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/image/image_upload_details_page.jsp"/>
                 	<set-property property="title" value="title.image.upload"/>
            	</forward>
				<forward name="view_uploaded_image_listing" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/image/view_uploded_image_listing.jsp"/>
                 	<set-property property="title" value="title.image.upload"/>
            	</forward>
				<forward name="remove_uploaded_image_listing" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/image/remove_uploaded_image_listing.jsp"/>
                 	<set-property property="title" value="title.image.upload"/>
            	</forward>
				<!-- Anil End -->
				<forward name="doc_linkage_details" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/image/doc_linkage_details.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>
				<forward name="submit_doc_linkage" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/image/ack_doc_linkage_details.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>				
				<forward name="maker_view_delete_page" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/systemBank/maker_view_delete_image_upload.jsp"/>
               	    <set-property property="title" value="title.image.upload"/>
            	</forward>
				<forward name="maker_prepare_submit" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/common/ack_submit.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>
				<forward name="close_page"   path="/template/common_template.jsp" >
		   	  		<set-property property="rightFrame" value="/image/view_image_list.jsp"/>
		   	  		<set-property property="title" value="title.image.upload"/>
	  	        </forward>
	  	        <forward name="process" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/image/checker_image_upload.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>
				<forward name="checker_add_read" path="/template/common_template.jsp">
					<set-property property="rightFrame" value="/image/checker_image_upload_details.jsp" />
					<set-property property="title" value="title.image.upload" />
				</forward>
				<forward name="checker_reject_add" path="/template/common_template.jsp">
               		    <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                		<set-property property="title" value="title.image.upload"/>
           		</forward>
				<forward name="checker_reject_edit" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/common/ack_reject.jsp"/>
               	    <set-property property="title" value="title.image.upload"/>
            	</forward>
            	 <forward name="maker_prepare_close" path="/template/common_template.jsp">
               		 <set-property property="rightFrame" value="/image/maker_close_systemBank.jsp"/>
               		 <set-property property="title" value="title.image.upload"/>
            	</forward>
           		 <forward name="maker_confirm_close" path="/template/common_template.jsp">
            		<set-property property="rightFrame" value="/common/ack_close.jsp"/>
           	    	<set-property property="title" value="title.image.upload"/>
        		</forward>
        		 <forward name="maker_confirm_close" path="/template/common_template.jsp">
            		 <set-property property="rightFrame" value="/common/ack_close.jsp"/>
           			 <set-property property="title" value="title.image.upload"/>
       		 	</forward>
       		 	 <forward name="checker_confirm_approve_create" path="/template/common_template.jsp">
           			 <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
            		 <set-property property="title" value="title.image.upload"/>
        		</forward>
        		<forward name="image_gallary" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/image/image_gallary.jsp"/>
                 	<set-property property="title" value="title.image.tag"/>
            	</forward>
				<forward name="imagePath"	path="/image/imagePath.jsp" />
        		
        		<forward name="maker_view_todo_page" path="/template/common_template.jsp">
                	<set-property property="rightFrame" value="/image/maker_view_todo_image_upload.jsp"/>
                	<set-property property="title" value="title.image.upload"/>
                </forward>
                <forward name="get_filter_subfolder_name"	path="/image/subfolderNameList.jsp" />
			<forward name="get_filter_docname"	path="/image/documentNameList.jsp" />
			<forward name="refresh_facility_document_name"	path="/image/facilityDocumentNameList.jsp" />
			<forward name="refresh_security_document_name"	path="/image/securityDocumentNameList.jsp" />
			<forward name="downloadImage" path="/image/popup.jsp" />
			<forward name="refresh_other_doc"	path="/image/otherDocumentNameList.jsp" />
        </action>
        
    </action-mappings>
    
    
    
    <message-resources key="message-testmaster" parameter="ApplicationResources-image-upload"/>
</struts-config>