<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainAssetLifeForm" type="com.integrosys.cms.ui.systemparameters.assetlife.MaintainAssetLifeForm">
        </form-bean>    
    </form-beans>
            
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/MaintainAssetLife"
            type="com.integrosys.cms.ui.systemparameters.assetlife.MaintainAssetLifeAction"
            name="MaintainAssetLifeForm" scope="request">
            <forward name="maker_view_assetlife_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemparameters/assetlife/assetlife_maker_view.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="maker_edit_assetlife_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemparameters/assetlife/assetlife_maker_edit.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="maker_edit_assetlife_confirm" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_submit.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="checker_edit_assetlife_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemparameters/assetlife/assetlife_checker_edit.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="maker_close_assetlife_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemparameters/assetlife/assetlife_maker_view.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="view" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemparameters/assetlife/assetlife_maker_view.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="after_to_track" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/systemparameters/assetlife/assetlife_maker_view.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="title.assetlife.edit"/>
            </forward>
        </action>
    
    </action-mappings>
    
    <message-resources key="message-param-assetlife" parameter="ApplicationResources-param-assetlife"/>
</struts-config>