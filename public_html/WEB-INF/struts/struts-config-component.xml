<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans type="org.apache.struts.action.ActionFormBean">
        <form-bean name="MaintainComponentForm" type="com.integrosys.cms.ui.component.MaintainComponentForm">
        </form-bean>
    </form-beans>
    
    <action-mappings type="com.integrosys.base.uiinfra.common.IntegroActionMapping">
        <action path="/component" type="com.integrosys.cms.ui.component.MaintainComponentAction" name="MaintainComponentForm" validate="false" scope="request">
        
        
            <forward name="maker_list_component_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_list_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="prepare_create_component" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_add_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="common_submit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/ack_submit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="checker_component_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/checker_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="common_approve_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_approve.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="maker_view_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_view_delete_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="maker_view_todo_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_view_todo_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="common_reject_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_reject.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="common_resubmit_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_resubmit.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="maker_prepare_resubmit_delete" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_resubmit_delete_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
             <forward name="maker_view_delete_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_read_delete_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="maker_add_edit_component_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_add_edit_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="work_in_process_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/wip.html"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="maker_prepare_close" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/component/maker_close_component.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
            <forward name="common_close_page" path="/template/common_template.jsp">
                <set-property property="rightFrame" value="/common/ack_close.jsp"/>
                <set-property property="title" value="menu.manual_feeds.component.name"/>
            </forward>
        
        </action>
        </action-mappings>
</struts-config>
