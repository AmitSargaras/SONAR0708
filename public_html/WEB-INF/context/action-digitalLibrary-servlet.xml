<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

	<description>
		struts config to context mapping files for Digital Library
	</description>

	<bean name="/digital_library_list" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.digitalLibrary.DigitalLibraryAction">
    	<property name="nameCommandMap">
      		<map>
		        <entry key="AddDigitalLibraryCommand" value-ref="DigitalLibraryAction.AddDigitalLibraryCommand"/>
		        <entry key="ApproveDigitalLibraryCommand" value-ref="DigitalLibraryAction.ApproveDigitalLibraryCommand"/>
		        <entry key="CloseDigitalLibraryCommand" value-ref="DigitalLibraryAction.CloseDigitalLibraryCommand"/>
		        <entry key="CompareDigitalLibraryCommand" value-ref="DigitalLibraryAction.CompareDigitalLibraryCommand"/>
		        <entry key="DeleteDigitalLibraryCommand" value-ref="DigitalLibraryAction.DeleteDigitalLibraryCommand"/>
		        <entry key="ListDigitalLibraryCommand" value-ref="DigitalLibraryAction.ListDigitalLibraryCommand"/>
		        <entry key="ListCompareDigitalLibraryCommand" value-ref="DigitalLibraryAction.ListCompareDigitalLibraryCommand"/>
		        <entry key="ListViewDigitalLibraryCommand" value-ref="DigitalLibraryAction.ListViewDigitalLibraryCommand"/>
		        <entry key="PaginateDigitalLibraryCommand" value-ref="DigitalLibraryAction.PaginateDigitalLibraryCommand"/>
		        <entry key="ReadDigitalLibraryCommand" value-ref="DigitalLibraryAction.ReadDigitalLibraryCommand"/>
		        <entry key="RejectDigitalLibraryCommand" value-ref="DigitalLibraryAction.RejectDigitalLibraryCommand"/>
		        <entry key="SaveDigitalLibraryCommand" value-ref="DigitalLibraryAction.SaveDigitalLibraryCommand"/>
		        <entry key="SubmitDigitalLibraryCommand" value-ref="DigitalLibraryAction.SubmitDigitalLibraryCommand"/>
		        <entry key="ReloadMarketableSecValuationProfileCommand" value-ref="CollateralAction.ReloadMarketableSecValuationProfileSingletonCommand"/>
      		</map>
    	</property>
  	</bean>

</beans>