<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

	<description>
		struts config to context mapping files for function
	</description>

	<bean name="/MaintainFunction" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.function.TeamFunctionGrpAction">
		<property name="nameCommandMap">
			<map>
				<entry key="ReadTeamFunctionGrpCommand" value-ref="TeamFunctionGrpAction.ReadTeamFunctionGrpCommand"/>
				<entry key="ApproveTeamFunctionGrpCommand" value-ref="TeamFunctionGrpAction.ApproveTeamFunctionGrpCommand"/>
				<entry key="ListTeamFunctionGrpCommand" value-ref="TeamFunctionGrpAction.ListTeamFunctionGrpCommand"/>
				<entry key="RejectTeamFunctionGrpCommand" value-ref="TeamFunctionGrpAction.RejectTeamFunctionGrpCommand"/>
				<entry key="CloseTeamFunctionGrpCommand" value-ref="TeamFunctionGrpAction.CloseTeamFunctionGrpCommand"/>
				<entry key="SubmitTeamFunctionGrpCommand" value-ref="TeamFunctionGrpAction.SubmitTeamFunctionGrpCommand"/> 
			</map>
		</property>
	</bean>

</beans>