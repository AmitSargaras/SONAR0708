<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">


  <bean name="/RecurrentReceipt" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.checklist.recreceipt.RecurrentReceiptAction">
    <property name="nameCommandMap">
      <map>
        <entry key="ReadRecurrentHistoryCommand" value-ref="RecurrentReceiptAction.ReadRecurrentHistoryCommand" />
        <entry key="UpdateCovenantCommand" value-ref="RecurrentReceiptAction.UpdateCovenantCommand" />
        <entry key="ReadStagingRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.ReadStagingRecurrentReceiptCommand" />
        <entry key="ApproveRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.ApproveRecurrentReceiptCommand" />
        <entry key="CheckerViewCovenantItemCommand" value-ref="RecurrentReceiptAction.CheckerViewCovenantItemCommand" />
        <entry key="ReadRecurrentCommand" value-ref="RecurrentReceiptAction.ReadRecurrentCommand" />
        <entry key="ReadCovenantCommand" value-ref="RecurrentReceiptAction.ReadCovenantCommand" />
        <entry key="SaveRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.SaveRecurrentReceiptCommand" />
        <entry key="SubmitRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.SubmitRecurrentReceiptCommand" />
        <entry key="ListRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.ListRecurrentReceiptCommand" />
        <entry key="CloseRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.CloseRecurrentReceiptCommand" />
        <entry key="RejectRecurrentReceiptCommand" value-ref="RecurrentReceiptAction.RejectRecurrentReceiptCommand" />
        <entry key="UpdateRecurrentCommand" value-ref="RecurrentReceiptAction.UpdateRecurrentCommand" />
        <entry key="CheckerViewRecurrentItemCommand" value-ref="RecurrentReceiptAction.CheckerViewRecurrentItemCommand" />
        <entry key="PreparePrintDocCommand" value-ref="RecurrentReceiptAction.PreparePrintDocCommand" />
        <entry key="FrameFlagSetterCommand" value-ref="RecurrentReceiptAction.FrameFlagSetterCommand" />
        <entry key="ReadCovenantHistoryCommand" value-ref="RecurrentReceiptAction.ReadCovenantHistoryCommand" />
        <entry key="RecurrentCheckListAction.RecurrentAccessControlCommand" value-ref="RecurrentCheckListAction.RecurrentAccessControlCommand" />
        <entry key="CCReceiptAction.FrameFlagSetterCommand" value-ref="CCReceiptAction.FrameFlagSetterCommand" />
      </map>
    </property>
  </bean>

  <bean name="/RecurrentCheckList" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.checklist.recurrent.RecurrentCheckListAction">
    <property name="nameCommandMap">
      <map>
        <entry key="ReadStagingRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.ReadStagingRecurrentCheckListCommand" />
        <entry key="SaveRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.SaveRecurrentCheckListCommand" />
        <entry key="PrepareRecurrentCommand" value-ref="RecurrentCheckListAction.PrepareRecurrentCommand" />
        <entry key="ApproveRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.ApproveRecurrentCheckListCommand" />
        <entry key="ListRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.ListRecurrentCheckListCommand" />
        <entry key="RejectRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.RejectRecurrentCheckListCommand" />
        <entry key="UpdateCovenantCommand" value-ref="RecurrentCheckListAction.UpdateCovenantCommand" />
        <entry key="SubmitRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.SubmitRecurrentCheckListCommand" />
        <entry key="CheckerViewCovenantItemCommand" value-ref="RecurrentCheckListAction.CheckerViewCovenantItemCommand" />
        <entry key="ReadRecurrentCommand" value-ref="RecurrentCheckListAction.ReadRecurrentCommand" />
        <entry key="ReadCovenantCommand" value-ref="RecurrentCheckListAction.ReadCovenantCommand" />
        <entry key="RecurrentAccessControlCommand" value-ref="RecurrentCheckListAction.RecurrentAccessControlCommand" />
        <entry key="AddRecurrentCommand" value-ref="RecurrentCheckListAction.AddRecurrentCommand" />
        <entry key="CloseRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.CloseRecurrentCheckListCommand" />
        <entry key="UpdateRecurrentCommand" value-ref="RecurrentCheckListAction.UpdateRecurrentCommand" />
        <entry key="AddCovenantCommand" value-ref="RecurrentCheckListAction.AddCovenantCommand" />
        <entry key="CheckerViewRecurrentItemCommand" value-ref="RecurrentCheckListAction.CheckerViewRecurrentItemCommand" />
        <entry key="RemoveRecurrentCheckListCommand" value-ref="RecurrentCheckListAction.RemoveRecurrentCheckListCommand" />
        <entry key="RemoveCovenantCheckListCommand" value-ref="RecurrentCheckListAction.RemoveCovenantCheckListCommand" />
        <entry key="FrameFlagSetterCommand" value-ref="RecurrentCheckListAction.FrameFlagSetterCommand" />
         <entry key="CCReceiptAction.FrameFlagSetterCommand" value-ref="CCReceiptAction.FrameFlagSetterCommand" />
      </map>
    </property>
    <property name="maintainChecklistWithoutApproval" value="${recurrent.maintain.suppress.checker.approval}" />
  </bean>

  <bean name="/CheckListReceipt" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.checklist.CheckListReceiptAction">
    <property name="nameCommandMap">
      <map>
        <entry key="ReadCheckListReceiptTrxCommand" value-ref="CheckListReceiptAction.ReadCheckListReceiptTrxCommand" />
      </map>
    </property>
  </bean>
  
  <bean name="/Annexure" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.checklist.annexure.AnnexureAction">
    <property name="nameCommandMap">
      <map>
        <entry key="ReadAnnexureHistoryCommand" value-ref="AnnexureAction.ReadAnnexureHistoryCommand" />
        <entry key="UpdateCovenantCommand" value-ref="AnnexureAction.UpdateCovenantCommand" />
        <entry key="ReadStagingAnnexureCommand" value-ref="AnnexureAction.ReadStagingAnnexureCommand" />
        <entry key="ApproveAnnexureCommand" value-ref="AnnexureAction.ApproveAnnexureCommand" />
        <entry key="CheckerViewCovenantItemCommand" value-ref="AnnexureAction.CheckerViewCovenantItemCommand" />
        <entry key="ReadAnnexureCommand" value-ref="AnnexureAction.ReadAnnexureCommand" />
        <entry key="ReadCovenantCommand" value-ref="AnnexureAction.ReadCovenantCommand" />
        <entry key="SaveAnnexureCommand" value-ref="AnnexureAction.SaveAnnexureCommand" />
        <entry key="SubmitAnnexureCommand" value-ref="AnnexureAction.SubmitAnnexureCommand" />
        <entry key="ListAnnexureCommand" value-ref="AnnexureAction.ListAnnexureCommand" />
        <entry key="CloseAnnexureCommand" value-ref="AnnexureAction.CloseAnnexureCommand" />
        <entry key="RejectAnnexureCommand" value-ref="AnnexureAction.RejectAnnexureCommand" />
        <entry key="UpdateAnnexureCommand" value-ref="AnnexureAction.UpdateAnnexureCommand" />
        <entry key="CheckerViewAnnexureItemCommand" value-ref="AnnexureAction.CheckerViewAnnexureItemCommand" />
        <entry key="PreparePrintDocCommand" value-ref="AnnexureAction.PreparePrintDocCommand" />
        <entry key="FrameFlagSetterCommand" value-ref="AnnexureAction.FrameFlagSetterCommand" />
        <entry key="ReadCovenantHistoryCommand" value-ref="AnnexureAction.ReadCovenantHistoryCommand" />
        <entry key="RecurrentCheckListAction.RecurrentAccessControlCommand" value-ref="RecurrentCheckListAction.RecurrentAccessControlCommand" />
        <entry key="CCReceiptAction.FrameFlagSetterCommand" value-ref="CCReceiptAction.FrameFlagSetterCommand" />
      </map>
    </property>
  </bean>
  
</beans>