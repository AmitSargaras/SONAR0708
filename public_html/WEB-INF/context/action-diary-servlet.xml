<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:aop="http://www.springframework.org/schema/aop"
		xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">



	<bean name="/DiaryItem" scope="prototype" autowire="byName" class="com.integrosys.cms.ui.diaryitem.DiaryItemAction">
		<property name="nameCommandMap">
			<map>
				<entry key="ListDueItemsCommand" value-ref="ListDueItemsCommand"/>
				<entry key="ListNonExpiredItemsCommand" value-ref="ListNonExpiredItemsCommand"/>
				<entry key="PrepareDiaryItemCommand" value-ref="PrepareDiaryItemCommand"/>
				<entry key="PrintDiaryItemCommand" value-ref="PrintDiaryItemCommand"/>
				<entry key="ReadDiaryItemCommand" value-ref="ReadDiaryItemCommand"/>
				<entry key="RefreshNonExpiredItemsCommand" value-ref="RefreshNonExpiredItemsCommand"/>
				<entry key="SearchNonExpiredItemsCommand" value-ref="SearchNonExpiredItemsCommand"/>
				<entry key="UpdateDiaryItemCommand" value-ref="UpdateDiaryItemCommand"/>
				<entry key="CreateDiaryItemCommand" value-ref="CreateDiaryItemCommand"/>
				<entry key="ListDiarySegmentWiseCommand" value-ref="ListDiarySegmentWiseCommand"/>
				<entry key="DeleteDiaryItemCommand" value-ref="DeleteDiaryItemCommand"/>
				<entry key="TodaysDiaryItemCommand" value-ref="TodaysDiaryItemCommand"/>
				<entry key="AckODErrorCommand" value-ref="AckODErrorCommand"/>
				
			</map>
		</property>
	</bean>
</beans>