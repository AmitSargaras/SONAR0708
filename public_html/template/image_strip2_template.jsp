<%-- this is for the top and bottom strip --%>
<%-- for those pages using this file will see the same image for top and bottom strip --%>
<script type="text/javascript">
	var cookie = read_Cookie("cms_style");
	var styletitle = cookie ? cookie : get_PreferredStyleSheet();

	var stripImgName = "<%=request.getContextPath()%>/themes/" + styletitle + "/img/strip.jpg";
	document.write('<td width="100%" height="27" colspan="3" align="center" style="background-image:url(' + stripImgName + ')">&nbsp;</td>');
</script>